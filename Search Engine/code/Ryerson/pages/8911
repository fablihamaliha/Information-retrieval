<script>
    $(document).ready(function(){
    	
        $("#b4007c05-d50b-41a6-a04d-26813f2090c6-play, #b4007c05-d50b-41a6-a04d-26813f2090c6-pause").click(function() {
            if ($(this).attr("id") == "b4007c05-d50b-41a6-a04d-26813f2090c6-play") {
                $('#b4007c05-d50b-41a6-a04d-26813f2090c6').carousel('cycle');
                $(this).attr("id", "b4007c05-d50b-41a6-a04d-26813f2090c6-pause");
                $("#b4007c05-d50b-41a6-a04d-26813f2090c6-pause i").attr("class", "fa fa-pause");
            } else {
                $('#b4007c05-d50b-41a6-a04d-26813f2090c6').carousel('pause');
                $(this).attr("id", "b4007c05-d50b-41a6-a04d-26813f2090c6-play");
                $("#b4007c05-d50b-41a6-a04d-26813f2090c6-play i").attr("class", "fa fa-play");
            }
        });
        
        		$('#b4007c05-d50b-41a6-a04d-26813f2090c6').carousel('cycle');
        	
    });
</script>
