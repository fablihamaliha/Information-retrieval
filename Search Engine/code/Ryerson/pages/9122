        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#7d8d881a-6d0e-456a-8814-ca4676e0f2b6 button").click(function() {
                	jQuery("#7d8d881a-6d0e-456a-8814-ca4676e0f2b6.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = false;
                    var latLng = JSON.parse('{"lat": 43.65722239999999, "lng": -79.3777944}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('7d8d881a-6d0e-456a-8814-ca4676e0f2b6',latLng,'300px',18,true);
                        } else{
                            drawDynamicMapByAddress('7d8d881a-6d0e-456a-8814-ca4676e0f2b6','288 Church St, Toronto, ON M5B 1Z5, Canada','300px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('7d8d881a-6d0e-456a-8814-ca4676e0f2b6',latLng,'288 Church St, Toronto, ON M5B 1Z5, Canada','1140px','300px',18,true);
