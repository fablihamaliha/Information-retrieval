    $(document).ready(function(){
    	
        $("#86b6ba79-df99-49b1-b373-3c1bcbac1654-play, #86b6ba79-df99-49b1-b373-3c1bcbac1654-pause").click(function() {
            if ($(this).attr("id") == "86b6ba79-df99-49b1-b373-3c1bcbac1654-play") {
                $('#86b6ba79-df99-49b1-b373-3c1bcbac1654').carousel('cycle');
                $(this).attr("id", "86b6ba79-df99-49b1-b373-3c1bcbac1654-pause");
                $("#86b6ba79-df99-49b1-b373-3c1bcbac1654-pause i").attr("class", "fa fa-pause");
            } else {
                $('#86b6ba79-df99-49b1-b373-3c1bcbac1654').carousel('pause');
                $(this).attr("id", "86b6ba79-df99-49b1-b373-3c1bcbac1654-play");
                $("#86b6ba79-df99-49b1-b373-3c1bcbac1654-play i").attr("class", "fa fa-play");
