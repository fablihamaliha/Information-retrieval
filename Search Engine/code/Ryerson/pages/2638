<script>
    $(document).ready(function(){
    	
        $("#517c94bc-1de8-4c81-a158-ad9fbbe9be4f-play, #517c94bc-1de8-4c81-a158-ad9fbbe9be4f-pause").click(function() {
            if ($(this).attr("id") == "517c94bc-1de8-4c81-a158-ad9fbbe9be4f-play") {
                $('#517c94bc-1de8-4c81-a158-ad9fbbe9be4f').carousel('cycle');
                $(this).attr("id", "517c94bc-1de8-4c81-a158-ad9fbbe9be4f-pause");
                $("#517c94bc-1de8-4c81-a158-ad9fbbe9be4f-pause i").attr("class", "fa fa-pause");
            } else {
                $('#517c94bc-1de8-4c81-a158-ad9fbbe9be4f').carousel('pause');
                $(this).attr("id", "517c94bc-1de8-4c81-a158-ad9fbbe9be4f-play");
                $("#517c94bc-1de8-4c81-a158-ad9fbbe9be4f-play i").attr("class", "fa fa-play");
            }
        });
        
        		$('#517c94bc-1de8-4c81-a158-ad9fbbe9be4f').carousel('cycle');
        	
    });
</script>
