<script>
    $(document).ready(function(){
    	
        $("#bb230b20-ddfd-48df-b63e-8382bf8942c7-play, #bb230b20-ddfd-48df-b63e-8382bf8942c7-pause").click(function() {
            if ($(this).attr("id") == "bb230b20-ddfd-48df-b63e-8382bf8942c7-play") {
                $('#bb230b20-ddfd-48df-b63e-8382bf8942c7').carousel('cycle');
                $(this).attr("id", "bb230b20-ddfd-48df-b63e-8382bf8942c7-pause");
                $("#bb230b20-ddfd-48df-b63e-8382bf8942c7-pause i").attr("class", "fa fa-pause");
            } else {
                $('#bb230b20-ddfd-48df-b63e-8382bf8942c7').carousel('pause');
                $(this).attr("id", "bb230b20-ddfd-48df-b63e-8382bf8942c7-play");
                $("#bb230b20-ddfd-48df-b63e-8382bf8942c7-play i").attr("class", "fa fa-play");
            }
        });
        
        		$('#bb230b20-ddfd-48df-b63e-8382bf8942c7').carousel('cycle');
        	
    });
