    $(document).ready(function(){
    	
        $("#858eee2f-a6e9-4a01-8adb-3304947b6fe1-play, #858eee2f-a6e9-4a01-8adb-3304947b6fe1-pause").click(function() {
            if ($(this).attr("id") == "858eee2f-a6e9-4a01-8adb-3304947b6fe1-play") {
                $('#858eee2f-a6e9-4a01-8adb-3304947b6fe1').carousel('cycle');
                $(this).attr("id", "858eee2f-a6e9-4a01-8adb-3304947b6fe1-pause");
                $("#858eee2f-a6e9-4a01-8adb-3304947b6fe1-pause i").attr("class", "fa fa-pause");
            } else {
                $('#858eee2f-a6e9-4a01-8adb-3304947b6fe1').carousel('pause');
                $(this).attr("id", "858eee2f-a6e9-4a01-8adb-3304947b6fe1-play");
                $("#858eee2f-a6e9-4a01-8adb-3304947b6fe1-play i").attr("class", "fa fa-play");
