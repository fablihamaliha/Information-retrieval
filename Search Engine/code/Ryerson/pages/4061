<script>
$("#rye-d0aedab5-9632-4c51-a68f-5e132ff95847 input, #rye-d0aedab5-9632-4c51-a68f-5e132ff95847 select").each(function(i, v) {
	if (!$(this).hasClass('form-control')) {
		if ($(this).is("input[type='file']")) {
			$(this).attr('id', 'rye-d0aedab5-9632-4c51-a68f-5e132ff95847_upload'+i).wrap("<span class='htmlSnippetForm'></span>").after("<label for='rye-d0aedab5-9632-4c51-a68f-5e132ff95847_upload"+i+"' class='customInput'>Choose file</label>");
		} else {
			$(this).wrap("<span class='htmlSnippetForm'></span>").after("<span class='customInput'></span>");
		}
	}
});
</script>

