                $('#05af26e0-7101-466f-8af8-cda93b5cd8e1').carousel('pause');
                $(this).attr("id", "05af26e0-7101-466f-8af8-cda93b5cd8e1-play");
                $("#05af26e0-7101-466f-8af8-cda93b5cd8e1-play i").attr("class", "fa fa-play");
