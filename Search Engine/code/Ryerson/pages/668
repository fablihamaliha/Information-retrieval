                $('#05af26e0-7101-466f-8af8-cda93b5cd8e1').carousel('cycle');
                $(this).attr("id", "05af26e0-7101-466f-8af8-cda93b5cd8e1-pause");
                $("#05af26e0-7101-466f-8af8-cda93b5cd8e1-pause i").attr("class", "fa fa-pause");
