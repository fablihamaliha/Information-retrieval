	<script>
	
	//convert to plugin in pure js....later (soon)
	$(document).ready(function() {
		$('#8e823357-2754-4198-9202-5f173a42c271 table').addClass("table");
	    
		$('#8e823357-2754-4198-9202-5f173a42c271 table').addClass("table-bordered");
		
			var removeHeading = false;
			
				//mandatory to have heading on initialization but remove it after. See #NOTE-REMOVE
				if($('#8e823357-2754-4198-9202-5f173a42c271 table thead').eq(0).length == 0){
						//insert fake row and delete
						removeHeading = true;
						var numOfColsTD = $('#8e823357-2754-4198-9202-5f173a42c271 table tbody tr').eq(0).find('td').length;
						var numOfColsTH = $('#8e823357-2754-4198-9202-5f173a42c271 table tbody tr').eq(0).find('th').length;
						
						var appendTH = "<thead>";
						for(var i = 0; i < numOfColsTD+numOfColsTH;i++){
							appendTH += "<th>Column</th>";
						}
						appendTH += "</thead>";
						$('#8e823357-2754-4198-9202-5f173a42c271 table').prepend(appendTH);
				}
			
			
			$('#8e823357-2754-4198-9202-5f173a42c271 table').removeAttr("border");
			$('#8e823357-2754-4198-9202-5f173a42c271 table').removeAttr("cellpadding");
			$('#8e823357-2754-4198-9202-5f173a42c271 table').removeAttr("cellspacing");
			
			$('#8e823357-2754-4198-9202-5f173a42c271 table').DataTable({
				
				"sDom":"lfrtp",
				"sort":false,
				"pagingType": "simple_numbers","paging":false,
				"autoWidth":true
			});
		
			//#NOTE-REMOVE
			if(removeHeading){
				$('#8e823357-2754-4198-9202-5f173a42c271 table thead').remove();
			}
			
			
		    
	    
	    
	} );
	</script>
