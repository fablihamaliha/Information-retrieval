                        if(latLng != null){
                            drawDynamicMapByLatLng('dde25c91-6cfa-4f56-8309-d1c2141757ef',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('dde25c91-6cfa-4f56-8309-d1c2141757ef','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','600px',18,true);
                        }
