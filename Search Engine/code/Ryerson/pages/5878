<script>
$("#rye-d554b5c9-4dfd-4d47-be23-63f258301db2 input, #rye-d554b5c9-4dfd-4d47-be23-63f258301db2 select").each(function(i, v) {
	if (!$(this).hasClass('form-control')) {
		if ($(this).is("input[type='file']")) {
			$(this).attr('id', 'rye-d554b5c9-4dfd-4d47-be23-63f258301db2_upload'+i).wrap("<span class='htmlSnippetForm'></span>").after("<label for='rye-d554b5c9-4dfd-4d47-be23-63f258301db2_upload"+i+"' class='customInput'>Choose file</label>");
		} else {
			$(this).wrap("<span class='htmlSnippetForm'></span>").after("<span class='customInput'></span>");
		}
	}
});
</script>

