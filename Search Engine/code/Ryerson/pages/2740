                        if(latLng != null){
                            drawDynamicMapByLatLng('45049270-0293-4ea5-9470-ed60fdd9d6f1',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('45049270-0293-4ea5-9470-ed60fdd9d6f1','50 Gould Street, Toronto, ON, M5B 1X8, Canada','600px',18,true);
                        }
