                jQuery("#45049270-0293-4ea5-9470-ed60fdd9d6f1 button").click(function() {
                	jQuery("#45049270-0293-4ea5-9470-ed60fdd9d6f1.res-map-placeholder").removeClass('res-map-placeholder');
