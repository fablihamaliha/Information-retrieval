jQuery(document).ready(function() {
    jQuery("#component574069804 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component574069804 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component574069804 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component574069804 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component574069804 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
 </script>
 
