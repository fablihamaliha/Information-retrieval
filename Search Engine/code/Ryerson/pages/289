<script>
    $(document).ready(function(){
    	
        $("#cd033700-0b32-4695-94cd-bf63ad4325f4-play, #cd033700-0b32-4695-94cd-bf63ad4325f4-pause").click(function() {
            if ($(this).attr("id") == "cd033700-0b32-4695-94cd-bf63ad4325f4-play") {
                $('#cd033700-0b32-4695-94cd-bf63ad4325f4').carousel('cycle');
                $(this).attr("id", "cd033700-0b32-4695-94cd-bf63ad4325f4-pause");
                $("#cd033700-0b32-4695-94cd-bf63ad4325f4-pause i").attr("class", "fa fa-pause");
            } else {
                $('#cd033700-0b32-4695-94cd-bf63ad4325f4').carousel('pause');
                $(this).attr("id", "cd033700-0b32-4695-94cd-bf63ad4325f4-play");
                $("#cd033700-0b32-4695-94cd-bf63ad4325f4-play i").attr("class", "fa fa-play");
            }
        });
        
