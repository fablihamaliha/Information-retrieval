        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#78afb0d2-d442-4366-bb9f-cad88d168b87 button").click(function() {
                	jQuery("#78afb0d2-d442-4366-bb9f-cad88d168b87.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6558668, "lng": -79.3819145}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('78afb0d2-d442-4366-bb9f-cad88d168b87',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('78afb0d2-d442-4366-bb9f-cad88d168b87','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('78afb0d2-d442-4366-bb9f-cad88d168b87',latLng,'1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('78afb0d2-d442-4366-bb9f-cad88d168b87','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','1140px','600px',18,true);
                        }
                    }
                });
            });
