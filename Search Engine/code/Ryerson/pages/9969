jQuery(document).ready(function() {
    jQuery("#component222268255 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component222268255 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component222268255 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component222268255 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component222268255 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
 </script>
 
