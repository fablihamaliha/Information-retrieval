                        if(latLng != null){
                            drawDynamicMapByLatLng('e39f2679-f224-4f22-a708-df6e0de02130',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('e39f2679-f224-4f22-a708-df6e0de02130','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','600px',18,true);
                        }
