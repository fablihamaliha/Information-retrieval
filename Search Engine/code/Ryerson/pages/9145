        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#e39f2679-f224-4f22-a708-df6e0de02130 button").click(function() {
                	jQuery("#e39f2679-f224-4f22-a708-df6e0de02130.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6558668, "lng": -79.3819145}');
