<script>
    $(document).ready(function(){
    	
        $("#6e5e8194-ff67-416a-847e-3977846eef23-play, #6e5e8194-ff67-416a-847e-3977846eef23-pause").click(function() {
            if ($(this).attr("id") == "6e5e8194-ff67-416a-847e-3977846eef23-play") {
                $('#6e5e8194-ff67-416a-847e-3977846eef23').carousel('cycle');
                $(this).attr("id", "6e5e8194-ff67-416a-847e-3977846eef23-pause");
                $("#6e5e8194-ff67-416a-847e-3977846eef23-pause i").attr("class", "fa fa-pause");
            } else {
                $('#6e5e8194-ff67-416a-847e-3977846eef23').carousel('pause');
                $(this).attr("id", "6e5e8194-ff67-416a-847e-3977846eef23-play");
                $("#6e5e8194-ff67-416a-847e-3977846eef23-play i").attr("class", "fa fa-play");
            }
        });
        
        	$('#6e5e8194-ff67-416a-847e-3977846eef23').carousel('pause');
        	
    });
</script>
