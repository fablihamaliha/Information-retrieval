<section id="mk-page-title" class="left-align" data-intro="none" data-fullHeight="false"><div class="mk-page-title-bg"></div><div class="mk-effect-gradient-layer"></div><div class="mk-grid"><h1 class="mk-page-heading">Hello from the other side!</h1><div class="clearboth"></div></div></section>

<div id="theme-page" class="mk-blog-single page-master-holder">
		<div class="mk-main-wrapper-holder">
	<div class="background-img background-img--page"></div>
	<div class="theme-page-wrapper right-layout mk-grid vc_row-fluid ">
			<div class="theme-content " id="blog-entry-20466" class="post-20466 post type-post status-publish format-standard has-post-thumbnail hentry category-student tag-campus tag-campus-life tag-discover-ryerson tag-downtown tag-dubai tag-fcad tag-fun tag-international tag-international-student tag-journalism tag-residence tag-ryerson tag-student-life tag-third-year tag-toronto tag-university tag-whyryerson" itemprop="mainContentOfPage">
		
												<div class="featured-image">
								<a href="https://why.ryerson.ca/wp-content/uploads/2020/11/eggy.jpg" class="mk-lightbox"><img alt="Hello from the other side!" title="Hello from the other side!" src="https://why.ryerson.ca/wp-content/uploads/bfi_thumb/eggy-oxvkgzr2349rci5rcuodggql4d6vuznnkwjusqicf4.jpg" height="400" width="815" itemprop="image" /></a>
							</div>
					
				 	

 					<div class="entry-meta">
				<div class="item-holder">
					<time class="mk-post-date" datetime="2020-11-09" itemprop="datePublished" pubdate>
