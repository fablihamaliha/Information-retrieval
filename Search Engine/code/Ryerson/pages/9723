        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#f45e764e-de46-4a85-8642-1366c919559d button").click(function() {
                	jQuery("#f45e764e-de46-4a85-8642-1366c919559d.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6600414, "lng": -79.3771506}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('f45e764e-de46-4a85-8642-1366c919559d',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('f45e764e-de46-4a85-8642-1366c919559d','99 Gerrard Street East, Toronto, On M5B 1G7','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
