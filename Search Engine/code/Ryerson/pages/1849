                            drawDynamicMapByLatLng('d697edab-3970-4dd3-89b1-14cd55a0525b',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('d697edab-3970-4dd3-89b1-14cd55a0525b','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','600px',18,true);
