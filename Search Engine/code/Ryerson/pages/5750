        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component1691227251 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component1691227251 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component1691227251 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component1691227251 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
 </script>
 
