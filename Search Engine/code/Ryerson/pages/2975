        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#a145d161-af86-4636-8a37-517e9a56f215 button").click(function() {
                	jQuery("#a145d161-af86-4636-8a37-517e9a56f215.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.657564, "lng": -79.3772027}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('a145d161-af86-4636-8a37-517e9a56f215',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('a145d161-af86-4636-8a37-517e9a56f215','245 Church Street, Toronto, ON, M5B 1Z2, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('a145d161-af86-4636-8a37-517e9a56f215',latLng,'245 Church Street, Toronto, ON, M5B 1Z2, Canada','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('a145d161-af86-4636-8a37-517e9a56f215','245 Church Street, Toronto, ON, M5B 1Z2, Canada','1140px','600px',18,true);
                        }
                    }
                });
            });
