<script type="text/javascript">
jQuery(document).ready(function() {
    jQuery("#component2108486683 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component2108486683 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component2108486683 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component2108486683 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component2108486683 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
 </script>
 
