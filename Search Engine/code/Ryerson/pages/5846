<script>
    $(document).ready(function(){
    	
        $("#0300873a-d027-43da-a342-5022e75e3772-play, #0300873a-d027-43da-a342-5022e75e3772-pause").click(function() {
            if ($(this).attr("id") == "0300873a-d027-43da-a342-5022e75e3772-play") {
                $('#0300873a-d027-43da-a342-5022e75e3772').carousel('cycle');
                $(this).attr("id", "0300873a-d027-43da-a342-5022e75e3772-pause");
                $("#0300873a-d027-43da-a342-5022e75e3772-pause i").attr("class", "fa fa-pause");
            } else {
                $('#0300873a-d027-43da-a342-5022e75e3772').carousel('pause');
                $(this).attr("id", "0300873a-d027-43da-a342-5022e75e3772-play");
                $("#0300873a-d027-43da-a342-5022e75e3772-play i").attr("class", "fa fa-play");
            }
        });
        
        	$('#0300873a-d027-43da-a342-5022e75e3772').carousel('pause');
        	
    });
</script>
