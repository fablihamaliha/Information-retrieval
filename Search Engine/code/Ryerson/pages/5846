    $(document).ready(function(){
    	
        $("#38a6dac4-2f98-49c2-8c53-5f54543ff2e2-play, #38a6dac4-2f98-49c2-8c53-5f54543ff2e2-pause").click(function() {
            if ($(this).attr("id") == "38a6dac4-2f98-49c2-8c53-5f54543ff2e2-play") {
                $('#38a6dac4-2f98-49c2-8c53-5f54543ff2e2').carousel('cycle');
                $(this).attr("id", "38a6dac4-2f98-49c2-8c53-5f54543ff2e2-pause");
                $("#38a6dac4-2f98-49c2-8c53-5f54543ff2e2-pause i").attr("class", "fa fa-pause");
            } else {
                $('#38a6dac4-2f98-49c2-8c53-5f54543ff2e2').carousel('pause');
                $(this).attr("id", "38a6dac4-2f98-49c2-8c53-5f54543ff2e2-play");
                $("#38a6dac4-2f98-49c2-8c53-5f54543ff2e2-play i").attr("class", "fa fa-play");
