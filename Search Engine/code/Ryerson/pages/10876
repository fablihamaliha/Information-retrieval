			var removeHeading = false;
			
			$('#827c4a3c-3eac-451a-9ba8-8524f35eb6e5 table tbody tr').eq(0).find("td").replaceWith(function(){
				return $("<th>", {attr: {'width': $(this).attr('width'), 'rowspan': $(this).attr('rowspan'), 'colspan': $(this).attr('colspan'), 'valign': $(this).attr('valign'), 'headers': $(this).attr('headers'), 'style': $(this).attr('style')}, html: $(this).html()}).css("vertical-align", $(this).attr('valign'));;
			});
			$('#827c4a3c-3eac-451a-9ba8-8524f35eb6e5 table:not(.dataTable) tbody tr').eq(0).wrap( "<thead></thead>" );
		
			$('#827c4a3c-3eac-451a-9ba8-8524f35eb6e5 table thead').prependTo('#827c4a3c-3eac-451a-9ba8-8524f35eb6e5 table');
			
			
