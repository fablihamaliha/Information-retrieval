        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#33a63e6c-7d36-4475-b3a2-91fee6cf6d3e button").click(function() {
                	jQuery("#33a63e6c-7d36-4475-b3a2-91fee6cf6d3e.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.657564, "lng": -79.3772027}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('33a63e6c-7d36-4475-b3a2-91fee6cf6d3e',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('33a63e6c-7d36-4475-b3a2-91fee6cf6d3e','245 Church Street, Toronto, ON, M5B 1Z2, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('33a63e6c-7d36-4475-b3a2-91fee6cf6d3e',latLng,'245 Church Street, Toronto, ON, M5B 1Z2, Canada','1140px','600px',18,true);
