<script>
$("#rye-b76729c1-b592-486b-a160-2a794bb0103a input, #rye-b76729c1-b592-486b-a160-2a794bb0103a select").each(function(i, v) {
	if (!$(this).hasClass('form-control')) {
		if ($(this).is("input[type='file']")) {
			$(this).attr('id', 'rye-b76729c1-b592-486b-a160-2a794bb0103a_upload'+i).wrap("<span class='htmlSnippetForm'></span>").after("<label for='rye-b76729c1-b592-486b-a160-2a794bb0103a_upload"+i+"' class='customInput'>Choose file</label>");
		} else {
			$(this).wrap("<span class='htmlSnippetForm'></span>").after("<span class='customInput'></span>");
		}
	}
});
</script>

