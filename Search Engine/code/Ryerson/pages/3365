<script>
    $(document).ready(function(){
    	
        $("#52222bc0-f4d6-425a-8638-8c18b1b08f0d-play, #52222bc0-f4d6-425a-8638-8c18b1b08f0d-pause").click(function() {
            if ($(this).attr("id") == "52222bc0-f4d6-425a-8638-8c18b1b08f0d-play") {
                $('#52222bc0-f4d6-425a-8638-8c18b1b08f0d').carousel('cycle');
                $(this).attr("id", "52222bc0-f4d6-425a-8638-8c18b1b08f0d-pause");
                $("#52222bc0-f4d6-425a-8638-8c18b1b08f0d-pause i").attr("class", "fa fa-pause");
            } else {
                $('#52222bc0-f4d6-425a-8638-8c18b1b08f0d').carousel('pause');
                $(this).attr("id", "52222bc0-f4d6-425a-8638-8c18b1b08f0d-play");
                $("#52222bc0-f4d6-425a-8638-8c18b1b08f0d-play i").attr("class", "fa fa-play");
            }
        });
        
        		$('#52222bc0-f4d6-425a-8638-8c18b1b08f0d').carousel('cycle');
        	
    });
</script>
