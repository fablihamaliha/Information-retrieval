<script>
    $(document).ready(function(){
    	
        $("#3e354969-1cd3-441d-8de2-16b94843cafc-play, #3e354969-1cd3-441d-8de2-16b94843cafc-pause").click(function() {
            if ($(this).attr("id") == "3e354969-1cd3-441d-8de2-16b94843cafc-play") {
                $('#3e354969-1cd3-441d-8de2-16b94843cafc').carousel('cycle');
                $(this).attr("id", "3e354969-1cd3-441d-8de2-16b94843cafc-pause");
                $("#3e354969-1cd3-441d-8de2-16b94843cafc-pause i").attr("class", "fa fa-pause");
            } else {
                $('#3e354969-1cd3-441d-8de2-16b94843cafc').carousel('pause');
                $(this).attr("id", "3e354969-1cd3-441d-8de2-16b94843cafc-play");
                $("#3e354969-1cd3-441d-8de2-16b94843cafc-play i").attr("class", "fa fa-play");
            }
        });
        
        		$('#3e354969-1cd3-441d-8de2-16b94843cafc').carousel('cycle');
        	
    });
