                	jQuery("#c4f3a51f-9cd9-4608-be1d-535024353a18.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.65815790000001, "lng": -79.37879660000002}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('c4f3a51f-9cd9-4608-be1d-535024353a18',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('c4f3a51f-9cd9-4608-be1d-535024353a18','50 Gould Street, Toronto, ON, M5B 1X8, Canada','600px',18,true);
                        }
