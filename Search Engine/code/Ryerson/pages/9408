<script>
    $(document).ready(function(){
    	
        $("#5c98fd73-7755-4059-9b0c-be371c86f41f-play, #5c98fd73-7755-4059-9b0c-be371c86f41f-pause").click(function() {
            if ($(this).attr("id") == "5c98fd73-7755-4059-9b0c-be371c86f41f-play") {
                $('#5c98fd73-7755-4059-9b0c-be371c86f41f').carousel('cycle');
                $(this).attr("id", "5c98fd73-7755-4059-9b0c-be371c86f41f-pause");
                $("#5c98fd73-7755-4059-9b0c-be371c86f41f-pause i").attr("class", "fa fa-pause");
            } else {
                $('#5c98fd73-7755-4059-9b0c-be371c86f41f').carousel('pause');
                $(this).attr("id", "5c98fd73-7755-4059-9b0c-be371c86f41f-play");
                $("#5c98fd73-7755-4059-9b0c-be371c86f41f-play i").attr("class", "fa fa-play");
            }
        });
        
        		$('#5c98fd73-7755-4059-9b0c-be371c86f41f').carousel('cycle');
        	
    });
