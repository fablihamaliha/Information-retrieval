jQuery(document).ready(function() {
    jQuery("#component625339376 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component625339376 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component625339376 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component625339376 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component625339376 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
 </script>
 
