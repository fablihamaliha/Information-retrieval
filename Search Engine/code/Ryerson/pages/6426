<script>
    $(document).ready(function(){
    	
        $("#f6aabe01-9ca1-442b-9ccb-aad854735254-play, #f6aabe01-9ca1-442b-9ccb-aad854735254-pause").click(function() {
            if ($(this).attr("id") == "f6aabe01-9ca1-442b-9ccb-aad854735254-play") {
                $('#f6aabe01-9ca1-442b-9ccb-aad854735254').carousel('cycle');
                $(this).attr("id", "f6aabe01-9ca1-442b-9ccb-aad854735254-pause");
                $("#f6aabe01-9ca1-442b-9ccb-aad854735254-pause i").attr("class", "fa fa-pause");
            } else {
                $('#f6aabe01-9ca1-442b-9ccb-aad854735254').carousel('pause');
                $(this).attr("id", "f6aabe01-9ca1-442b-9ccb-aad854735254-play");
                $("#f6aabe01-9ca1-442b-9ccb-aad854735254-play i").attr("class", "fa fa-play");
            }
        });
        
        		$('#f6aabe01-9ca1-442b-9ccb-aad854735254').carousel('cycle');
        	
    });
</script>
</div>

