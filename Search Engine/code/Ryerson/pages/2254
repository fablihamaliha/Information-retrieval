            <div class="panel panel-default">
                <!-- Accordion Heading -->
                <div class="panel-heading container-clipboard">
                    <h3 class="panel-title">
                        <a id="accordion-1504126007731-caretaking-services" role="tab" data-toggle="collapse" data-parent="#" href="#accordion-content-1504126007731-caretaking-services" aria-controls="accordion-content-1504126007731-caretaking-services" aria-expanded="false">
