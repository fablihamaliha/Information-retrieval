          <script>
          var returnVal = false;
          function containsCurrPage(item){
              
              //returnval is being set to false on each recursi ve call
              jQuery(item).each(function(k,v){
                 if(jQuery(v).attr('class') != undefined && jQuery(v).attr('class').indexOf("currPage") >= 0){
                     jQuery(v).children("ul").children("li").children("ul").each(function(x,xx){
                         jQuery(xx).remove();
                     });
                       returnVal = true;
                 }
                 
                 if(jQuery(v).children() != null && jQuery(v).children().length > 0){
                     jQuery(v).children().each(function(kk,vv){containsCurrPage(vv);});
                 }
             });
             return returnVal;
          }
          
          
          </script>
