jQuery(document).ready(function() {
    jQuery("#component1723826260 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component1723826260 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component1723826260 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component1723826260 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component1723826260 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
 </script>
 
