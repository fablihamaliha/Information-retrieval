    $(document).ready(function(){
    	
        $("#148d02a9-abb1-481b-b75f-78d54210d8d5-play, #148d02a9-abb1-481b-b75f-78d54210d8d5-pause").click(function() {
            if ($(this).attr("id") == "148d02a9-abb1-481b-b75f-78d54210d8d5-play") {
                $('#148d02a9-abb1-481b-b75f-78d54210d8d5').carousel('cycle');
                $(this).attr("id", "148d02a9-abb1-481b-b75f-78d54210d8d5-pause");
                $("#148d02a9-abb1-481b-b75f-78d54210d8d5-pause i").attr("class", "fa fa-pause");
            } else {
                $('#148d02a9-abb1-481b-b75f-78d54210d8d5').carousel('pause');
                $(this).attr("id", "148d02a9-abb1-481b-b75f-78d54210d8d5-play");
                $("#148d02a9-abb1-481b-b75f-78d54210d8d5-play i").attr("class", "fa fa-play");
            }
        });
        
        		$('#148d02a9-abb1-481b-b75f-78d54210d8d5').carousel('cycle');
        	
    });
</script>
</div>

				

				
