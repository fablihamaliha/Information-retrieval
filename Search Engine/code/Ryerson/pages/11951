	<script>
	
	//convert to plugin in pure js....later (soon)
	$(document).ready(function() {
		$('#643469a9-8b93-45e1-be45-6a8d230d0278 table').addClass("table");
	    
			$('#643469a9-8b93-45e1-be45-6a8d230d0278 table').addClass("table-striped");
		
		$('#643469a9-8b93-45e1-be45-6a8d230d0278 table').addClass("table-borderless");
		
			var removeHeading = false;
			
			$('#643469a9-8b93-45e1-be45-6a8d230d0278 table tbody tr').eq(0).find("td").replaceWith(function(){
				return $("<th>", {attr: {'width': $(this).attr('width'), 'rowspan': $(this).attr('rowspan'), 'colspan': $(this).attr('colspan'), 'valign': $(this).attr('valign'), 'headers': $(this).attr('headers'), 'style': $(this).attr('style')}, html: $(this).html()}).css("vertical-align", $(this).attr('valign'));;
			});
			$('#643469a9-8b93-45e1-be45-6a8d230d0278 table:not(.dataTable) tbody tr').eq(0).wrap( "<thead></thead>" );
		
			$('#643469a9-8b93-45e1-be45-6a8d230d0278 table thead').prependTo('#643469a9-8b93-45e1-be45-6a8d230d0278 table');
			
			
			$('#643469a9-8b93-45e1-be45-6a8d230d0278 table').removeAttr("border");
			$('#643469a9-8b93-45e1-be45-6a8d230d0278 table').removeAttr("cellpadding");
			$('#643469a9-8b93-45e1-be45-6a8d230d0278 table').removeAttr("cellspacing");
			
			$('#643469a9-8b93-45e1-be45-6a8d230d0278 table').DataTable({
				
				"sDom":"lrtp",
				"sort":false,
				"pagingType": "simple_numbers","paging":false,
				"autoWidth":true
			});
		
			//#NOTE-REMOVE
			if(removeHeading){
				$('#643469a9-8b93-45e1-be45-6a8d230d0278 table thead').remove();
			}
			
			
		    
	    
	    
				$('#643469a9-8b93-45e1-be45-6a8d230d0278 table').attr("summary","This is the list of the Faculty Tenure Committee of FEAS in 2016-2017. Each row represents one faculty member.");
	        
	} );
	</script>
