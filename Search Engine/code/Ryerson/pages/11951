	<script>
	
	//convert to plugin in pure js....later (soon)
	$(document).ready(function() {
		$('#f05ede6f-df47-47f4-b6ec-2bb0c9346b73 table').addClass("table");
	    
			$('#f05ede6f-df47-47f4-b6ec-2bb0c9346b73 table').addClass("table-striped");
		
		$('#f05ede6f-df47-47f4-b6ec-2bb0c9346b73 table').addClass("table-borderless");
		
			var removeHeading = false;
			
			$('#f05ede6f-df47-47f4-b6ec-2bb0c9346b73 table tbody tr').eq(0).find("td").replaceWith(function(){
				return $("<th>", {attr: {'width': $(this).attr('width'), 'rowspan': $(this).attr('rowspan'), 'colspan': $(this).attr('colspan'), 'valign': $(this).attr('valign'), 'headers': $(this).attr('headers'), 'style': $(this).attr('style')}, html: $(this).html()}).css("vertical-align", $(this).attr('valign'));;
			});
			$('#f05ede6f-df47-47f4-b6ec-2bb0c9346b73 table:not(.dataTable) tbody tr').eq(0).wrap( "<thead></thead>" );
		
			$('#f05ede6f-df47-47f4-b6ec-2bb0c9346b73 table thead').prependTo('#f05ede6f-df47-47f4-b6ec-2bb0c9346b73 table');
			
			
			$('#f05ede6f-df47-47f4-b6ec-2bb0c9346b73 table').removeAttr("border");
			$('#f05ede6f-df47-47f4-b6ec-2bb0c9346b73 table').removeAttr("cellpadding");
			$('#f05ede6f-df47-47f4-b6ec-2bb0c9346b73 table').removeAttr("cellspacing");
			
			$('#f05ede6f-df47-47f4-b6ec-2bb0c9346b73 table').DataTable({
				
				"sDom":"lrtp",
				"sort":false,
				"pagingType": "simple_numbers","paging":false,
				"autoWidth":true
			});
		
			//#NOTE-REMOVE
			if(removeHeading){
				$('#f05ede6f-df47-47f4-b6ec-2bb0c9346b73 table thead').remove();
			}
			
			
		    
	    
	    
				$('#f05ede6f-df47-47f4-b6ec-2bb0c9346b73 table').attr("summary","This is the list of the Faculty Tenure Committee of FCAD in 2016-2017. Each row represents one faculty member.");
	        
	} );
	</script>
<script>$(document).ready(function(){addCoursePopover("#f05ede6f-df47-47f4-b6ec-2bb0c9346b73");});</script></div>

				</div>

                    
