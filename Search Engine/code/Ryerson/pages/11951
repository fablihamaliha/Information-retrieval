				return $("<th>", {attr: {'width': $(this).attr('width'), 'rowspan': $(this).attr('rowspan'), 'colspan': $(this).attr('colspan'), 'valign': $(this).attr('valign'), 'headers': $(this).attr('headers'), 'style': $(this).attr('style')}, html: $(this).html()}).css("vertical-align", $(this).attr('valign'));;
			});
			$('#92046429-3a91-4762-8800-5c725b23abf2 table:not(.dataTable) tbody tr').eq(0).wrap( "<thead></thead>" );
		
			$('#92046429-3a91-4762-8800-5c725b23abf2 table thead').prependTo('#92046429-3a91-4762-8800-5c725b23abf2 table');
			
			
			$('#92046429-3a91-4762-8800-5c725b23abf2 table').removeAttr("border");
