			$('#92046429-3a91-4762-8800-5c725b23abf2 table').removeAttr("cellspacing");
			
			$('#92046429-3a91-4762-8800-5c725b23abf2 table').DataTable({
				
				"sDom":"lrtp",
				"sort":false,
				"pagingType": "simple_numbers","paging":false,
				"autoWidth":true
			});
		
			//#NOTE-REMOVE
			if(removeHeading){
				$('#92046429-3a91-4762-8800-5c725b23abf2 table thead').remove();
			}
			
			
		    
	    
	    
				$('#92046429-3a91-4762-8800-5c725b23abf2 table').attr("summary","The list of Faculty Tenure Committee members in Arts, 2016-2017. Each row represents one member.");
	        
	} );
	</script>
