			$('#c3972ea4-a80c-4e5b-9852-9834bce75ae8 table thead').prependTo('#c3972ea4-a80c-4e5b-9852-9834bce75ae8 table');
			
			
			$('#c3972ea4-a80c-4e5b-9852-9834bce75ae8 table').removeAttr("border");
			$('#c3972ea4-a80c-4e5b-9852-9834bce75ae8 table').removeAttr("cellpadding");
			$('#c3972ea4-a80c-4e5b-9852-9834bce75ae8 table').removeAttr("cellspacing");
			
			$('#c3972ea4-a80c-4e5b-9852-9834bce75ae8 table').DataTable({
				
				"sDom":"lrtp",
				"sort":false,
				"pagingType": "simple_numbers","paging":false,
				"autoWidth":true
			});
		
			//#NOTE-REMOVE
			if(removeHeading){
				$('#c3972ea4-a80c-4e5b-9852-9834bce75ae8 table thead').remove();
			}
			
			
		    
	    
	    
				$('#c3972ea4-a80c-4e5b-9852-9834bce75ae8 table').attr("summary","This is the list of the Faculty Tenure Committee of FCS in 2016-2017. Each row represents one faculty member.");
	        
	} );
	</script>
