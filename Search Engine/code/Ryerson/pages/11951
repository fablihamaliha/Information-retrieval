	<script>
	
	//convert to plugin in pure js....later (soon)
	$(document).ready(function() {
		$('#31b24bd4-72ac-4cfa-84d0-c14730d74589 table').addClass("table");
	    
			$('#31b24bd4-72ac-4cfa-84d0-c14730d74589 table').addClass("table-striped");
		
		$('#31b24bd4-72ac-4cfa-84d0-c14730d74589 table').addClass("table-borderless");
		
			var removeHeading = false;
			
			$('#31b24bd4-72ac-4cfa-84d0-c14730d74589 table tbody tr').eq(0).find("td").replaceWith(function(){
				return $("<th>", {attr: {'width': $(this).attr('width'), 'rowspan': $(this).attr('rowspan'), 'colspan': $(this).attr('colspan'), 'valign': $(this).attr('valign'), 'headers': $(this).attr('headers'), 'style': $(this).attr('style')}, html: $(this).html()}).css("vertical-align", $(this).attr('valign'));;
			});
			$('#31b24bd4-72ac-4cfa-84d0-c14730d74589 table:not(.dataTable) tbody tr').eq(0).wrap( "<thead></thead>" );
		
			$('#31b24bd4-72ac-4cfa-84d0-c14730d74589 table thead').prependTo('#31b24bd4-72ac-4cfa-84d0-c14730d74589 table');
			
			
			$('#31b24bd4-72ac-4cfa-84d0-c14730d74589 table').removeAttr("border");
			$('#31b24bd4-72ac-4cfa-84d0-c14730d74589 table').removeAttr("cellpadding");
			$('#31b24bd4-72ac-4cfa-84d0-c14730d74589 table').removeAttr("cellspacing");
			
			$('#31b24bd4-72ac-4cfa-84d0-c14730d74589 table').DataTable({
				
				"sDom":"lrtp",
				"sort":false,
				"pagingType": "simple_numbers","paging":false,
				"autoWidth":true
			});
		
			//#NOTE-REMOVE
			if(removeHeading){
				$('#31b24bd4-72ac-4cfa-84d0-c14730d74589 table thead').remove();
			}
			
			
		    
	    
	    
				$('#31b24bd4-72ac-4cfa-84d0-c14730d74589 table').attr("summary","This is the list of the Faculty Tenure Committee of FCS in 2016-2017. Each row represents one faculty member.");
	        
	} );
	</script>
<script>$(document).ready(function(){addCoursePopover("#31b24bd4-72ac-4cfa-84d0-c14730d74589");});</script></div>

				</div>

                    
