        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component443779412 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component443779412 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component443779412 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component443779412 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
