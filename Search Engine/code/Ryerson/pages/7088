                jQuery("#236f38bd-67e9-412d-a26b-61c6a4ee7183 button").click(function() {
                	jQuery("#236f38bd-67e9-412d-a26b-61c6a4ee7183.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6594928, "lng": -79.3787833}');
