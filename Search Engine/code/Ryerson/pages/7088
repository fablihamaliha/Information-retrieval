                        if(latLng != null){
                            drawDynamicMapByLatLng('236f38bd-67e9-412d-a26b-61c6a4ee7183',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('236f38bd-67e9-412d-a26b-61c6a4ee7183','340 Church Street, Toronto, ON','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('236f38bd-67e9-412d-a26b-61c6a4ee7183',latLng,'340 Church Street, Toronto, ON','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('236f38bd-67e9-412d-a26b-61c6a4ee7183','340 Church Street, Toronto, ON','1140px','600px',18,true);
                        }
                    }
                });
            });
