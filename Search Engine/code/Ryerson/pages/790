    $(document).ready(function(){
    	
        $("#789d3939-6f30-4083-810d-27de69eb8647-play, #789d3939-6f30-4083-810d-27de69eb8647-pause").click(function() {
            if ($(this).attr("id") == "789d3939-6f30-4083-810d-27de69eb8647-play") {
                $('#789d3939-6f30-4083-810d-27de69eb8647').carousel('cycle');
                $(this).attr("id", "789d3939-6f30-4083-810d-27de69eb8647-pause");
                $("#789d3939-6f30-4083-810d-27de69eb8647-pause i").attr("class", "fa fa-pause");
            } else {
                $('#789d3939-6f30-4083-810d-27de69eb8647').carousel('pause');
                $(this).attr("id", "789d3939-6f30-4083-810d-27de69eb8647-play");
                $("#789d3939-6f30-4083-810d-27de69eb8647-play i").attr("class", "fa fa-play");
