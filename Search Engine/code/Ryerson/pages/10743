                jQuery("#754627f1-5c13-4d10-97f9-ba3b4e791792 button").click(function() {
                	jQuery("#754627f1-5c13-4d10-97f9-ba3b4e791792.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6600414, "lng": -79.3771506}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('754627f1-5c13-4d10-97f9-ba3b4e791792',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('754627f1-5c13-4d10-97f9-ba3b4e791792','99 Gerrard St E, Toronto, ON, M5B 2M2, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('754627f1-5c13-4d10-97f9-ba3b4e791792',latLng,'99 Gerrard St E, Toronto, ON, M5B 2M2, Canada','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('754627f1-5c13-4d10-97f9-ba3b4e791792','99 Gerrard St E, Toronto, ON, M5B 2M2, Canada','1140px','600px',18,true);
                        }
                    }
                });
            });
