<script>
    $(document).ready(function(){
    	
        $("#3fa10696-0ac0-425d-bd35-b5317cae6f14-play, #3fa10696-0ac0-425d-bd35-b5317cae6f14-pause").click(function() {
            if ($(this).attr("id") == "3fa10696-0ac0-425d-bd35-b5317cae6f14-play") {
                $('#3fa10696-0ac0-425d-bd35-b5317cae6f14').carousel('cycle');
                $(this).attr("id", "3fa10696-0ac0-425d-bd35-b5317cae6f14-pause");
                $("#3fa10696-0ac0-425d-bd35-b5317cae6f14-pause i").attr("class", "fa fa-pause");
            } else {
                $('#3fa10696-0ac0-425d-bd35-b5317cae6f14').carousel('pause');
                $(this).attr("id", "3fa10696-0ac0-425d-bd35-b5317cae6f14-play");
                $("#3fa10696-0ac0-425d-bd35-b5317cae6f14-play i").attr("class", "fa fa-play");
