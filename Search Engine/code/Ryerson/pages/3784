jQuery(document).ready(function() {
    jQuery("#component934685418 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component934685418 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component934685418 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component934685418 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component934685418 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
 </script>
 
