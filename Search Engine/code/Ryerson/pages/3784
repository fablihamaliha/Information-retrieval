jQuery(document).ready(function() {
    jQuery("#component529507611 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component529507611 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component529507611 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component529507611 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component529507611 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
 </script>
 
