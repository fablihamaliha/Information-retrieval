jQuery(document).ready(function() {
    jQuery("#component554877704 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component554877704 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component554877704 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component554877704 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component554877704 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
