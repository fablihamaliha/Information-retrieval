<script type="text/javascript">
jQuery(document).ready(function() {
    jQuery("#component721210133 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component721210133 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component721210133 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component721210133 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component721210133 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
 </script>
 
