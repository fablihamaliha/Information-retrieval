        <script>
        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#d18f3e92-3ddb-4f48-a080-66b62d7ec532 button").click(function() {
                	jQuery("#d18f3e92-3ddb-4f48-a080-66b62d7ec532.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6577631, "lng": -79.38116409999999}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('d18f3e92-3ddb-4f48-a080-66b62d7ec532',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('d18f3e92-3ddb-4f48-a080-66b62d7ec532','345 Yonge Street, Toronto, ON, M5B 2K3','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('d18f3e92-3ddb-4f48-a080-66b62d7ec532',latLng,'345 Yonge Street, Toronto, ON, M5B 2K3','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('d18f3e92-3ddb-4f48-a080-66b62d7ec532','345 Yonge Street, Toronto, ON, M5B 2K3','1140px','600px',18,true);
                        }
                    }
                });
            });
