    $(document).ready(function(){
    	
        $("#64f5c00a-144a-466d-a266-2c6c54bc3cbb-play, #64f5c00a-144a-466d-a266-2c6c54bc3cbb-pause").click(function() {
            if ($(this).attr("id") == "64f5c00a-144a-466d-a266-2c6c54bc3cbb-play") {
                $('#64f5c00a-144a-466d-a266-2c6c54bc3cbb').carousel('cycle');
                $(this).attr("id", "64f5c00a-144a-466d-a266-2c6c54bc3cbb-pause");
                $("#64f5c00a-144a-466d-a266-2c6c54bc3cbb-pause i").attr("class", "fa fa-pause");
            } else {
                $('#64f5c00a-144a-466d-a266-2c6c54bc3cbb').carousel('pause');
                $(this).attr("id", "64f5c00a-144a-466d-a266-2c6c54bc3cbb-play");
                $("#64f5c00a-144a-466d-a266-2c6c54bc3cbb-play i").attr("class", "fa fa-play");
            }
        });
        
