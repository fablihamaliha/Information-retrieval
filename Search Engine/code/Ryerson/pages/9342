	<script>
	
	//convert to plugin in pure js....later (soon)
	$(document).ready(function() {
		$('#0f79a435-1c2a-4a70-8ee9-e7213329ca33 table').addClass("table");
	    
			$('#0f79a435-1c2a-4a70-8ee9-e7213329ca33 table').addClass("table-striped");
		
		$('#0f79a435-1c2a-4a70-8ee9-e7213329ca33 table').addClass("table-borderless");
		
			var removeHeading = false;
			
			$('#0f79a435-1c2a-4a70-8ee9-e7213329ca33 table tbody tr').eq(0).find("td").replaceWith(function(){
				return $("<th>", {attr: {'width': $(this).attr('width'), 'rowspan': $(this).attr('rowspan'), 'colspan': $(this).attr('colspan'), 'valign': $(this).attr('valign'), 'headers': $(this).attr('headers'), 'style': $(this).attr('style')}, html: $(this).html()}).css("vertical-align", $(this).attr('valign'));;
			});
			$('#0f79a435-1c2a-4a70-8ee9-e7213329ca33 table:not(.dataTable) tbody tr').eq(0).wrap( "<thead></thead>" );
		
			$('#0f79a435-1c2a-4a70-8ee9-e7213329ca33 table thead').prependTo('#0f79a435-1c2a-4a70-8ee9-e7213329ca33 table');
			
			
			$('#0f79a435-1c2a-4a70-8ee9-e7213329ca33 table').removeAttr("border");
			$('#0f79a435-1c2a-4a70-8ee9-e7213329ca33 table').removeAttr("cellpadding");
			$('#0f79a435-1c2a-4a70-8ee9-e7213329ca33 table').removeAttr("cellspacing");
			
			$('#0f79a435-1c2a-4a70-8ee9-e7213329ca33 table').DataTable({
				
				"sDom":"lrtp",
				"sort":false,
				"pagingType": "simple_numbers","paging":false,
				"autoWidth":true
			});
		
			//#NOTE-REMOVE
			if(removeHeading){
				$('#0f79a435-1c2a-4a70-8ee9-e7213329ca33 table thead').remove();
			}
			
			
		    
	    
	    
	} );
	</script>
