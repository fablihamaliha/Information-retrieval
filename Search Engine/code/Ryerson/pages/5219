        <script>
        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#c793b26e-865d-4e91-84b4-851971f35741 button").click(function() {
                	jQuery("#c793b26e-865d-4e91-84b4-851971f35741.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6559513, "lng": -79.381742}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('c793b26e-865d-4e91-84b4-851971f35741',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('c793b26e-865d-4e91-84b4-851971f35741','1 Dundas Street West, Toronto, ON m5b 2k3','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('c793b26e-865d-4e91-84b4-851971f35741',latLng,'1 Dundas Street West, Toronto, ON m5b 2k3','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('c793b26e-865d-4e91-84b4-851971f35741','1 Dundas Street West, Toronto, ON m5b 2k3','1140px','600px',18,true);
                        }
                    }
                });
            });
        </script>
    
    
    
</div>
