        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#781969b9-f5b1-47d9-a22a-5ba4c7072e69 button").click(function() {
                	jQuery("#781969b9-f5b1-47d9-a22a-5ba4c7072e69.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6586299, "lng": -79.3770727}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('781969b9-f5b1-47d9-a22a-5ba4c7072e69',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('781969b9-f5b1-47d9-a22a-5ba4c7072e69','80 Gould St, Toronto, ON M5B 2M7','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('781969b9-f5b1-47d9-a22a-5ba4c7072e69',latLng,'80 Gould St, Toronto, ON M5B 2M7','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('781969b9-f5b1-47d9-a22a-5ba4c7072e69','80 Gould St, Toronto, ON M5B 2M7','1140px','600px',18,true);
                        }
                    }
                });
            });
