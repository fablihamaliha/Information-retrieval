        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#ba81ca82-0ea0-4fd7-a596-f16f55a07531 button").click(function() {
                	jQuery("#ba81ca82-0ea0-4fd7-a596-f16f55a07531.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.659324, "lng": -79.378299}');
