                        if(latLng != null){
                            drawDynamicMapByLatLng('ba81ca82-0ea0-4fd7-a596-f16f55a07531',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('ba81ca82-0ea0-4fd7-a596-f16f55a07531','325 church Street, Toronto, ON, M5B 1Z2, Canada','600px',18,true);
                        }
