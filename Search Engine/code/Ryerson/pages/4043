        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#30e16892-227a-4b85-9857-afe83c031706 button").click(function() {
                	jQuery("#30e16892-227a-4b85-9857-afe83c031706.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6570679, "lng": -79.380062}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('30e16892-227a-4b85-9857-afe83c031706',latLng,'600px',15,false);
                        } else{
                            drawDynamicMapByAddress('30e16892-227a-4b85-9857-afe83c031706','285 Victoria Street, Toronto, ON, M5B 2K3, Canada','600px',15,false);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('30e16892-227a-4b85-9857-afe83c031706',latLng,'285 Victoria Street, Toronto, ON, M5B 2K3, Canada','1140px','600px',15,false);
                        } else{
                            drawStaticMapByAddress('30e16892-227a-4b85-9857-afe83c031706','285 Victoria Street, Toronto, ON, M5B 2K3, Canada','1140px','600px',15,false);
                        }
                    }
                });
