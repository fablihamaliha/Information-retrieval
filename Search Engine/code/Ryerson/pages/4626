jQuery(document).ready(function() {
    jQuery("#component221880104 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component221880104 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component221880104 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component221880104 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component221880104 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
 </script>
 
