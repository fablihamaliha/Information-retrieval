        <script>
        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#fe8a06c6-d717-4265-bb2b-a4ba6103d695 button").click(function() {
                	jQuery("#fe8a06c6-d717-4265-bb2b-a4ba6103d695.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6559513, "lng": -79.381742}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('fe8a06c6-d717-4265-bb2b-a4ba6103d695',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('fe8a06c6-d717-4265-bb2b-a4ba6103d695','1 Dundas Street West, Toronto, ON m5b 2k3','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('fe8a06c6-d717-4265-bb2b-a4ba6103d695',latLng,'1 Dundas Street West, Toronto, ON m5b 2k3','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('fe8a06c6-d717-4265-bb2b-a4ba6103d695','1 Dundas Street West, Toronto, ON m5b 2k3','1140px','600px',18,true);
                        }
                    }
                });
            });
        </script>
    
    
    
</div>
