			$(this).attr('id', 'rye-ff3dcfa7-05a4-4cee-a0df-859381e32ebd_upload'+i).wrap("<span class='htmlSnippetForm'></span>").after("<label for='rye-ff3dcfa7-05a4-4cee-a0df-859381e32ebd_upload"+i+"' class='customInput'>Choose file</label>");
		} else {
			$(this).wrap("<span class='htmlSnippetForm'></span>").after("<span class='customInput'></span>");
		}
	}
});
</script>

