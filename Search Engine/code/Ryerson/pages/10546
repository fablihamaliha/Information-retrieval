        <script>
        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#05b79195-fb8f-49c9-b407-7c1868c21059 button").click(function() {
                	jQuery("#05b79195-fb8f-49c9-b407-7c1868c21059.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6558668, "lng": -79.3819145}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('05b79195-fb8f-49c9-b407-7c1868c21059',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('05b79195-fb8f-49c9-b407-7c1868c21059','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('05b79195-fb8f-49c9-b407-7c1868c21059',latLng,'1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('05b79195-fb8f-49c9-b407-7c1868c21059','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','1140px','600px',18,true);
                        }
                    }
                });
            });
        </script>
    
    
    
