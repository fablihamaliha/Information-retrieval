                	jQuery("#60c58cd5-a9c5-47b9-8cb2-bdd090dedd51.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = false;
                    var latLng = JSON.parse('{"lat": 43.6556761, "lng": -79.3828745}');
