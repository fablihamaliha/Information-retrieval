                        if(latLng != null){
                            drawDynamicMapByLatLng('60c58cd5-a9c5-47b9-8cb2-bdd090dedd51',latLng,'300px',15,true);
                        } else{
                            drawDynamicMapByAddress('60c58cd5-a9c5-47b9-8cb2-bdd090dedd51','55 Dundas St. West, Toronto, ON, M5G 2C5','300px',15,true);
                        }
