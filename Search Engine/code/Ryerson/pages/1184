jQuery(document).ready(function() {
    jQuery("#component841052320 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component841052320 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component841052320 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component841052320 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component841052320 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
