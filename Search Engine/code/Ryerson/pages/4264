<script>
    $(document).ready(function(){
    	
        $("#d4cc9ae2-f3d3-4cc2-abc3-aaecabe1e4f8-play, #d4cc9ae2-f3d3-4cc2-abc3-aaecabe1e4f8-pause").click(function() {
            if ($(this).attr("id") == "d4cc9ae2-f3d3-4cc2-abc3-aaecabe1e4f8-play") {
                $('#d4cc9ae2-f3d3-4cc2-abc3-aaecabe1e4f8').carousel('cycle');
                $(this).attr("id", "d4cc9ae2-f3d3-4cc2-abc3-aaecabe1e4f8-pause");
                $("#d4cc9ae2-f3d3-4cc2-abc3-aaecabe1e4f8-pause i").attr("class", "fa fa-pause");
            } else {
                $('#d4cc9ae2-f3d3-4cc2-abc3-aaecabe1e4f8').carousel('pause');
