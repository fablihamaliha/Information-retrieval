        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#fd4e1362-125a-46ae-8d90-6e19c983ea55 button").click(function() {
                	jQuery("#fd4e1362-125a-46ae-8d90-6e19c983ea55.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6558668, "lng": -79.3819145}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('fd4e1362-125a-46ae-8d90-6e19c983ea55',latLng,'600px',18,false);
                        } else{
                            drawDynamicMapByAddress('fd4e1362-125a-46ae-8d90-6e19c983ea55','1 Dundas St. West Toronto, ON M5S 2Z3','600px',18,false);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('fd4e1362-125a-46ae-8d90-6e19c983ea55',latLng,'1 Dundas St. West Toronto, ON M5S 2Z3','1140px','600px',18,false);
                        } else{
                            drawStaticMapByAddress('fd4e1362-125a-46ae-8d90-6e19c983ea55','1 Dundas St. West Toronto, ON M5S 2Z3','1140px','600px',18,false);
                        }
                    }
                });
            });
        </script>
    
    
    
