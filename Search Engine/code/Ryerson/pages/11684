jQuery(document).ready(function() {
    jQuery("#component442269820 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component442269820 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component442269820 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component442269820 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component442269820 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
 </script>
 
