    $(document).ready(function(){
    	
        $("#3933d2db-a338-46ea-b7d8-383759cb5ee3-play, #3933d2db-a338-46ea-b7d8-383759cb5ee3-pause").click(function() {
            if ($(this).attr("id") == "3933d2db-a338-46ea-b7d8-383759cb5ee3-play") {
                $('#3933d2db-a338-46ea-b7d8-383759cb5ee3').carousel('cycle');
                $(this).attr("id", "3933d2db-a338-46ea-b7d8-383759cb5ee3-pause");
                $("#3933d2db-a338-46ea-b7d8-383759cb5ee3-pause i").attr("class", "fa fa-pause");
