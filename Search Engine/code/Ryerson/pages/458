<script>
    $(document).ready(function(){
    	
        $("#743aca8f-0bd1-4c31-8925-9316eedccc0a-play, #743aca8f-0bd1-4c31-8925-9316eedccc0a-pause").click(function() {
            if ($(this).attr("id") == "743aca8f-0bd1-4c31-8925-9316eedccc0a-play") {
                $('#743aca8f-0bd1-4c31-8925-9316eedccc0a').carousel('cycle');
                $(this).attr("id", "743aca8f-0bd1-4c31-8925-9316eedccc0a-pause");
                $("#743aca8f-0bd1-4c31-8925-9316eedccc0a-pause i").attr("class", "fa fa-pause");
            } else {
                $('#743aca8f-0bd1-4c31-8925-9316eedccc0a').carousel('pause');
                $(this).attr("id", "743aca8f-0bd1-4c31-8925-9316eedccc0a-play");
                $("#743aca8f-0bd1-4c31-8925-9316eedccc0a-play i").attr("class", "fa fa-play");
            }
