        <script>
        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#abaef6a0-c3ac-47dc-bb3c-2f4588dc06d8 button").click(function() {
                	jQuery("#abaef6a0-c3ac-47dc-bb3c-2f4588dc06d8.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.65829859999999, "lng": -79.3807846}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('abaef6a0-c3ac-47dc-bb3c-2f4588dc06d8',latLng,'600px',20,true);
                        } else{
                            drawDynamicMapByAddress('abaef6a0-c3ac-47dc-bb3c-2f4588dc06d8','350 Victoria Street, Toronto, ON, M5B 2K3, Canada','600px',20,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('abaef6a0-c3ac-47dc-bb3c-2f4588dc06d8',latLng,'350 Victoria Street, Toronto, ON, M5B 2K3, Canada','1140px','600px',20,true);
                        } else{
                            drawStaticMapByAddress('abaef6a0-c3ac-47dc-bb3c-2f4588dc06d8','350 Victoria Street, Toronto, ON, M5B 2K3, Canada','1140px','600px',20,true);
                        }
                    }
                });
            });
        </script>
    
    
    
