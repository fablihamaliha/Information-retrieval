    $(document).ready(function(){
    	
        $("#fc065f80-3314-46c3-a772-d55234db82ba-play, #fc065f80-3314-46c3-a772-d55234db82ba-pause").click(function() {
            if ($(this).attr("id") == "fc065f80-3314-46c3-a772-d55234db82ba-play") {
                $('#fc065f80-3314-46c3-a772-d55234db82ba').carousel('cycle');
                $(this).attr("id", "fc065f80-3314-46c3-a772-d55234db82ba-pause");
                $("#fc065f80-3314-46c3-a772-d55234db82ba-pause i").attr("class", "fa fa-pause");
            } else {
                $('#fc065f80-3314-46c3-a772-d55234db82ba').carousel('pause');
                $(this).attr("id", "fc065f80-3314-46c3-a772-d55234db82ba-play");
                $("#fc065f80-3314-46c3-a772-d55234db82ba-play i").attr("class", "fa fa-play");
