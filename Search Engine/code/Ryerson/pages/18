<script>
    $(document).ready(function(){
    	
        $("#a5ec0755-a718-4a65-ad42-d2f9090d52ac-play, #a5ec0755-a718-4a65-ad42-d2f9090d52ac-pause").click(function() {
            if ($(this).attr("id") == "a5ec0755-a718-4a65-ad42-d2f9090d52ac-play") {
                $('#a5ec0755-a718-4a65-ad42-d2f9090d52ac').carousel('cycle');
                $(this).attr("id", "a5ec0755-a718-4a65-ad42-d2f9090d52ac-pause");
                $("#a5ec0755-a718-4a65-ad42-d2f9090d52ac-pause i").attr("class", "fa fa-pause");
            } else {
                $('#a5ec0755-a718-4a65-ad42-d2f9090d52ac').carousel('pause');
                $(this).attr("id", "a5ec0755-a718-4a65-ad42-d2f9090d52ac-play");
                $("#a5ec0755-a718-4a65-ad42-d2f9090d52ac-play i").attr("class", "fa fa-play");
            }
        });
        
        		$('#a5ec0755-a718-4a65-ad42-d2f9090d52ac').carousel('cycle');
        	
    });
</script>
