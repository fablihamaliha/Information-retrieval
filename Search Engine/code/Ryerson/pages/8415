jQuery(document).ready(function() {
    jQuery("#component396143098 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component396143098 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component396143098 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component396143098 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component396143098 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
