                jQuery("#bacf07a6-bfa9-4914-b8ee-25d6d729fa98 button").click(function() {
                	jQuery("#bacf07a6-bfa9-4914-b8ee-25d6d729fa98.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6586299, "lng": -79.3770727}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('bacf07a6-bfa9-4914-b8ee-25d6d729fa98',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('bacf07a6-bfa9-4914-b8ee-25d6d729fa98','80 Gould St, Toronto, ON M5B 2M7','600px',18,true);
                        }
