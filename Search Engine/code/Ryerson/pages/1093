jQuery(document).ready(function() {
    jQuery("#component1816024716 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component1816024716 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component1816024716 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component1816024716 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component1816024716 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
