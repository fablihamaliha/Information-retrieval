<script>
    $(document).ready(function(){
    	
        $("#e8bd0154-6ffb-4826-b493-2a89c6e03b7d-play, #e8bd0154-6ffb-4826-b493-2a89c6e03b7d-pause").click(function() {
            if ($(this).attr("id") == "e8bd0154-6ffb-4826-b493-2a89c6e03b7d-play") {
                $('#e8bd0154-6ffb-4826-b493-2a89c6e03b7d').carousel('cycle');
                $(this).attr("id", "e8bd0154-6ffb-4826-b493-2a89c6e03b7d-pause");
                $("#e8bd0154-6ffb-4826-b493-2a89c6e03b7d-pause i").attr("class", "fa fa-pause");
            } else {
                $('#e8bd0154-6ffb-4826-b493-2a89c6e03b7d').carousel('pause');
                $(this).attr("id", "e8bd0154-6ffb-4826-b493-2a89c6e03b7d-play");
                $("#e8bd0154-6ffb-4826-b493-2a89c6e03b7d-play i").attr("class", "fa fa-play");
            }
        });
        
