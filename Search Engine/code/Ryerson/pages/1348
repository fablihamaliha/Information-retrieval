jQuery(document).ready(function() {
    jQuery("#component374176347 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component374176347 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component374176347 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component374176347 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component374176347 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
 </script>
 
