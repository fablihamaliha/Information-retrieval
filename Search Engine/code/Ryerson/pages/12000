        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#3ba64a5b-e5a9-43a8-931e-d2fc93f11167 button").click(function() {
                	jQuery("#3ba64a5b-e5a9-43a8-931e-d2fc93f11167.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.65696579999999, "lng": -79.3804789}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('3ba64a5b-e5a9-43a8-931e-d2fc93f11167',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('3ba64a5b-e5a9-43a8-931e-d2fc93f11167','10 Dundas Street East, Toronto, ON, M5B 2G9, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('3ba64a5b-e5a9-43a8-931e-d2fc93f11167',latLng,'10 Dundas Street East, Toronto, ON, M5B 2G9, Canada','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('3ba64a5b-e5a9-43a8-931e-d2fc93f11167','10 Dundas Street East, Toronto, ON, M5B 2G9, Canada','1140px','600px',18,true);
                        }
                    }
                });
            });
        </script>
    
    
    
