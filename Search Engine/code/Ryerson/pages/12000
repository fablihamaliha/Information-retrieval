	if (!$(this).hasClass('form-control')) {
		if ($(this).is("input[type='file']")) {
			$(this).attr('id', 'rye-a07de23d-1a71-498f-8500-8bdca9b10263_upload'+i).wrap("<span class='htmlSnippetForm'></span>").after("<label for='rye-a07de23d-1a71-498f-8500-8bdca9b10263_upload"+i+"' class='customInput'>Choose file</label>");
		} else {
			$(this).wrap("<span class='htmlSnippetForm'></span>").after("<span class='customInput'></span>");
		}
	}
});
</script>

