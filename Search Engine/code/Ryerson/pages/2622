			var tempForm;

			function wshopDetail(id) {
				url = "../workshop_detail.cfm?id="+id+"&isView=true";
				window.open(url, "Workshop_Detail","width=650,height=450,resizable=yes,scrollbars=yes");
			}

			function expend(index) {
				e = document.getElementById("desc"+index);
				if(e.style.display == "none") {
					e.style.display = "";
				}
				else
					e.style.display = "none";
			}
