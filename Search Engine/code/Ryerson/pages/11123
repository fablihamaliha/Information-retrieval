                $('#e47e6e90-8f5f-434a-a29b-c73628837cc6').carousel('cycle');
                $(this).attr("id", "e47e6e90-8f5f-434a-a29b-c73628837cc6-pause");
                $("#e47e6e90-8f5f-434a-a29b-c73628837cc6-pause i").attr("class", "fa fa-pause");
            } else {
                $('#e47e6e90-8f5f-434a-a29b-c73628837cc6').carousel('pause');
                $(this).attr("id", "e47e6e90-8f5f-434a-a29b-c73628837cc6-play");
                $("#e47e6e90-8f5f-434a-a29b-c73628837cc6-play i").attr("class", "fa fa-play");
