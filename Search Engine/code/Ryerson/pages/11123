    $(document).ready(function(){
    	
        $("#131bbe10-af15-4b0e-99de-cbfe62153202-play, #131bbe10-af15-4b0e-99de-cbfe62153202-pause").click(function() {
            if ($(this).attr("id") == "131bbe10-af15-4b0e-99de-cbfe62153202-play") {
                $('#131bbe10-af15-4b0e-99de-cbfe62153202').carousel('cycle');
                $(this).attr("id", "131bbe10-af15-4b0e-99de-cbfe62153202-pause");
                $("#131bbe10-af15-4b0e-99de-cbfe62153202-pause i").attr("class", "fa fa-pause");
            } else {
                $('#131bbe10-af15-4b0e-99de-cbfe62153202').carousel('pause');
                $(this).attr("id", "131bbe10-af15-4b0e-99de-cbfe62153202-play");
                $("#131bbe10-af15-4b0e-99de-cbfe62153202-play i").attr("class", "fa fa-play");
            }
        });
        
        		$('#131bbe10-af15-4b0e-99de-cbfe62153202').carousel('cycle');
        	
    });
