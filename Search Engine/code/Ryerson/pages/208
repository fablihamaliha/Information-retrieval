                        if(latLng != null){
                            drawDynamicMapByLatLng('5b5f9e2f-8dea-46aa-a708-1f8f90c8ef93',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('5b5f9e2f-8dea-46aa-a708-1f8f90c8ef93','1 Dundas Street West, Toronto, ON m5b 2k3','600px',18,true);
