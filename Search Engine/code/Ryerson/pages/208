<script>
    $(document).ready(function(){
    	
        $("#87752972-5a35-4639-a9a2-b644c9302caa-play, #87752972-5a35-4639-a9a2-b644c9302caa-pause").click(function() {
            if ($(this).attr("id") == "87752972-5a35-4639-a9a2-b644c9302caa-play") {
                $('#87752972-5a35-4639-a9a2-b644c9302caa').carousel('cycle');
                $(this).attr("id", "87752972-5a35-4639-a9a2-b644c9302caa-pause");
                $("#87752972-5a35-4639-a9a2-b644c9302caa-pause i").attr("class", "fa fa-pause");
            } else {
                $('#87752972-5a35-4639-a9a2-b644c9302caa').carousel('pause');
                $(this).attr("id", "87752972-5a35-4639-a9a2-b644c9302caa-play");
                $("#87752972-5a35-4639-a9a2-b644c9302caa-play i").attr("class", "fa fa-play");
            }
        });
        
