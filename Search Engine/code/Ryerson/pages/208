        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#5b5f9e2f-8dea-46aa-a708-1f8f90c8ef93 button").click(function() {
                	jQuery("#5b5f9e2f-8dea-46aa-a708-1f8f90c8ef93.res-map-placeholder").removeClass('res-map-placeholder');
