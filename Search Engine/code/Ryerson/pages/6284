                        if(latLng != null){
                            drawDynamicMapByLatLng('d06def94-5a86-4d80-98ee-97d5df26eb8f',latLng,'600px',18,false);
                        } else{
                            drawDynamicMapByAddress('d06def94-5a86-4d80-98ee-97d5df26eb8f','60 Gould Street Toronto, Ontario ','600px',18,false);
