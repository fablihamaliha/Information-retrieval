<script>
$("#rye-73a35135-3485-4e79-97df-28bb8b32e8dd input, #rye-73a35135-3485-4e79-97df-28bb8b32e8dd select").each(function(i, v) {
	if (!$(this).hasClass('form-control')) {
		if ($(this).is("input[type='file']")) {
			$(this).attr('id', 'rye-73a35135-3485-4e79-97df-28bb8b32e8dd_upload'+i).wrap("<span class='htmlSnippetForm'></span>").after("<label for='rye-73a35135-3485-4e79-97df-28bb8b32e8dd_upload"+i+"' class='customInput'>Choose file</label>");
		} else {
			$(this).wrap("<span class='htmlSnippetForm'></span>").after("<span class='customInput'></span>");
		}
	}
});
</script>

