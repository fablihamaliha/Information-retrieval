    $(document).ready(function(){
    	
        $("#4a241b1e-fe8d-4bee-aa1a-a0d32da2d292-play, #4a241b1e-fe8d-4bee-aa1a-a0d32da2d292-pause").click(function() {
            if ($(this).attr("id") == "4a241b1e-fe8d-4bee-aa1a-a0d32da2d292-play") {
                $('#4a241b1e-fe8d-4bee-aa1a-a0d32da2d292').carousel('cycle');
                $(this).attr("id", "4a241b1e-fe8d-4bee-aa1a-a0d32da2d292-pause");
                $("#4a241b1e-fe8d-4bee-aa1a-a0d32da2d292-pause i").attr("class", "fa fa-pause");
            } else {
                $('#4a241b1e-fe8d-4bee-aa1a-a0d32da2d292').carousel('pause');
                $(this).attr("id", "4a241b1e-fe8d-4bee-aa1a-a0d32da2d292-play");
                $("#4a241b1e-fe8d-4bee-aa1a-a0d32da2d292-play i").attr("class", "fa fa-play");
            }
        });
        
