<script>
$("#rye-a00788a3-c7fa-45e8-b8e8-48d1f4fef161 input, #rye-a00788a3-c7fa-45e8-b8e8-48d1f4fef161 select").each(function(i, v) {
	if (!$(this).hasClass('form-control')) {
		if ($(this).is("input[type='file']")) {
			$(this).attr('id', 'rye-a00788a3-c7fa-45e8-b8e8-48d1f4fef161_upload'+i).wrap("<span class='htmlSnippetForm'></span>").after("<label for='rye-a00788a3-c7fa-45e8-b8e8-48d1f4fef161_upload"+i+"' class='customInput'>Choose file</label>");
		} else {
			$(this).wrap("<span class='htmlSnippetForm'></span>").after("<span class='customInput'></span>");
		}
	}
});
</script>

