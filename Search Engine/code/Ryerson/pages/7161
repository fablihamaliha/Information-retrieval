<script>
    $(document).ready(function(){
    	
        $("#8cfadf6e-799a-4b6c-afef-f3dac951e0e8-play, #8cfadf6e-799a-4b6c-afef-f3dac951e0e8-pause").click(function() {
            if ($(this).attr("id") == "8cfadf6e-799a-4b6c-afef-f3dac951e0e8-play") {
                $('#8cfadf6e-799a-4b6c-afef-f3dac951e0e8').carousel('cycle');
                $(this).attr("id", "8cfadf6e-799a-4b6c-afef-f3dac951e0e8-pause");
                $("#8cfadf6e-799a-4b6c-afef-f3dac951e0e8-pause i").attr("class", "fa fa-pause");
            } else {
                $('#8cfadf6e-799a-4b6c-afef-f3dac951e0e8').carousel('pause');
                $(this).attr("id", "8cfadf6e-799a-4b6c-afef-f3dac951e0e8-play");
                $("#8cfadf6e-799a-4b6c-afef-f3dac951e0e8-play i").attr("class", "fa fa-play");
            }
        });
        
