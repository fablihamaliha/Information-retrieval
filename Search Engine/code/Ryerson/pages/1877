    $(document).ready(function(){
    	
        $("#ceda9652-65c7-46f4-8231-8f38af797d15-play, #ceda9652-65c7-46f4-8231-8f38af797d15-pause").click(function() {
            if ($(this).attr("id") == "ceda9652-65c7-46f4-8231-8f38af797d15-play") {
                $('#ceda9652-65c7-46f4-8231-8f38af797d15').carousel('cycle');
                $(this).attr("id", "ceda9652-65c7-46f4-8231-8f38af797d15-pause");
                $("#ceda9652-65c7-46f4-8231-8f38af797d15-pause i").attr("class", "fa fa-pause");
            } else {
                $('#ceda9652-65c7-46f4-8231-8f38af797d15').carousel('pause');
                $(this).attr("id", "ceda9652-65c7-46f4-8231-8f38af797d15-play");
                $("#ceda9652-65c7-46f4-8231-8f38af797d15-play i").attr("class", "fa fa-play");
