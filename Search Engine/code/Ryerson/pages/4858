jQuery(document).ready(function() {
    jQuery("#component624236529 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component624236529 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component624236529 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component624236529 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component624236529 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
