<script>
$("#rye-1787fede-6b55-4ff3-a8dc-e3ed7f8483fb input, #rye-1787fede-6b55-4ff3-a8dc-e3ed7f8483fb select").each(function(i, v) {
	if (!$(this).hasClass('form-control')) {
		if ($(this).is("input[type='file']")) {
			$(this).attr('id', 'rye-1787fede-6b55-4ff3-a8dc-e3ed7f8483fb_upload'+i).wrap("<span class='htmlSnippetForm'></span>").after("<label for='rye-1787fede-6b55-4ff3-a8dc-e3ed7f8483fb_upload"+i+"' class='customInput'>Choose file</label>");
		} else {
			$(this).wrap("<span class='htmlSnippetForm'></span>").after("<span class='customInput'></span>");
		}
	}
});
</script>

