jQuery(document).ready(function() {
    jQuery("#component505533696 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component505533696 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component505533696 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component505533696 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component505533696 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
 </script>
 
