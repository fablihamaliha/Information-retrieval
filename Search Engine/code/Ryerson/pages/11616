    $(document).ready(function(){
    	
        $("#e4c51a3c-55eb-4502-9e1c-b09a8e7e4ffb-play, #e4c51a3c-55eb-4502-9e1c-b09a8e7e4ffb-pause").click(function() {
            if ($(this).attr("id") == "e4c51a3c-55eb-4502-9e1c-b09a8e7e4ffb-play") {
                $('#e4c51a3c-55eb-4502-9e1c-b09a8e7e4ffb').carousel('cycle');
                $(this).attr("id", "e4c51a3c-55eb-4502-9e1c-b09a8e7e4ffb-pause");
