    $(document).ready(function(){
    	
        $("#35ea56d9-1685-4157-975d-fc48088eedcc-play, #35ea56d9-1685-4157-975d-fc48088eedcc-pause").click(function() {
            if ($(this).attr("id") == "35ea56d9-1685-4157-975d-fc48088eedcc-play") {
                $('#35ea56d9-1685-4157-975d-fc48088eedcc').carousel('cycle');
                $(this).attr("id", "35ea56d9-1685-4157-975d-fc48088eedcc-pause");
                $("#35ea56d9-1685-4157-975d-fc48088eedcc-pause i").attr("class", "fa fa-pause");
            } else {
                $('#35ea56d9-1685-4157-975d-fc48088eedcc').carousel('pause');
                $(this).attr("id", "35ea56d9-1685-4157-975d-fc48088eedcc-play");
                $("#35ea56d9-1685-4157-975d-fc48088eedcc-play i").attr("class", "fa fa-play");
            }
        });
        
