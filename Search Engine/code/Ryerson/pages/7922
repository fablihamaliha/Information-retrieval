		<script type="text/javascript">
			function postDimensions() {
				if (window.location != window.parent.location) {
					// post dimensions as JSON
					var dimensionsJson = {
						width : $(".pageWrapper").prop("scrollWidth"),
						height : $(".pageWrapper").prop("scrollHeight")
					};
					var dimensions = JSON.stringify(dimensionsJson);
					console.log("Posting page dimensions " + dimensions + " to parent window.");
					parent.postMessage(dimensions, "*");

					// post height to my.ryerson portal
					if (parent.document.getElementById("rmsManageMyOnlineResources") != null) {
						parent.document.getElementById("rmsManageMyOnlineResources").setAttribute("height", $(".pageWrapper").prop("scrollHeight") + 20);
					}
					if (parent.document.getElementById("rmsManageAccountRecovery") != null) {
						parent.document.getElementById("rmsManageAccountRecovery").setAttribute("height", $(".pageWrapper").prop("scrollHeight") + 20);
					}
				}
			}
			$(document).ready(postDimensions);
			$(window).on('load', function() {
				var url = window.location.href;
				if ( window.location.href.indexOf("#") > -1 ) {
					var hashPosition = url.indexOf("#");
					var urlHash = url.substr( (hashPosition + 1), (url.length) );
					$("a#" + urlHash).click();
				}
			});
		</script>
