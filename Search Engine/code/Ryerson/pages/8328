                <!-- Accordion Heading -->
                <div class="panel-heading container-clipboard">
                    <div class="panel-title">
                        <a id="accordion-1537363859057-convert-your-site-to-responsive" role="tab" data-toggle="collapse" data-parent="#6964a484-9555-4cd5-9fcb-ae683e9d1155" href="#accordion-content-1537363859057-convert-your-site-to-responsive" aria-controls="accordion-content-1537363859057-convert-your-site-to-responsive" aria-expanded="false">
                            Convert your site to responsive
                        </a>
                    </div>
                    
                </div>
                <!-- Accordion Body -->
                <div tabindex="0" id="accordion-content-1537363859057-convert-your-site-to-responsive" class="panel-collapse collapse" role="tabpanel" aria-labelledby="accordion-1537363859057-convert-your-site-to-responsive">
