        <script>
        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#37bcdda6-94a5-47bc-8332-0111a9389358 button").click(function() {
                	jQuery("#37bcdda6-94a5-47bc-8332-0111a9389358.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6570679, "lng": -79.380062}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('37bcdda6-94a5-47bc-8332-0111a9389358',latLng,'600px',15,false);
                        } else{
                            drawDynamicMapByAddress('37bcdda6-94a5-47bc-8332-0111a9389358','285 Victoria Street, Toronto, ON, M5B 2K3, Canada','600px',15,false);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('37bcdda6-94a5-47bc-8332-0111a9389358',latLng,'285 Victoria Street, Toronto, ON, M5B 2K3, Canada','1140px','600px',15,false);
                        } else{
                            drawStaticMapByAddress('37bcdda6-94a5-47bc-8332-0111a9389358','285 Victoria Street, Toronto, ON, M5B 2K3, Canada','1140px','600px',15,false);
                        }
                    }
                });
            });
        </script>
    
    
    
</div>
