        <script>
        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#a5f68faa-e382-4bd9-adc0-6121fd157d40 button").click(function() {
                	jQuery("#a5f68faa-e382-4bd9-adc0-6121fd157d40.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6558668, "lng": -79.3819145}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('a5f68faa-e382-4bd9-adc0-6121fd157d40',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('a5f68faa-e382-4bd9-adc0-6121fd157d40','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('a5f68faa-e382-4bd9-adc0-6121fd157d40',latLng,'1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('a5f68faa-e382-4bd9-adc0-6121fd157d40','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','1140px','600px',18,true);
                        }
                    }
                });
            });
        </script>
    
    
    
