<script>
    $(document).ready(function(){
    	
        $("#b0713b52-2d67-42e7-8e8a-5da957776a0d-play, #b0713b52-2d67-42e7-8e8a-5da957776a0d-pause").click(function() {
            if ($(this).attr("id") == "b0713b52-2d67-42e7-8e8a-5da957776a0d-play") {
                $('#b0713b52-2d67-42e7-8e8a-5da957776a0d').carousel('cycle');
                $(this).attr("id", "b0713b52-2d67-42e7-8e8a-5da957776a0d-pause");
                $("#b0713b52-2d67-42e7-8e8a-5da957776a0d-pause i").attr("class", "fa fa-pause");
            } else {
                $('#b0713b52-2d67-42e7-8e8a-5da957776a0d').carousel('pause');
                $(this).attr("id", "b0713b52-2d67-42e7-8e8a-5da957776a0d-play");
                $("#b0713b52-2d67-42e7-8e8a-5da957776a0d-play i").attr("class", "fa fa-play");
            }
        });
        
        	$('#b0713b52-2d67-42e7-8e8a-5da957776a0d').carousel('pause');
        	
    });
</script>
