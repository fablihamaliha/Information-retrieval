        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#29b2a7f1-44b2-4f81-b18e-32487af14012 button").click(function() {
                	jQuery("#29b2a7f1-44b2-4f81-b18e-32487af14012.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = false;
                    var latLng = JSON.parse('{"lat": 43.6590295, "lng": -79.38106719999999}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('29b2a7f1-44b2-4f81-b18e-32487af14012',latLng,'400px',18,true);
                        } else{
                            drawDynamicMapByAddress('29b2a7f1-44b2-4f81-b18e-32487af14012','380 Victoria St. Toronto, Ontario M5B 2K3','400px',18,true);
                        }
                    } else {
                        if(latLng != null){
