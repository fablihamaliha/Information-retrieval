        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#3a658213-3b9e-4d40-8460-c3e435c5455a button").click(function() {
                	jQuery("#3a658213-3b9e-4d40-8460-c3e435c5455a.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.65696579999999, "lng": -79.3804789}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('3a658213-3b9e-4d40-8460-c3e435c5455a',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('3a658213-3b9e-4d40-8460-c3e435c5455a','10 Dundas Street East, Toronto, ON, M5B 2G9, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('3a658213-3b9e-4d40-8460-c3e435c5455a',latLng,'10 Dundas Street East, Toronto, ON, M5B 2G9, Canada','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('3a658213-3b9e-4d40-8460-c3e435c5455a','10 Dundas Street East, Toronto, ON, M5B 2G9, Canada','1140px','600px',18,true);
                        }
                    }
                });
            });
        </script>
    
    
    
