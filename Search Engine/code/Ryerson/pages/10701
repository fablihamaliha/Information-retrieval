jQuery(document).ready(function() {
    jQuery("#component109354478 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component109354478 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component109354478 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component109354478 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component109354478 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
