        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#b29794f3-056a-4dd4-aeb5-4244e99caef1 button").click(function() {
                	jQuery("#b29794f3-056a-4dd4-aeb5-4244e99caef1.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6570679, "lng": -79.380062}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('b29794f3-056a-4dd4-aeb5-4244e99caef1',latLng,'600px',15,false);
                        } else{
                            drawDynamicMapByAddress('b29794f3-056a-4dd4-aeb5-4244e99caef1','285 Victoria Street, Toronto, ON, M5B 2K3, Canada','600px',15,false);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('b29794f3-056a-4dd4-aeb5-4244e99caef1',latLng,'285 Victoria Street, Toronto, ON, M5B 2K3, Canada','1140px','600px',15,false);
                        } else{
                            drawStaticMapByAddress('b29794f3-056a-4dd4-aeb5-4244e99caef1','285 Victoria Street, Toronto, ON, M5B 2K3, Canada','1140px','600px',15,false);
                        }
                    }
                });
            });
