    <div class="col-xs-12">
        <div class="panel-group accordion background-hidden" id="5964dbb0-1f13-4919-ab5a-658a5a0641ac" role="tablist" aria-multiselectable="true">
			
				<div class="resAccordionControl" role="group" aria-label="Accordion Actions">
