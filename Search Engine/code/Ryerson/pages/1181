                    <h4 class="panel-title">
                        <a id="accordion-1529439893852-dossier" role="tab" data-toggle="collapse" data-parent="#" href="#accordion-content-1529439893852-dossier" aria-controls="accordion-content-1529439893852-dossier" aria-expanded="false">
                            Dossier
                        </a>
                    </h4>
                    
                </div>
                <!-- Accordion Body -->
                <div tabindex="0" id="accordion-content-1529439893852-dossier" class="panel-collapse collapse" role="tabpanel" aria-labelledby="accordion-1529439893852-dossier">
