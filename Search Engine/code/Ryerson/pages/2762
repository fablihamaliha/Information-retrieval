        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#066dd0dd-3464-452f-b158-2eb959e1e9be button").click(function() {
                	jQuery("#066dd0dd-3464-452f-b158-2eb959e1e9be.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6586299, "lng": -79.3770727}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('066dd0dd-3464-452f-b158-2eb959e1e9be',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('066dd0dd-3464-452f-b158-2eb959e1e9be','80 Gould St, Toronto, ON M5B 2M7','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('066dd0dd-3464-452f-b158-2eb959e1e9be',latLng,'80 Gould St, Toronto, ON M5B 2M7','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('066dd0dd-3464-452f-b158-2eb959e1e9be','80 Gould St, Toronto, ON M5B 2M7','1140px','600px',18,true);
                        }
                    }
                });
            });
        </script>
    
    
    
