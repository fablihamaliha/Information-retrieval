<script type="text/javascript">
jQuery(document).ready(function() {
    jQuery("#component1669312696 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component1669312696 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component1669312696 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component1669312696 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component1669312696 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
 </script>
 
