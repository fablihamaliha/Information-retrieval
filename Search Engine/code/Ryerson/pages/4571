        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#0d474354-fc62-40c2-9eff-420a0a87fec9 button").click(function() {
                	jQuery("#0d474354-fc62-40c2-9eff-420a0a87fec9.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6558668, "lng": -79.3819145}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('0d474354-fc62-40c2-9eff-420a0a87fec9',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('0d474354-fc62-40c2-9eff-420a0a87fec9','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('0d474354-fc62-40c2-9eff-420a0a87fec9',latLng,'1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('0d474354-fc62-40c2-9eff-420a0a87fec9','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','1140px','600px',18,true);
                        }
                    }
                });
            });
