			$(this).attr('id', 'rye-02b16f2b-ff97-4406-a96f-677d88595c16_upload'+i).wrap("<span class='htmlSnippetForm'></span>").after("<label for='rye-02b16f2b-ff97-4406-a96f-677d88595c16_upload"+i+"' class='customInput'>Choose file</label>");
		} else {
			$(this).wrap("<span class='htmlSnippetForm'></span>").after("<span class='customInput'></span>");
		}
	}
});
