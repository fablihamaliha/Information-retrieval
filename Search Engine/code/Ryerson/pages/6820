jQuery(document).ready(function() {
    jQuery("#component795528110 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component795528110 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component795528110 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component795528110 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component795528110 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
