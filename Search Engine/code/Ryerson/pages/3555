jQuery(document).ready(function() {
    jQuery("#component1868509000 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component1868509000 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component1868509000 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component1868509000 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component1868509000 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
