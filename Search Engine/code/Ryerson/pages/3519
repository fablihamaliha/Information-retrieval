                $('#a188672a-b96d-46e4-8b50-8f0363332826').carousel('cycle');
                $(this).attr("id", "a188672a-b96d-46e4-8b50-8f0363332826-pause");
                $("#a188672a-b96d-46e4-8b50-8f0363332826-pause i").attr("class", "fa fa-pause");
            } else {
                $('#a188672a-b96d-46e4-8b50-8f0363332826').carousel('pause');
                $(this).attr("id", "a188672a-b96d-46e4-8b50-8f0363332826-play");
                $("#a188672a-b96d-46e4-8b50-8f0363332826-play i").attr("class", "fa fa-play");
            }
        });
        
        		$('#a188672a-b96d-46e4-8b50-8f0363332826').carousel('cycle');
        	
    });
</script>
