$("#rye-e162ef12-eb42-4b87-a424-be077b762204 input, #rye-e162ef12-eb42-4b87-a424-be077b762204 select").each(function(i, v) {
	if (!$(this).hasClass('form-control')) {
		if ($(this).is("input[type='file']")) {
			$(this).attr('id', 'rye-e162ef12-eb42-4b87-a424-be077b762204_upload'+i).wrap("<span class='htmlSnippetForm'></span>").after("<label for='rye-e162ef12-eb42-4b87-a424-be077b762204_upload"+i+"' class='customInput'>Choose file</label>");
		} else {
			$(this).wrap("<span class='htmlSnippetForm'></span>").after("<span class='customInput'></span>");
		}
	}
