<script type="text/javascript">
jQuery(document).ready(function() {
    jQuery("#component760062773 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component760062773 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component760062773 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component760062773 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component760062773 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
 </script>
 
