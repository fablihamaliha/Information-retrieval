jQuery(document).ready(function() {
    jQuery("#component1314281274 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component1314281274 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component1314281274 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component1314281274 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component1314281274 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
