  <script type="text/javascript">
  var jQuery = jQuery.noConflict(); 
  jQuery(function() {
    
             jQuery('img.image0').data('ad-title', "<a target='_blank' >&quot;Ryerson’s focus on academic and practical learning provided a valuable journey in the field of early learning.&quot;</a>");                              
             
             jQuery('img.image1').data('ad-title', "<a target='_blank' >&quot;The experience gained from the BA in ECE program at Ryerson has been invaluable in pursuing my passions and furthering my education.&quot;</a>");                              
             
             jQuery('img.image2').data('ad-title', "<a target='_blank' >&quot;Ryerson University’s School of Early Childhood Studies opened many doors for me.&quot;</a>");                              
             
             jQuery('img.image3').data('ad-title', "<a target='_blank' >&quot;I credit my success as a graduate student to the preparation provided in Ryerson’s ECS program.&quot;</a>");                              
             
             jQuery('img.image4').data('ad-title', "<a target='_blank' >&quot;The skills learned in my degree and the contacts made during field placements&nbsp;are what got me to where I am &nbsp;today.&quot;<br></a>");                              
             
    
    var galleries = jQuery('.ad-gallery').adGallery({
        effect: 'fade',width:false, height: false, slideshow: {animate_first_image: false, speed: 5000}
    });
    galleries[0].settings.effect = 'fade'; 
   
    jQuery('#toggle-slideshow').click(
      function() {
        galleries[0].slideshow.toggle();
        return false;
      }
    );
    jQuery('#toggle-description').click(
      function() {
        if(!galleries[0].settings.description_wrapper) {
          galleries[0].settings.description_wrapper = jQuery('#descriptions');
        } else {
          galleries[0].settings.description_wrapper = false;
        }
        return false;
      }
    );
  });
  </script>

