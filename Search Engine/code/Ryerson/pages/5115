                    var latLng = JSON.parse('{"lat": 43.6558668, "lng": -79.3819145}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('3ce6b23c-8419-4e11-8712-e5b8507bb61f',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('3ce6b23c-8419-4e11-8712-e5b8507bb61f','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('3ce6b23c-8419-4e11-8712-e5b8507bb61f',latLng,'1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','1140px','600px',18,true);
