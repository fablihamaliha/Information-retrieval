<script>

function getPage(){
    var tempPage =  "NEVER_SELECT_PLACEHOLDER";
  if(location.pathname.indexOf("ryerson-works/") >= 0){
    tempPage = location.pathname.substring(location.pathname.indexOf("ryerson-works/") + 14);

    if(tempPage.indexOf("/") >= 0){
    tempPage = tempPage.substring(0,tempPage.indexOf("/"));
    }

    if(tempPage.indexOf(".") >= 0){
    tempPage = tempPage.substring(0,tempPage.indexOf("."));
    }

  }
  return tempPage;
}

jQuery(document).ready(function() {
   jQuery("div.mt_mainContent").addClass("maincontent-adjust");
   jQuery(".sf-menu.sf-navbar a[href*='"+getPage()+"']").addClass("selected");
   jQuery(".pullquoteWrapper").attr( "aria-hidden", "true" );
});

</script>
</div>

                  
          <div class="contentArea">
          
          <script>
          var returnVal = false;
          function containsCurrPage(item){
              
              //returnval is being set to false on each recursi ve call
              jQuery(item).each(function(k,v){
                 if(jQuery(v).attr('class') != undefined && jQuery(v).attr('class').indexOf("currPage") >= 0){
                     jQuery(v).children("ul").children("li").children("ul").each(function(x,xx){
                         jQuery(xx).remove();
                     });
                       returnVal = true;
                 }
                 
                 if(jQuery(v).children() != null && jQuery(v).children().length > 0){
                     jQuery(v).children().each(function(kk,vv){containsCurrPage(vv);});
                 }
             });
             return returnVal;
          }
          
          
          </script>
