        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#52b5f9af-63ad-4656-8917-ca79a9091fe3 button").click(function() {
                	jQuery("#52b5f9af-63ad-4656-8917-ca79a9091fe3.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6558668, "lng": -79.3819145}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('52b5f9af-63ad-4656-8917-ca79a9091fe3',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('52b5f9af-63ad-4656-8917-ca79a9091fe3','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('52b5f9af-63ad-4656-8917-ca79a9091fe3',latLng,'1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('52b5f9af-63ad-4656-8917-ca79a9091fe3','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','1140px','600px',18,true);
                        }
                    }
                });
            });
        </script>
    
    
    
