   <script>
   var scrollWidth = null;
   jQuery(document).ready(function(){
	   scrollWidth = jQuery("#main_content_panel").width();
	   if(scrollWidth == null){
		   scrollWidth = jQuery(".mt_mainContent").width();
	   }
	   jQuery(".galleryGridComponent .item img").width((scrollWidth/3)-5+"px");
	   jQuery(".galleryGridComponent .item .caption").width(((scrollWidth/3)-21)+"px");
	   jQuery(".galleryGridComponent .caption p").css("fontSize", (((scrollWidth/3)-5)/scrollWidth)+0.6+"em");
	   var container = jQuery('.grid');
	// initialize
	container.masonry({
	  columnWidth: (scrollWidth/3)-5,
	  itemSelector: '.item',
	  "gutter": 5
	});
	var slider = jQuery( ".gridSlider" ).slider({
	    min: (scrollWidth/4)-5,
	    max: (scrollWidth/2)-5,
	    value:(scrollWidth/3)-5,
	    range:"min",
	    slide: function( event, ui ) {
	        jQuery(".galleryGridComponent .item img").width(ui.value+"px");
	        jQuery(".galleryGridComponent .item .caption").width((ui.value-16)+"px");
	        jQuery(".galleryGridComponent .caption p").css("fontSize", (ui.value/scrollWidth)+0.5+"em");
	        container.masonry(
       		{
       		      columnWidth: ui.value,
       		      itemSelector: '.item',
       		      "gutter": 5,
       		   transitionDuration: 0
       		});
	    },
	    create: function(event,ui){
	    	jQuery(".ui-slider-handle").attr("alt","Drag left and right to decrease and increase image size.");
	    }
	});
	//incase images are not downloaded in time, this forces to relayout the gallery once images loaded. 
	//It is rarely used but important to avoid images cascading on each other in Chrome
	jQuery(".galleryGridComponent .item img").load(function(){
	container.masonry({
	      columnWidth: (scrollWidth/3)-5,
	      itemSelector: '.item',
	      "gutter": 5
	    });
	});
   });
   </script>
   












</div>
