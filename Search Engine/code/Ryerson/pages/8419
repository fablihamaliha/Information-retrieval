<script>
$(document).ready(function(){addCoursePopover('#ff6ff6e1-bab1-4171-b52f-a083926df73b');});
if (flowWidthCollection == undefined) {
	var flowWidthCollection = new Object();	
}
$(document).on('_intervalshort', {
    layoutID: "ff6ff6e1-bab1-4171-b52f-a083926df73b"
}, function(event) {
	var cachedSize = $('#'+event.data.layoutID).height() + $('#'+event.data.layoutID).width();
	if (flowWidthCollection[event.data.layoutID] == undefined || flowWidthCollection[event.data.layoutID] != cachedSize){
		if ($('#'+event.data.layoutID).width() <= 263) {
			if (!$('#'+event.data.layoutID+' .overlayText').hasClass('smallScreenFullOverlay')) $('#'+event.data.layoutID+' .overlayText').addClass('smallScreenFullOverlay');
		} else {
			if ($('#'+event.data.layoutID+' .overlayText').hasClass('smallScreenFullOverlay')) $('#'+event.data.layoutID+' .overlayText.smallScreenFullOverlay').removeClass('smallScreenFullOverlay');
		}
		if ($('#'+event.data.layoutID).width() <= 360) {
			if (!$('#'+event.data.layoutID+' > .row > div').hasClass('smallScreenFullWidth')) {
				if ($('#'+event.data.layoutID+' > .row > div:first > div.caption').length) {
					$('#'+event.data.layoutID+' > .row > div:first').addClass('overlayTextSwap').insertAfter($('#'+event.data.layoutID+' > .row > div:eq(1)'));
				}
				$('#'+event.data.layoutID+' > .row > div').addClass('smallScreenFullWidth');
			}
		} else {
			if ($('#'+event.data.layoutID+' > .row > div').hasClass('smallScreenFullWidth')) {
				if ($('#'+event.data.layoutID+' > .row > div.overlayTextSwap').length) {
					$('#'+event.data.layoutID+' > .row > div.overlayTextSwap').removeClass('overlayTextSwap').insertBefore($('#'+event.data.layoutID+' > .row > div:first'));
				}
				$('#'+event.data.layoutID+' > .row > div.smallScreenFullWidth').removeClass('smallScreenFullWidth');
			}
		}
		flowWidthCollection[event.data.layoutID] = cachedSize;
	}
});
</script></div>

				

				
				<div class="resTitle section">


























<h2>
	
    Ryerson University EarlyON Gerrard Resource Centre (GRC)
    
	
</h2>


</div>

				

				
				<div class="resImageText section">


























		<div class="thumbnail customStyle1" id="4df343bc-f85b-4cd7-a5bc-1ea49d8f7fd9">
			<div class="row">
				<div class="col-xs-12 col-sm-4">
					
						
<img src="/content/dam/gerrard-resource-centre/images/output-onlinepngtools.jpg" alt=""/>



   
					
				</div>
				<div class="col-xs-12 col-sm-8">
				
						<div class="caption richtextContent">
	<p>Our EarlyON Child and Family Centres are drop-in based programs where parents, caregivers, and children can meet, play and, learn. Our programs are inclusive spaces for families to exchange help, advice and get reassurance from one another and staff in all areas of parenting and child care.&nbsp;</p> 
<p><strong>Registration:&nbsp;</strong></p> 
<p>No formal registration is required.&nbsp; Parents and caregivers may attend daily or on an as-needed basis.&nbsp; Parents and caregivers are requested to provide a mailing address and phone number on their first visit; however, this is voluntary.</p> 
<p><strong>Fees:&nbsp;</strong>No fee</p> 
<p><strong>Funding:</strong>&nbsp;This program is funded by&nbsp;<a href="http://www.toronto.ca/children/famresource.htm">Toronto Children's Services, external link<span aria-hidden="true" class="autoIconLink fa fa-external-link"></span><span class="sr-only">, external link</span></a>, City of Toronto.&nbsp;</p> 
<p><strong>Support:</strong>&nbsp;<a href="https://www.ryerson.ca/">Ryerson University</a>&nbsp;and&nbsp;<a href="https://www.ryerson.ca/ecs/">School of Early Childhood Studies</a></p>
</div>  
						    
					
				</div>
			</div>
		</div> 
		
<script>
$(document).ready(function(){addCoursePopover('#4df343bc-f85b-4cd7-a5bc-1ea49d8f7fd9');});
