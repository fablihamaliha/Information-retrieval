                $('#8829f37a-a4d7-4676-81fc-d7ed9ee966f2').carousel('cycle');
                $(this).attr("id", "8829f37a-a4d7-4676-81fc-d7ed9ee966f2-pause");
                $("#8829f37a-a4d7-4676-81fc-d7ed9ee966f2-pause i").attr("class", "fa fa-pause");
            } else {
                $('#8829f37a-a4d7-4676-81fc-d7ed9ee966f2').carousel('pause');
                $(this).attr("id", "8829f37a-a4d7-4676-81fc-d7ed9ee966f2-play");
                $("#8829f37a-a4d7-4676-81fc-d7ed9ee966f2-play i").attr("class", "fa fa-play");
            }
        });
        
