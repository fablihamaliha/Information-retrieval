	if (!$(this).hasClass('form-control')) {
		if ($(this).is("input[type='file']")) {
			$(this).attr('id', 'rye-848d92f2-16ca-4dc4-8bae-2938d820b7f5_upload'+i).wrap("<span class='htmlSnippetForm'></span>").after("<label for='rye-848d92f2-16ca-4dc4-8bae-2938d820b7f5_upload"+i+"' class='customInput'>Choose file</label>");
		} else {
			$(this).wrap("<span class='htmlSnippetForm'></span>").after("<span class='customInput'></span>");
		}
	}
});
</script>

