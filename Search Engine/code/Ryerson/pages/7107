                    <h2 class="panel-title">
                        <a id="accordion-1524512780716-income-tax-receipts" role="tab" data-toggle="collapse" data-parent="#" href="#accordion-content-1524512780716-income-tax-receipts" aria-controls="accordion-content-1524512780716-income-tax-receipts" aria-expanded="false">
                            Income tax receipts
                        </a>
                    </h2>
                    
                </div>
                <!-- Accordion Body -->
                <div tabindex="0" id="accordion-content-1524512780716-income-tax-receipts" class="panel-collapse collapse" role="tabpanel" aria-labelledby="accordion-1524512780716-income-tax-receipts">
                    <div class="panel-body ">
                        <div class="1524512780716 stackparsys">










				
