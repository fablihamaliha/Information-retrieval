                jQuery("#26c0cf78-30ce-4e51-95d5-a55830f7ba21 button").click(function() {
                	jQuery("#26c0cf78-30ce-4e51-95d5-a55830f7ba21.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6558668, "lng": -79.3819145}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('26c0cf78-30ce-4e51-95d5-a55830f7ba21',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('26c0cf78-30ce-4e51-95d5-a55830f7ba21','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('26c0cf78-30ce-4e51-95d5-a55830f7ba21',latLng,'1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('26c0cf78-30ce-4e51-95d5-a55830f7ba21','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','1140px','600px',18,true);
                        }
                    }
                });
            });
        </script>
    
    
    
