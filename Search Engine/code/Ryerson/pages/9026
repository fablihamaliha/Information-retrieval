			$('#70b503e8-02d4-4f30-8d39-022040eeccfb table').removeAttr("cellspacing");
			
			$('#70b503e8-02d4-4f30-8d39-022040eeccfb table').DataTable({
				
				"sDom":"lrtp",
				"sort":false,
				"pagingType": "simple_numbers","paging":false,
				"autoWidth":true
			});
		
