	//convert to plugin in pure js....later (soon)
	$(document).ready(function() {
		$('#70b503e8-02d4-4f30-8d39-022040eeccfb table').addClass("table");
	    
		$('#70b503e8-02d4-4f30-8d39-022040eeccfb table').addClass("table-bordered");
		
			var removeHeading = false;
			
			$('#70b503e8-02d4-4f30-8d39-022040eeccfb table tbody tr').eq(0).find("td").replaceWith(function(){
				return $("<th>", {attr: {'width': $(this).attr('width'), 'rowspan': $(this).attr('rowspan'), 'colspan': $(this).attr('colspan'), 'valign': $(this).attr('valign'), 'headers': $(this).attr('headers'), 'style': $(this).attr('style')}, html: $(this).html()}).css("vertical-align", $(this).attr('valign'));;
			});
			$('#70b503e8-02d4-4f30-8d39-022040eeccfb table:not(.dataTable) tbody tr').eq(0).wrap( "<thead></thead>" );
		
			$('#70b503e8-02d4-4f30-8d39-022040eeccfb table thead').prependTo('#70b503e8-02d4-4f30-8d39-022040eeccfb table');
			
			
