<script>
$("#rye-5a1a2572-8375-4620-b608-7370dadd42fd input, #rye-5a1a2572-8375-4620-b608-7370dadd42fd select").each(function(i, v) {
	if (!$(this).hasClass('form-control')) {
		if ($(this).is("input[type='file']")) {
			$(this).attr('id', 'rye-5a1a2572-8375-4620-b608-7370dadd42fd_upload'+i).wrap("<span class='htmlSnippetForm'></span>").after("<label for='rye-5a1a2572-8375-4620-b608-7370dadd42fd_upload"+i+"' class='customInput'>Choose file</label>");
		} else {
			$(this).wrap("<span class='htmlSnippetForm'></span>").after("<span class='customInput'></span>");
		}
	}
});
</script>

