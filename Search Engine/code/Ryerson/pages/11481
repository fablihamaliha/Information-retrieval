        <script>
        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#3cbe86ca-81c9-4abd-b70a-fd2edc380c77 button").click(function() {
                	jQuery("#3cbe86ca-81c9-4abd-b70a-fd2edc380c77.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6570679, "lng": -79.380062}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('3cbe86ca-81c9-4abd-b70a-fd2edc380c77',latLng,'600px',15,false);
                        } else{
                            drawDynamicMapByAddress('3cbe86ca-81c9-4abd-b70a-fd2edc380c77','285 Victoria Street, Toronto, ON, M5B 2K3, Canada','600px',15,false);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('3cbe86ca-81c9-4abd-b70a-fd2edc380c77',latLng,'285 Victoria Street, Toronto, ON, M5B 2K3, Canada','1140px','600px',15,false);
                        } else{
                            drawStaticMapByAddress('3cbe86ca-81c9-4abd-b70a-fd2edc380c77','285 Victoria Street, Toronto, ON, M5B 2K3, Canada','1140px','600px',15,false);
                        }
                    }
                });
            });
        </script>
    
    
    
