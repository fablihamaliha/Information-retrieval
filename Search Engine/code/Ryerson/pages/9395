        <div class="courseListCount" ><span class="courseInfoLabels">Count: </span><span class="courseInfoValues">1.00</span></div>
    
    
	
	
		<div class="courseListPrerequisites"><span class="courseInfoLabels">Prerequisites: </span><span class="courseInfoValues"><a href="/content/ryerson/calendar/2020-2021/courses/environmental-health/ENH/121.html" class="qTipCourse" data-url="/content/ryerson/calendar/2020-2021/courses/environmental-health/ENH/121/jcr:content/center/uiwcalendarcourse.json">ENH 121</a></span></div>
	
	
	
	


<div style="height:0px;clear:both;"></div>
