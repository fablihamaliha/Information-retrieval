jQuery(document).ready(function() {
    jQuery("#component498873570 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component498873570 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component498873570 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component498873570 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component498873570 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
 </script>
 
