    $(document).ready(function(){
    	
        $("#7fff50c3-e452-4c1c-b694-06adcfe7c91b-play, #7fff50c3-e452-4c1c-b694-06adcfe7c91b-pause").click(function() {
            if ($(this).attr("id") == "7fff50c3-e452-4c1c-b694-06adcfe7c91b-play") {
                $('#7fff50c3-e452-4c1c-b694-06adcfe7c91b').carousel('cycle');
                $(this).attr("id", "7fff50c3-e452-4c1c-b694-06adcfe7c91b-pause");
                $("#7fff50c3-e452-4c1c-b694-06adcfe7c91b-pause i").attr("class", "fa fa-pause");
            } else {
                $('#7fff50c3-e452-4c1c-b694-06adcfe7c91b').carousel('pause');
                $(this).attr("id", "7fff50c3-e452-4c1c-b694-06adcfe7c91b-play");
                $("#7fff50c3-e452-4c1c-b694-06adcfe7c91b-play i").attr("class", "fa fa-play");
