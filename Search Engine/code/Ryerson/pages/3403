        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#1b5fdb2e-5095-4883-8ef3-d28dee806d23 button").click(function() {
                	jQuery("#1b5fdb2e-5095-4883-8ef3-d28dee806d23.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.657564, "lng": -79.3772027}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('1b5fdb2e-5095-4883-8ef3-d28dee806d23',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('1b5fdb2e-5095-4883-8ef3-d28dee806d23','245 Church Street, Toronto, ON, M5B 1Z2, Canada','600px',18,true);
                        }
