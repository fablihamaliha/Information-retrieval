        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#91f80c2b-2d3a-4116-b397-bfcd9e3f9675 button").click(function() {
                	jQuery("#91f80c2b-2d3a-4116-b397-bfcd9e3f9675.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6600414, "lng": -79.3771506}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('91f80c2b-2d3a-4116-b397-bfcd9e3f9675',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('91f80c2b-2d3a-4116-b397-bfcd9e3f9675','99 Gerrard St E, Toronto, ON, M5B 2M2, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
