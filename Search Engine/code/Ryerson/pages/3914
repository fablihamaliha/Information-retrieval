<script>
    $(document).ready(function(){
    	
        $("#3f928f03-c76f-47eb-8107-89b5645d6e2a-play, #3f928f03-c76f-47eb-8107-89b5645d6e2a-pause").click(function() {
            if ($(this).attr("id") == "3f928f03-c76f-47eb-8107-89b5645d6e2a-play") {
                $('#3f928f03-c76f-47eb-8107-89b5645d6e2a').carousel('cycle');
                $(this).attr("id", "3f928f03-c76f-47eb-8107-89b5645d6e2a-pause");
                $("#3f928f03-c76f-47eb-8107-89b5645d6e2a-pause i").attr("class", "fa fa-pause");
            } else {
                $('#3f928f03-c76f-47eb-8107-89b5645d6e2a').carousel('pause');
                $(this).attr("id", "3f928f03-c76f-47eb-8107-89b5645d6e2a-play");
                $("#3f928f03-c76f-47eb-8107-89b5645d6e2a-play i").attr("class", "fa fa-play");
            }
        });
        
