                        if(latLng != null){
                            drawDynamicMapByLatLng('c525a8ae-a88d-4228-ac10-f9afb3452e53',latLng,'600px',20,true);
                        } else{
                            drawDynamicMapByAddress('c525a8ae-a88d-4228-ac10-f9afb3452e53','350 Victoria Street, Toronto, ON, M5B 2K3, Canada','600px',20,true);
