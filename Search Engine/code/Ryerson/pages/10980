        <script>
        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#59dd6ea9-e273-4ffb-8bfd-d56c5fc4b880 button").click(function() {
                	jQuery("#59dd6ea9-e273-4ffb-8bfd-d56c5fc4b880.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6570679, "lng": -79.380062}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('59dd6ea9-e273-4ffb-8bfd-d56c5fc4b880',latLng,'600px',15,false);
                        } else{
                            drawDynamicMapByAddress('59dd6ea9-e273-4ffb-8bfd-d56c5fc4b880','285 Victoria Street, Toronto, ON, M5B 2K3, Canada','600px',15,false);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('59dd6ea9-e273-4ffb-8bfd-d56c5fc4b880',latLng,'285 Victoria Street, Toronto, ON, M5B 2K3, Canada','1140px','600px',15,false);
                        } else{
                            drawStaticMapByAddress('59dd6ea9-e273-4ffb-8bfd-d56c5fc4b880','285 Victoria Street, Toronto, ON, M5B 2K3, Canada','1140px','600px',15,false);
                        }
                    }
                });
            });
        </script>
    
    
    
