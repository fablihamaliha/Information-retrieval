        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#d9a5d3ea-3214-414a-95ca-2b2ed6900c4c button").click(function() {
                	jQuery("#d9a5d3ea-3214-414a-95ca-2b2ed6900c4c.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6585292, "lng": -79.3800808}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('d9a5d3ea-3214-414a-95ca-2b2ed6900c4c',latLng,'600px',20,false);
                        } else{
                            drawDynamicMapByAddress('d9a5d3ea-3214-414a-95ca-2b2ed6900c4c','Kerr Hall, 350 Victoria St, Toronto, ON M5B 0A1','600px',20,false);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('d9a5d3ea-3214-414a-95ca-2b2ed6900c4c',latLng,'Kerr Hall, 350 Victoria St, Toronto, ON M5B 0A1','1140px','600px',20,false);
                        } else{
                            drawStaticMapByAddress('d9a5d3ea-3214-414a-95ca-2b2ed6900c4c','Kerr Hall, 350 Victoria St, Toronto, ON M5B 0A1','1140px','600px',20,false);
                        }
                    }
                });
            });
        </script>
    
    
    
</div>
