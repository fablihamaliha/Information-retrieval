<script>
    $(document).ready(function(){
    	
        $("#d5d4d7f8-dae8-45fd-b0d5-00920b8fdd04-play, #d5d4d7f8-dae8-45fd-b0d5-00920b8fdd04-pause").click(function() {
            if ($(this).attr("id") == "d5d4d7f8-dae8-45fd-b0d5-00920b8fdd04-play") {
                $('#d5d4d7f8-dae8-45fd-b0d5-00920b8fdd04').carousel('cycle');
                $(this).attr("id", "d5d4d7f8-dae8-45fd-b0d5-00920b8fdd04-pause");
                $("#d5d4d7f8-dae8-45fd-b0d5-00920b8fdd04-pause i").attr("class", "fa fa-pause");
            } else {
                $('#d5d4d7f8-dae8-45fd-b0d5-00920b8fdd04').carousel('pause');
                $(this).attr("id", "d5d4d7f8-dae8-45fd-b0d5-00920b8fdd04-play");
                $("#d5d4d7f8-dae8-45fd-b0d5-00920b8fdd04-play i").attr("class", "fa fa-play");
            }
        });
        
