<script>
    $(document).ready(function(){
    	
        $("#2e0d10e0-dd4b-454e-a116-7dbac35fbb02-play, #2e0d10e0-dd4b-454e-a116-7dbac35fbb02-pause").click(function() {
            if ($(this).attr("id") == "2e0d10e0-dd4b-454e-a116-7dbac35fbb02-play") {
                $('#2e0d10e0-dd4b-454e-a116-7dbac35fbb02').carousel('cycle');
                $(this).attr("id", "2e0d10e0-dd4b-454e-a116-7dbac35fbb02-pause");
                $("#2e0d10e0-dd4b-454e-a116-7dbac35fbb02-pause i").attr("class", "fa fa-pause");
            } else {
                $('#2e0d10e0-dd4b-454e-a116-7dbac35fbb02').carousel('pause');
                $(this).attr("id", "2e0d10e0-dd4b-454e-a116-7dbac35fbb02-play");
                $("#2e0d10e0-dd4b-454e-a116-7dbac35fbb02-play i").attr("class", "fa fa-play");
            }
        });
        
