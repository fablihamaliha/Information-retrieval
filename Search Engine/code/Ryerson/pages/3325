        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#39a9654b-4c6f-4e75-956e-e7e0e43746b0 button").click(function() {
                	jQuery("#39a9654b-4c6f-4e75-956e-e7e0e43746b0.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6558668, "lng": -79.3819145}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('39a9654b-4c6f-4e75-956e-e7e0e43746b0',latLng,'600px',18,true);
