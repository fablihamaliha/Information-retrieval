                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('39a9654b-4c6f-4e75-956e-e7e0e43746b0',latLng,'1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('39a9654b-4c6f-4e75-956e-e7e0e43746b0','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','1140px','600px',18,true);
                        }
                    }
                });
            });
        </script>
    
    
    
</div>
