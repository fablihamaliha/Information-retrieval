  jQuery(function() {
    
             jQuery('img.image0').data('ad-title', "<a  href='/content/dam/midwifery/documents/MEPEDIBriefJune2020SolidarityforBlackLives-mh2.pdf'>The MEP Lifts One Voice In Solidarity with Black Lives</a>");                              
             
