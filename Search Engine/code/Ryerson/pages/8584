        $("#21cc0f83-6126-4d51-ba18-50fd0cf84e45-play, #21cc0f83-6126-4d51-ba18-50fd0cf84e45-pause").click(function() {
            if ($(this).attr("id") == "21cc0f83-6126-4d51-ba18-50fd0cf84e45-play") {
                $('#21cc0f83-6126-4d51-ba18-50fd0cf84e45').carousel('cycle');
                $(this).attr("id", "21cc0f83-6126-4d51-ba18-50fd0cf84e45-pause");
                $("#21cc0f83-6126-4d51-ba18-50fd0cf84e45-pause i").attr("class", "fa fa-pause");
            } else {
                $('#21cc0f83-6126-4d51-ba18-50fd0cf84e45').carousel('pause');
                $(this).attr("id", "21cc0f83-6126-4d51-ba18-50fd0cf84e45-play");
                $("#21cc0f83-6126-4d51-ba18-50fd0cf84e45-play i").attr("class", "fa fa-play");
