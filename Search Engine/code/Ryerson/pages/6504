<script>
    $(document).ready(function(){
    	
        $("#3544a2c0-d786-4eba-b349-1d341dfbf31e-play, #3544a2c0-d786-4eba-b349-1d341dfbf31e-pause").click(function() {
            if ($(this).attr("id") == "3544a2c0-d786-4eba-b349-1d341dfbf31e-play") {
                $('#3544a2c0-d786-4eba-b349-1d341dfbf31e').carousel('cycle');
                $(this).attr("id", "3544a2c0-d786-4eba-b349-1d341dfbf31e-pause");
                $("#3544a2c0-d786-4eba-b349-1d341dfbf31e-pause i").attr("class", "fa fa-pause");
            } else {
                $('#3544a2c0-d786-4eba-b349-1d341dfbf31e').carousel('pause');
                $(this).attr("id", "3544a2c0-d786-4eba-b349-1d341dfbf31e-play");
                $("#3544a2c0-d786-4eba-b349-1d341dfbf31e-play i").attr("class", "fa fa-play");
            }
        });
        
