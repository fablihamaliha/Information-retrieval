                jQuery("#d5115b67-7fa1-4c4a-8704-b7b782de0890 button").click(function() {
                	jQuery("#d5115b67-7fa1-4c4a-8704-b7b782de0890.res-map-placeholder").removeClass('res-map-placeholder');
