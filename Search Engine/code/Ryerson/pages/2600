                        if(latLng != null){
                            drawDynamicMapByLatLng('d5115b67-7fa1-4c4a-8704-b7b782de0890',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('d5115b67-7fa1-4c4a-8704-b7b782de0890','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','600px',18,true);
                        }
