            <div class="panel panel-default">
                <!-- Accordion Heading -->
                <div class="panel-heading container-clipboard">
                    <h3 class="panel-title">
                        <a id="accordion-1584164244407-enable-lockdown-browser-for-the-exam" role="tab" data-toggle="collapse" data-parent="#" href="#accordion-content-1584164244407-enable-lockdown-browser-for-the-exam" aria-controls="accordion-content-1584164244407-enable-lockdown-browser-for-the-exam" aria-expanded="false">
                            Enable LockDown Browser for the exam
                        </a>
                    </h3>
                    
                </div>
                <!-- Accordion Body -->
                <div tabindex="0" id="accordion-content-1584164244407-enable-lockdown-browser-for-the-exam" class="panel-collapse collapse" role="tabpanel" aria-labelledby="accordion-1584164244407-enable-lockdown-browser-for-the-exam">
                    <div class="panel-body ">
                        <div class="1584164244407 stackparsys">










				
				<div class="resContentList parbase section">
























 



<div id="200e0929-15dc-4d3d-b88a-18a113eaee4f">
    
        <ol style="list-style-type:decimal">
            
            










				
				<li class="resContentListItem parbase section">










    














 

<div id="51d7018f-a071-4504-8adc-465e63be8546" >
