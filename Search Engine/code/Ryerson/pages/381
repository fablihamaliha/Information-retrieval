<script>
function showHeatClickMap(){
( function($) {
var heatmapGETTarget = "/content/heatmap.html?url="+window.location.pathname;
var xx = h337.create({"element":document.getElementById("heatmap"), "radius":25, "visible":true});               
    $.ajax({
      url: heatmapGETTarget ,
      type: "GET",
      async:false,
      success: function(msg){
      var el = msg;
       var obj = eval('('+el+')');
        xx.store.setDataSet(obj);
      }
  }   
)
}) ( jQuery );
}
</script>

<script>
function showHeatMouseMap(){
( function($) {
var heatmapGETTarget = "/content/heatmousemap.html?url="+window.location.pathname;
