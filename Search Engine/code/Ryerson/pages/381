<script>  

function enforceAltTags(){
var $altjq = jQuery.noConflict(); 
$altjq(document).ready(function() {
    var altArray = new Array();
    $altjq("#siteframe img").each(function(){
        if(this.alt.length == 0){
            altArray.push(this.src);
        }
    });
    var stringArray = JSON.stringify(altArray);
    //console.log(stringArray);
    
   if(stringArray.length > 0){
    $altjq.ajax({
        url: ("/content/altTagEnforcer.html"),
        type: "POST",
        async:true,
        data:stringArray,
        success: function(msg){
        var tagMsg = eval("("+$altjq.trim(msg)+")");
        //console.log(tagMsg);
        $altjq("#siteframe img").each(function(){
            //console.log(this.src);
            if(tagMsg[this.src] != undefined){
                this.alt = tagMsg[this.src];
            }
            
        });
        }
    });
    }
    
});
}

setTimeout("enforceAltTags()",5000);
</script> 

