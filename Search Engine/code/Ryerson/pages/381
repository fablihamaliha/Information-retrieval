    $.ajax({
      url: heatmapGETTarget ,
      type: "GET",
      async:false,
      success: function(msg){
      var el = msg;
       var obj = eval('('+el+')');
        xx.store.setDataSet(obj);
      }
  }   
)
}) ( jQuery );
}
</script>

     
