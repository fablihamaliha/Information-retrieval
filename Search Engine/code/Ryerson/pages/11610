<script>
    $(document).ready(function(){
    	
        $("#0465c562-d2f6-4909-b6ce-4006742a79cc-play, #0465c562-d2f6-4909-b6ce-4006742a79cc-pause").click(function() {
            if ($(this).attr("id") == "0465c562-d2f6-4909-b6ce-4006742a79cc-play") {
                $('#0465c562-d2f6-4909-b6ce-4006742a79cc').carousel('cycle');
                $(this).attr("id", "0465c562-d2f6-4909-b6ce-4006742a79cc-pause");
                $("#0465c562-d2f6-4909-b6ce-4006742a79cc-pause i").attr("class", "fa fa-pause");
            } else {
                $('#0465c562-d2f6-4909-b6ce-4006742a79cc').carousel('pause');
                $(this).attr("id", "0465c562-d2f6-4909-b6ce-4006742a79cc-play");
                $("#0465c562-d2f6-4909-b6ce-4006742a79cc-play i").attr("class", "fa fa-play");
            }
        });
        
