                jQuery("#24f18f7a-1027-45db-b28f-0b8d79ae2886 button").click(function() {
                	jQuery("#24f18f7a-1027-45db-b28f-0b8d79ae2886.res-map-placeholder").removeClass('res-map-placeholder');
