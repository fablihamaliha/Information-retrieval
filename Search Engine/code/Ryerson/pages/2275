                        if(latLng != null){
                            drawDynamicMapByLatLng('24f18f7a-1027-45db-b28f-0b8d79ae2886',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('24f18f7a-1027-45db-b28f-0b8d79ae2886','245 Church Street, Toronto, ON, M5B 1Z2, Canada','600px',18,true);
                        }
