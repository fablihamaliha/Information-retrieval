    <div class="col-xs-12">
        <div class="panel-group accordion background-opaque" id="7d30cb97-5d73-43f3-b9d0-35a0f081c055" role="tablist" aria-multiselectable="true">
			
				<div class="resAccordionControl" role="group" aria-label="Accordion Actions">
