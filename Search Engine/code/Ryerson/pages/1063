                    var latLng = JSON.parse('{"lat": 43.6570679, "lng": -79.380062}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('e936f45b-c0c6-4ebe-ad7a-0b3f6eeb3959',latLng,'600px',15,false);
                        } else{
                            drawDynamicMapByAddress('e936f45b-c0c6-4ebe-ad7a-0b3f6eeb3959','285 Victoria Street, Toronto, ON, M5B 2K3, Canada','600px',15,false);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('e936f45b-c0c6-4ebe-ad7a-0b3f6eeb3959',latLng,'285 Victoria Street, Toronto, ON, M5B 2K3, Canada','1140px','600px',15,false);
