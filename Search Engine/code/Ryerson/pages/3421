                <!-- Accordion Heading -->
                <div class="panel-heading container-clipboard">
                    <h3 class="panel-title">
                        <a id="accordion-1528141800130-sabbatical-and-leave-periods" role="tab" data-toggle="collapse" data-parent="#" href="#accordion-content-1528141800130-sabbatical-and-leave-periods" aria-controls="accordion-content-1528141800130-sabbatical-and-leave-periods" aria-expanded="false">
                            Sabbatical and leave periods
                        </a>
                    </h3>
                    
                </div>
                <!-- Accordion Body -->
                <div tabindex="0" id="accordion-content-1528141800130-sabbatical-and-leave-periods" class="panel-collapse collapse" role="tabpanel" aria-labelledby="accordion-1528141800130-sabbatical-and-leave-periods">
                    <div class="panel-body ">
                        <div class="1528141800130 stackparsys">










				
