        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#4beae389-524d-4e6b-be69-c7719370790f button").click(function() {
                	jQuery("#4beae389-524d-4e6b-be69-c7719370790f.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6570679, "lng": -79.380062}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('4beae389-524d-4e6b-be69-c7719370790f',latLng,'600px',15,false);
                        } else{
                            drawDynamicMapByAddress('4beae389-524d-4e6b-be69-c7719370790f','285 Victoria Street, Toronto, ON, M5B 2K3, Canada','600px',15,false);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('4beae389-524d-4e6b-be69-c7719370790f',latLng,'285 Victoria Street, Toronto, ON, M5B 2K3, Canada','1140px','600px',15,false);
                        } else{
                            drawStaticMapByAddress('4beae389-524d-4e6b-be69-c7719370790f','285 Victoria Street, Toronto, ON, M5B 2K3, Canada','1140px','600px',15,false);
                        }
                    }
                });
            });
        </script>
    
    
    
