		<script type="text/javascript">
			function showLoadingScreen() {
				HoldOn.open({
					theme: 'sk-bounce',
					message: '<h3>The requested action will take a few seconds to process.</h3><p>Please be patient while we activate your online identity.</p>'
				});
			}
			function postDimensions() {
				if (window.location != window.parent.location) {
					var dimensionsJson = {
						width : $(".pageWrapper").prop("scrollWidth"),
						height : $(".pageWrapper").prop("scrollHeight")
					};
					var dimensions = JSON.stringify(dimensionsJson);
					console.log("Posting page dimensions " + dimensions + " to parent window.");
					parent.postMessage(dimensions, "*");
				}
			}
			$(document).on("submit", ".activationForm", function() {
				setTimeout(showLoadingScreen, 1000);
			});
			$(document).ready(function() {
				postDimensions();
				var loginType = 'activation';
				var currentState = 'identityView';
				if (loginType === 'activation' && currentState !== 'identityView' && currentState !== 'unrecoverableErrorView') {
 					var states = [];
					
						states.push("Acceptable Use of Information Technology");
					
						states.push("Username and Password Selection");
					
						states.push("Account Recovery");
					
						states.push("Two-Factor Authentication");
					
						states.push("Confirmation");
					
					//TODO: This is a temporary logic until Rmail is completely deprecated. Must be removed in the future.
					if (currentState === 'alumniMailHostView') {
						states.splice(3, 0, "???progressbar.view.alumniMailHostView???")
					}
					ProgressBar.init(
					  states,
					  '???progressbar.view.identityView???',
					  'progress-bar-wrapper',
					  {
						  completeText: 'Completed:&nbsp;',
						  notCompleteText: 'Not complete:&nbsp;',
						  inProgressText: 'In Progress:&nbsp;'
					  }
					);
				}
			});
		</script>
		<script>
			function activationTypeFn(){
				var select = $('input:radio[name=activationType]:checked').val();
				if (select == "STUDENT") {
					$("#studentApplicantID").show();
					$("#employeeID").hide();
					$("#studentApplicantID").prop("disabled", false);
					$("#employeeID").prop("disabled", true);
					
					$("#employeeNumber").val("");
					
				} else if (select =="EMPLOYEE") {
					$("#studentApplicantID").hide();
					$("#employeeID").show();
					$("#studentApplicantID").prop("disabled", true);
					$("#employeeID").prop("disabled", false);
					
					$("#studentNumber").val("");
					
				} else {
					$("#studentApplicantID").hide();
					$("#employeeID").hide();
					$("#studentApplicantID").prop("disabled", true);
					$("#employeeID").prop("disabled", true);
					
					$("#employeeNumber").val("");
					$("#studentNumber").val("");
				}
			}
			function showRecordNotFoundFn() {
				$("#recordNotFoundTooltip").hide();
				$("#recordNotFoundInfo").show();
			}
			function showHelpDetails() {
				$("#helpinfo").hide();
				$("#helpdetails").show();
			}
			function hideHelpDetails() {
				$("#helpdetails").hide();
				$("#helpinfo").show();
			}
			$(document).on("change", "input:radio[name='activationType']", activationTypeFn);
			$(document).on("click", "a#recordNotFoundTooltip", showRecordNotFoundFn);
			$(document).on("click", "a#showOnlineIdentityActivationFAQ", showHelpDetails);
			$(document).on("click", "a#hideOnlineIdentityActivationFAQ", hideHelpDetails);
			$(document).ready(function() {
				activationTypeFn();
			});
		</script>
