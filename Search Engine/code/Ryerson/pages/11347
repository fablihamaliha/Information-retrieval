<script>
$("#rye-f1226e33-eb76-44f7-b47f-5553ecdc5683 input, #rye-f1226e33-eb76-44f7-b47f-5553ecdc5683 select").each(function(i, v) {
	if (!$(this).hasClass('form-control')) {
		if ($(this).is("input[type='file']")) {
			$(this).attr('id', 'rye-f1226e33-eb76-44f7-b47f-5553ecdc5683_upload'+i).wrap("<span class='htmlSnippetForm'></span>").after("<label for='rye-f1226e33-eb76-44f7-b47f-5553ecdc5683_upload"+i+"' class='customInput'>Choose file</label>");
		} else {
			$(this).wrap("<span class='htmlSnippetForm'></span>").after("<span class='customInput'></span>");
		}
	}
});
</script>

