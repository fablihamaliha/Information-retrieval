                        if(latLng != null){
                            drawDynamicMapByLatLng('1031fd95-675d-4f12-a1ce-300bdcc46fc6',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('1031fd95-675d-4f12-a1ce-300bdcc46fc6','87 Gerard Street, Toronto, ON, M5B 2K3, Canada','600px',18,true);
                        }
