                $(this).attr("id", "053a7b00-fcd4-43d6-9a2a-2ab7db1e614e-pause");
                $("#053a7b00-fcd4-43d6-9a2a-2ab7db1e614e-pause i").attr("class", "fa fa-pause");
            } else {
                $('#053a7b00-fcd4-43d6-9a2a-2ab7db1e614e').carousel('pause');
                $(this).attr("id", "053a7b00-fcd4-43d6-9a2a-2ab7db1e614e-play");
                $("#053a7b00-fcd4-43d6-9a2a-2ab7db1e614e-play i").attr("class", "fa fa-play");
            }
        });
        
        		$('#053a7b00-fcd4-43d6-9a2a-2ab7db1e614e').carousel('cycle');
        	
    });
</script>
</div>

				

				
