<script>
    $(document).ready(function(){
    	
        $("#47fed074-ff3b-4490-8e87-f7eb3fec12b7-play, #47fed074-ff3b-4490-8e87-f7eb3fec12b7-pause").click(function() {
            if ($(this).attr("id") == "47fed074-ff3b-4490-8e87-f7eb3fec12b7-play") {
                $('#47fed074-ff3b-4490-8e87-f7eb3fec12b7').carousel('cycle');
                $(this).attr("id", "47fed074-ff3b-4490-8e87-f7eb3fec12b7-pause");
                $("#47fed074-ff3b-4490-8e87-f7eb3fec12b7-pause i").attr("class", "fa fa-pause");
            } else {
                $('#47fed074-ff3b-4490-8e87-f7eb3fec12b7').carousel('pause');
                $(this).attr("id", "47fed074-ff3b-4490-8e87-f7eb3fec12b7-play");
                $("#47fed074-ff3b-4490-8e87-f7eb3fec12b7-play i").attr("class", "fa fa-play");
            }
        });
        
        		$('#47fed074-ff3b-4490-8e87-f7eb3fec12b7').carousel('cycle');
        	
    });
</script>
