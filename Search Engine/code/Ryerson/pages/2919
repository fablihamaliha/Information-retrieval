                            drawDynamicMapByLatLng('b6e4a555-6a50-466a-bfc3-cd1011bc8c13',latLng,'600px',18,false);
                        } else{
                            drawDynamicMapByAddress('b6e4a555-6a50-466a-bfc3-cd1011bc8c13','379  Victoria Street, Toronto, ON, M5B 2K3, Canada','600px',18,false);
                        }
