    $(document).ready(function(){
    	
        $("#7d01fd4e-ca45-4d47-b01a-9326055e33bc-play, #7d01fd4e-ca45-4d47-b01a-9326055e33bc-pause").click(function() {
            if ($(this).attr("id") == "7d01fd4e-ca45-4d47-b01a-9326055e33bc-play") {
                $('#7d01fd4e-ca45-4d47-b01a-9326055e33bc').carousel('cycle');
                $(this).attr("id", "7d01fd4e-ca45-4d47-b01a-9326055e33bc-pause");
                $("#7d01fd4e-ca45-4d47-b01a-9326055e33bc-pause i").attr("class", "fa fa-pause");
            } else {
                $('#7d01fd4e-ca45-4d47-b01a-9326055e33bc').carousel('pause');
                $(this).attr("id", "7d01fd4e-ca45-4d47-b01a-9326055e33bc-play");
                $("#7d01fd4e-ca45-4d47-b01a-9326055e33bc-play i").attr("class", "fa fa-play");
            }
