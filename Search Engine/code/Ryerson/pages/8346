        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#81e279c4-b60a-4b2f-9b23-24fe59eb4451 button").click(function() {
                	jQuery("#81e279c4-b60a-4b2f-9b23-24fe59eb4451.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.659324, "lng": -79.378299}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('81e279c4-b60a-4b2f-9b23-24fe59eb4451',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('81e279c4-b60a-4b2f-9b23-24fe59eb4451','325 church Street, Toronto, ON, M5B 1Z2, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('81e279c4-b60a-4b2f-9b23-24fe59eb4451',latLng,'325 church Street, Toronto, ON, M5B 1Z2, Canada','1140px','600px',18,true);
