<script>
    $(document).ready(function(){
    	
        $("#a4efbb39-9b9c-4529-abdf-ea7e415da866-play, #a4efbb39-9b9c-4529-abdf-ea7e415da866-pause").click(function() {
            if ($(this).attr("id") == "a4efbb39-9b9c-4529-abdf-ea7e415da866-play") {
                $('#a4efbb39-9b9c-4529-abdf-ea7e415da866').carousel('cycle');
                $(this).attr("id", "a4efbb39-9b9c-4529-abdf-ea7e415da866-pause");
                $("#a4efbb39-9b9c-4529-abdf-ea7e415da866-pause i").attr("class", "fa fa-pause");
            } else {
                $('#a4efbb39-9b9c-4529-abdf-ea7e415da866').carousel('pause');
                $(this).attr("id", "a4efbb39-9b9c-4529-abdf-ea7e415da866-play");
                $("#a4efbb39-9b9c-4529-abdf-ea7e415da866-play i").attr("class", "fa fa-play");
            }
        });
        
        		$('#a4efbb39-9b9c-4529-abdf-ea7e415da866').carousel('cycle');
        	
    });
