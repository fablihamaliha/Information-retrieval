                        if(latLng != null){
                            drawDynamicMapByLatLng('1133001a-2811-4f48-bc53-a2785e16de3d',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('1133001a-2811-4f48-bc53-a2785e16de3d','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','600px',18,true);
