<script>
    $(document).ready(function(){
    	
        $("#d7f5adcf-9be6-49f8-b848-3430221036cf-play, #d7f5adcf-9be6-49f8-b848-3430221036cf-pause").click(function() {
            if ($(this).attr("id") == "d7f5adcf-9be6-49f8-b848-3430221036cf-play") {
                $('#d7f5adcf-9be6-49f8-b848-3430221036cf').carousel('cycle');
                $(this).attr("id", "d7f5adcf-9be6-49f8-b848-3430221036cf-pause");
                $("#d7f5adcf-9be6-49f8-b848-3430221036cf-pause i").attr("class", "fa fa-pause");
            } else {
                $('#d7f5adcf-9be6-49f8-b848-3430221036cf').carousel('pause');
                $(this).attr("id", "d7f5adcf-9be6-49f8-b848-3430221036cf-play");
                $("#d7f5adcf-9be6-49f8-b848-3430221036cf-play i").attr("class", "fa fa-play");
            }
        });
        
        		$('#d7f5adcf-9be6-49f8-b848-3430221036cf').carousel('cycle');
        	
    });
</script>
