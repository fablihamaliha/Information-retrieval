        <div class="courseListCount" ><span class="courseInfoLabels">Count: </span><span class="courseInfoValues">1.00</span></div>
    
    
	
	<div class="courseListAttribute" ><span class="courseInfoLabels">Consent: </span><span class="courseInfoValues">Departmental consent required</span></div>
    
	
	
	
	


