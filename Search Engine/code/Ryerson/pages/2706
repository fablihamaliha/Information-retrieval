        <script>
        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#ec075e12-9737-4d13-9d3c-9cc358a8b89f button").click(function() {
                	jQuery("#ec075e12-9737-4d13-9d3c-9cc358a8b89f.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = false;
                    var latLng = JSON.parse('{"lat": 43.65722239999999, "lng": -79.3777944}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('ec075e12-9737-4d13-9d3c-9cc358a8b89f',latLng,'300px',20,true);
                        } else{
                            drawDynamicMapByAddress('ec075e12-9737-4d13-9d3c-9cc358a8b89f','288 Church St, Toronto, ON M5B 1Z5','300px',20,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('ec075e12-9737-4d13-9d3c-9cc358a8b89f',latLng,'288 Church St, Toronto, ON M5B 1Z5','1140px','300px',20,true);
                        } else{
                            drawStaticMapByAddress('ec075e12-9737-4d13-9d3c-9cc358a8b89f','288 Church St, Toronto, ON M5B 1Z5','1140px','300px',20,true);
                        }
                    }
                });
            });
        </script>
    
    
    
