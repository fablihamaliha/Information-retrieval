			var removeHeading = false;
			
			$('#10e7b529-65e9-4b4a-929a-06b4e9561dc9 table tbody tr').eq(0).find("td").replaceWith(function(){
				return $("<th>", {attr: {'width': $(this).attr('width'), 'rowspan': $(this).attr('rowspan'), 'colspan': $(this).attr('colspan'), 'valign': $(this).attr('valign'), 'headers': $(this).attr('headers'), 'style': $(this).attr('style')}, html: $(this).html()}).css("vertical-align", $(this).attr('valign'));;
			});
			$('#10e7b529-65e9-4b4a-929a-06b4e9561dc9 table:not(.dataTable) tbody tr').eq(0).wrap( "<thead></thead>" );
		
			$('#10e7b529-65e9-4b4a-929a-06b4e9561dc9 table thead').prependTo('#10e7b529-65e9-4b4a-929a-06b4e9561dc9 table');
			
			
