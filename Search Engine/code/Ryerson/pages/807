			$('#10e7b529-65e9-4b4a-929a-06b4e9561dc9 table').removeAttr("cellspacing");
			
			$('#10e7b529-65e9-4b4a-929a-06b4e9561dc9 table').DataTable({
				
				"sDom":"lrtp",
				"sort":false,
				"pagingType": "simple_numbers","paging":false,
				"autoWidth":true
			});
		
			//#NOTE-REMOVE
			if(removeHeading){
				$('#10e7b529-65e9-4b4a-929a-06b4e9561dc9 table thead').remove();
			}
			
			
		    
	    
	    
	} );
	</script>
