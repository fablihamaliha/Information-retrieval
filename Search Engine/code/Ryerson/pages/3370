jQuery(document).ready(function() {
    jQuery("#component533554924 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component533554924 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component533554924 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component533554924 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component533554924 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
 </script>
 
