jQuery(document).ready(function() {
    jQuery("#component497620538 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component497620538 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component497620538 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component497620538 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component497620538 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
