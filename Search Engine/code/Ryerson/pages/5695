<h2>
	
    Meet The Hosts
    
        <i aria-hidden='true' class='icon fa fa-video-camera'></i>
    
	
</h2>


</div>

				

				
				<div class="resAccordion section">
























 


<div class="row">
    <div class="col-xs-12">
        <div class="panel-group accordion background-hidden" id="a427a457-348c-4a25-952c-2419f052cd9d" role="tablist" aria-multiselectable="true">
			
            
            <div class="panel panel-default">
                <!-- Accordion Heading -->
                <div class="panel-heading container-clipboard">
                    <div class="panel-title">
                        <a id="accordion-1566223600872-chelsea-jones" role="tab" data-toggle="collapse" data-parent="#" href="#accordion-content-1566223600872-chelsea-jones" aria-controls="accordion-content-1566223600872-chelsea-jones" aria-expanded="false">
                            Chelsea Jones
                        </a>
                    </div>
                    
                </div>
