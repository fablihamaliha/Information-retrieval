<script>
    $(document).ready(function(){
    	
        $("#6eb8d645-dda4-4dca-b942-e81e6aa3a80c-play, #6eb8d645-dda4-4dca-b942-e81e6aa3a80c-pause").click(function() {
            if ($(this).attr("id") == "6eb8d645-dda4-4dca-b942-e81e6aa3a80c-play") {
                $('#6eb8d645-dda4-4dca-b942-e81e6aa3a80c').carousel('cycle');
                $(this).attr("id", "6eb8d645-dda4-4dca-b942-e81e6aa3a80c-pause");
                $("#6eb8d645-dda4-4dca-b942-e81e6aa3a80c-pause i").attr("class", "fa fa-pause");
            } else {
                $('#6eb8d645-dda4-4dca-b942-e81e6aa3a80c').carousel('pause');
                $(this).attr("id", "6eb8d645-dda4-4dca-b942-e81e6aa3a80c-play");
                $("#6eb8d645-dda4-4dca-b942-e81e6aa3a80c-play i").attr("class", "fa fa-play");
            }
        });
        
