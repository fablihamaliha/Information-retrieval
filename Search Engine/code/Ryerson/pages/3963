    	jQuery("#component100076922 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component100076922 .closeAll").click(function(event){
    	event.preventDefault();
