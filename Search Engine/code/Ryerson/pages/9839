jQuery(document).ready(function() {
    jQuery("#component412820429 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component412820429 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component412820429 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component412820429 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component412820429 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
 </script>
 
