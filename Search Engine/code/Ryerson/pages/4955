jQuery(document).ready(function() {
    jQuery("#component813346702 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component813346702 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component813346702 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component813346702 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component813346702 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
 </script>
 
