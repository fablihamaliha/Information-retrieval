                <!-- Accordion Heading -->
                <div class="panel-heading container-clipboard">
                    <h3 class="panel-title">
                        <a id="accordion-1600441516106-fall-winter" role="tab" data-toggle="collapse" data-parent="#25485790-37d5-4f43-88a5-64325c52e932" href="#accordion-content-1600441516106-fall-winter" aria-controls="accordion-content-1600441516106-fall-winter" aria-expanded="false">
                            Fall/Winter
                        </a>
                    </h3>
                    
                </div>
                <!-- Accordion Body -->
                <div tabindex="0" id="accordion-content-1600441516106-fall-winter" class="panel-collapse collapse" role="tabpanel" aria-labelledby="accordion-1600441516106-fall-winter">
