        <script>
        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#328ce2ec-d101-483b-a92f-ec099b4e35a8 button").click(function() {
                	jQuery("#328ce2ec-d101-483b-a92f-ec099b4e35a8.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.657564, "lng": -79.3772027}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('328ce2ec-d101-483b-a92f-ec099b4e35a8',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('328ce2ec-d101-483b-a92f-ec099b4e35a8','245 Church Street, Toronto, ON, M5B 1Z2, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('328ce2ec-d101-483b-a92f-ec099b4e35a8',latLng,'245 Church Street, Toronto, ON, M5B 1Z2, Canada','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('328ce2ec-d101-483b-a92f-ec099b4e35a8','245 Church Street, Toronto, ON, M5B 1Z2, Canada','1140px','600px',18,true);
                        }
                    }
                });
            });
        </script>
    
    
    
