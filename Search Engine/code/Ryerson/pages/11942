        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#d557408a-45ee-494f-987e-8b76d740639a button").click(function() {
                	jQuery("#d557408a-45ee-494f-987e-8b76d740639a.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6597321, "lng": -79.37971449999999}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('d557408a-45ee-494f-987e-8b76d740639a',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('d557408a-45ee-494f-987e-8b76d740639a','44 Gerrard Street East','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('d557408a-45ee-494f-987e-8b76d740639a',latLng,'44 Gerrard Street East','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('d557408a-45ee-494f-987e-8b76d740639a','44 Gerrard Street East','1140px','600px',18,true);
                        }
                    }
                });
            });
        </script>
    
    
    
