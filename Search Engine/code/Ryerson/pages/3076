<script type="text/javascript">
jQuery(document).ready(function() {
    jQuery("#component249029328 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component249029328 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component249029328 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component249029328 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component249029328 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
 </script>
 
