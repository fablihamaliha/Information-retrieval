<script>
$("#rye-f74b3c2c-3c24-4a50-a93d-5111e70c27e8 input, #rye-f74b3c2c-3c24-4a50-a93d-5111e70c27e8 select").each(function(i, v) {
	if (!$(this).hasClass('form-control')) {
		if ($(this).is("input[type='file']")) {
			$(this).attr('id', 'rye-f74b3c2c-3c24-4a50-a93d-5111e70c27e8_upload'+i).wrap("<span class='htmlSnippetForm'></span>").after("<label for='rye-f74b3c2c-3c24-4a50-a93d-5111e70c27e8_upload"+i+"' class='customInput'>Choose file</label>");
		} else {
			$(this).wrap("<span class='htmlSnippetForm'></span>").after("<span class='customInput'></span>");
		}
	}
});
</script>

