                $('#54a4413c-2d37-4b23-bbd4-11965c7ee906').carousel('pause');
                $(this).attr("id", "54a4413c-2d37-4b23-bbd4-11965c7ee906-play");
                $("#54a4413c-2d37-4b23-bbd4-11965c7ee906-play i").attr("class", "fa fa-play");
