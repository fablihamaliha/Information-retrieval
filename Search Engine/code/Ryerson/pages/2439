            if ($(this).attr("id") == "54a4413c-2d37-4b23-bbd4-11965c7ee906-play") {
                $('#54a4413c-2d37-4b23-bbd4-11965c7ee906').carousel('cycle');
                $(this).attr("id", "54a4413c-2d37-4b23-bbd4-11965c7ee906-pause");
                $("#54a4413c-2d37-4b23-bbd4-11965c7ee906-pause i").attr("class", "fa fa-pause");
