                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6586299, "lng": -79.3770727}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('f3ff694f-b2de-4171-b38f-06cd5ebc2f49',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('f3ff694f-b2de-4171-b38f-06cd5ebc2f49','80 Gould St, Toronto, ON M5B 2M7','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('f3ff694f-b2de-4171-b38f-06cd5ebc2f49',latLng,'80 Gould St, Toronto, ON M5B 2M7','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('f3ff694f-b2de-4171-b38f-06cd5ebc2f49','80 Gould St, Toronto, ON M5B 2M7','1140px','600px',18,true);
                        }
                    }
                });
            });
        </script>
    
    
    
