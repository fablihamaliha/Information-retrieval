            if ($(this).attr("id") == "b60bbe02-6c1b-4537-a47d-52435edd79af-play") {
                $('#b60bbe02-6c1b-4537-a47d-52435edd79af').carousel('cycle');
                $(this).attr("id", "b60bbe02-6c1b-4537-a47d-52435edd79af-pause");
                $("#b60bbe02-6c1b-4537-a47d-52435edd79af-pause i").attr("class", "fa fa-pause");
            } else {
                $('#b60bbe02-6c1b-4537-a47d-52435edd79af').carousel('pause');
                $(this).attr("id", "b60bbe02-6c1b-4537-a47d-52435edd79af-play");
                $("#b60bbe02-6c1b-4537-a47d-52435edd79af-play i").attr("class", "fa fa-play");
            }
        });
        
        		$('#b60bbe02-6c1b-4537-a47d-52435edd79af').carousel('cycle');
        	
    });
</script>
</div>

				

				
