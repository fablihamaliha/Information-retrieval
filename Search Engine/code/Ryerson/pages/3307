<script type="text/javascript">
jQuery(document).ready(function() {
    jQuery("#component953594587 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component953594587 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component953594587 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component953594587 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component953594587 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
 </script>
 
