                jQuery("#41eb395a-2248-4713-9674-bcf0de1aa52f button").click(function() {
                	jQuery("#41eb395a-2248-4713-9674-bcf0de1aa52f.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.657564, "lng": -79.3772027}');
