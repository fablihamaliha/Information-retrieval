        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#e5105b71-b9e1-4e9e-83d3-4dcf00f26239 button").click(function() {
                	jQuery("#e5105b71-b9e1-4e9e-83d3-4dcf00f26239.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.659324, "lng": -79.378299}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('e5105b71-b9e1-4e9e-83d3-4dcf00f26239',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('e5105b71-b9e1-4e9e-83d3-4dcf00f26239','325 church Street, Toronto, ON, M5B 1Z2, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('e5105b71-b9e1-4e9e-83d3-4dcf00f26239',latLng,'325 church Street, Toronto, ON, M5B 1Z2, Canada','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('e5105b71-b9e1-4e9e-83d3-4dcf00f26239','325 church Street, Toronto, ON, M5B 1Z2, Canada','1140px','600px',18,true);
                        }
                    }
                });
            });
