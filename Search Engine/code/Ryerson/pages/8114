        <script>
        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#7102d503-456c-4631-9838-cd77f2cea7e2 button").click(function() {
                	jQuery("#7102d503-456c-4631-9838-cd77f2cea7e2.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.657564, "lng": -79.3772027}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('7102d503-456c-4631-9838-cd77f2cea7e2',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('7102d503-456c-4631-9838-cd77f2cea7e2','245 Church Street, Toronto, ON, M5B 1Z2, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('7102d503-456c-4631-9838-cd77f2cea7e2',latLng,'245 Church Street, Toronto, ON, M5B 1Z2, Canada','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('7102d503-456c-4631-9838-cd77f2cea7e2','245 Church Street, Toronto, ON, M5B 1Z2, Canada','1140px','600px',18,true);
                        }
                    }
                });
            });
        </script>
    
    
    
