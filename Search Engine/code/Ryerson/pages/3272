    jQuery("#component736742362 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component736742362 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component736742362 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component736742362 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component736742362 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
