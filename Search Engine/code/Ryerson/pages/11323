    $(document).ready(function(){
    	
        $("#7ccbd49e-28fc-469b-a00f-b358f8d5b425-play, #7ccbd49e-28fc-469b-a00f-b358f8d5b425-pause").click(function() {
            if ($(this).attr("id") == "7ccbd49e-28fc-469b-a00f-b358f8d5b425-play") {
                $('#7ccbd49e-28fc-469b-a00f-b358f8d5b425').carousel('cycle');
                $(this).attr("id", "7ccbd49e-28fc-469b-a00f-b358f8d5b425-pause");
                $("#7ccbd49e-28fc-469b-a00f-b358f8d5b425-pause i").attr("class", "fa fa-pause");
            } else {
                $('#7ccbd49e-28fc-469b-a00f-b358f8d5b425').carousel('pause');
                $(this).attr("id", "7ccbd49e-28fc-469b-a00f-b358f8d5b425-play");
                $("#7ccbd49e-28fc-469b-a00f-b358f8d5b425-play i").attr("class", "fa fa-play");
            }
        });
        
