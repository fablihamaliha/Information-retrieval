jQuery(document).ready(function() {
    jQuery("#component228581888 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component228581888 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component228581888 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component228581888 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component228581888 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
