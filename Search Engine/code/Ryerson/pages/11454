</div></div>

				

				
				<div class="resTabs section">
























 



<div class="row">
    <div class="col-xs-12">
        <!-- Tabs Component -->
        <div class="tabs">
            <!-- Nav tabs -->
            <ul class="nav nav-tabs nav-tabs-responsive" role="tablist">
                
                <li role="presentation" class="active">
                    <a id="tab-1524834211277-digital-community" href="#tab-content-1524834211277-digital-community" role="tab" data-toggle="tab" aria-controls="tab-content-1524834211277-digital-community" aria-expanded="true">
                        <span class="text">Digital Community</span>
                    </a>
                </li>
                
                <li role="presentation" class="next">
                    <a id="tab-1524834232392-marketing---promotions-" href="#tab-content-1524834232392-marketing---promotions-" role="tab" data-toggle="tab" aria-controls="tab-content-1524834232392-marketing---promotions-" aria-expanded="false">
                        <span class="text">Marketing &amp; Promotions </span>
                    </a>
                </li>
                
                <li role="presentation" class="">
                    <a id="tab-1524834253002-research" href="#tab-content-1524834253002-research" role="tab" data-toggle="tab" aria-controls="tab-content-1524834253002-research" aria-expanded="false">
                        <span class="text">Research</span>
                    </a>
