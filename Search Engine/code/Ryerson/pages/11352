        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#b47ec18b-c120-411b-8840-0545210afdf3 button").click(function() {
                	jQuery("#b47ec18b-c120-411b-8840-0545210afdf3.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6570679, "lng": -79.380062}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('b47ec18b-c120-411b-8840-0545210afdf3',latLng,'600px',15,false);
                        } else{
                            drawDynamicMapByAddress('b47ec18b-c120-411b-8840-0545210afdf3','285 Victoria Street, Toronto, ON, M5B 2K3, Canada','600px',15,false);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('b47ec18b-c120-411b-8840-0545210afdf3',latLng,'285 Victoria Street, Toronto, ON, M5B 2K3, Canada','1140px','600px',15,false);
                        } else{
                            drawStaticMapByAddress('b47ec18b-c120-411b-8840-0545210afdf3','285 Victoria Street, Toronto, ON, M5B 2K3, Canada','1140px','600px',15,false);
                        }
                    }
                });
            });
        </script>
    
    
    
