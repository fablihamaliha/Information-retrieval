        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#fec48816-5545-424d-9544-24b305f994e7 button").click(function() {
                	jQuery("#fec48816-5545-424d-9544-24b305f994e7.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6586299, "lng": -79.3770727}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('fec48816-5545-424d-9544-24b305f994e7',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('fec48816-5545-424d-9544-24b305f994e7','80 Gould St, Toronto, ON M5B 2M7','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('fec48816-5545-424d-9544-24b305f994e7',latLng,'80 Gould St, Toronto, ON M5B 2M7','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('fec48816-5545-424d-9544-24b305f994e7','80 Gould St, Toronto, ON M5B 2M7','1140px','600px',18,true);
                        }
                    }
                });
            });
        </script>
    
    
    
