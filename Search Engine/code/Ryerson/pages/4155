<div class="resaccordion_2025867181 resAccordion">
























 


<div class="row">
    <div class="col-xs-12">
        <div class="panel-group accordion background-opaque" id="fa609487-5928-4719-9c11-17f1c23de775" role="tablist" aria-multiselectable="true">
			
            
            <div class="panel panel-default">
                <!-- Accordion Heading -->
                <div class="panel-heading container-clipboard">
                    <h3 class="panel-title">
                        <a id="accordion-1589204417377-prevent-students-from-downloading-shared-files" role="tab" data-toggle="collapse" data-parent="#fa609487-5928-4719-9c11-17f1c23de775" href="#accordion-content-1589204417377-prevent-students-from-downloading-shared-files" aria-controls="accordion-content-1589204417377-prevent-students-from-downloading-shared-files" aria-expanded="false">
                            Prevent students from downloading shared files
                        </a>
                    </h3>
                    
                </div>
