        <script>
        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#4890be0f-3d86-42f7-9329-a9e888b47b03 button").click(function() {
                	jQuery("#4890be0f-3d86-42f7-9329-a9e888b47b03.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6558668, "lng": -79.3819145}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('4890be0f-3d86-42f7-9329-a9e888b47b03',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('4890be0f-3d86-42f7-9329-a9e888b47b03','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('4890be0f-3d86-42f7-9329-a9e888b47b03',latLng,'1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('4890be0f-3d86-42f7-9329-a9e888b47b03','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','1140px','600px',18,true);
                        }
                    }
                });
            });
        </script>
    
    
    
