jQuery(document).ready(function() {
    jQuery("#component712132898 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component712132898 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component712132898 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component712132898 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component712132898 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
 </script>
 
