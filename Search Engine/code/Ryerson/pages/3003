        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#80ac99bd-cec2-4f08-876c-169faa96a6a7 button").click(function() {
                	jQuery("#80ac99bd-cec2-4f08-876c-169faa96a6a7.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6558668, "lng": -79.3819145}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('80ac99bd-cec2-4f08-876c-169faa96a6a7',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('80ac99bd-cec2-4f08-876c-169faa96a6a7','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('80ac99bd-cec2-4f08-876c-169faa96a6a7',latLng,'1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('80ac99bd-cec2-4f08-876c-169faa96a6a7','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','1140px','600px',18,true);
                        }
                    }
                });
            });
        </script>
    
    
    
