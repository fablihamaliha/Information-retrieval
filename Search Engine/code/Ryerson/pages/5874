    jQuery("#component2128371119 .image img").one("load", function() {
        var imgWidth = jQuery("#component2128371119 .image img").outerWidth(true);
        if(parseInt(imgWidth) < 0){
        	imgWidth = parseInt(imgWidth) + 0;
