	$(document).ready(function() {
		$('#be6b4090-175b-4cde-84e0-edf54f8852cb table').addClass("table");
	    
		$('#be6b4090-175b-4cde-84e0-edf54f8852cb table').addClass("table-bordered");
		
			var removeHeading = false;
			
			$('#be6b4090-175b-4cde-84e0-edf54f8852cb table tbody tr').eq(0).find("td").replaceWith(function(){
				return $("<th>", {attr: {'width': $(this).attr('width'), 'rowspan': $(this).attr('rowspan'), 'colspan': $(this).attr('colspan'), 'valign': $(this).attr('valign'), 'headers': $(this).attr('headers'), 'style': $(this).attr('style')}, html: $(this).html()}).css("vertical-align", $(this).attr('valign'));;
			});
			$('#be6b4090-175b-4cde-84e0-edf54f8852cb table:not(.dataTable) tbody tr').eq(0).wrap( "<thead></thead>" );
		
			$('#be6b4090-175b-4cde-84e0-edf54f8852cb table thead').prependTo('#be6b4090-175b-4cde-84e0-edf54f8852cb table');
			
			
			$('#be6b4090-175b-4cde-84e0-edf54f8852cb table').removeAttr("border");
			$('#be6b4090-175b-4cde-84e0-edf54f8852cb table').removeAttr("cellpadding");
			$('#be6b4090-175b-4cde-84e0-edf54f8852cb table').removeAttr("cellspacing");
			
			$('#be6b4090-175b-4cde-84e0-edf54f8852cb table').DataTable({
				
				"sDom":"lrtp",
				"sort":false,
				"pagingType": "simple_numbers","paging":false,
				"autoWidth":true
			});
		
			//#NOTE-REMOVE
			if(removeHeading){
				$('#be6b4090-175b-4cde-84e0-edf54f8852cb table thead').remove();
			}
			
			
		    
	    
	    
	} );
