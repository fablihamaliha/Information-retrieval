jQuery(document).ready(function() {
    jQuery("#component472942695 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component472942695 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component472942695 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component472942695 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component472942695 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
