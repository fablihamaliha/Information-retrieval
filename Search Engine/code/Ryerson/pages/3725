<script>
$("#rye-c2d614d2-7c79-4107-b0e2-62c855e25369 input, #rye-c2d614d2-7c79-4107-b0e2-62c855e25369 select").each(function(i, v) {
	if (!$(this).hasClass('form-control')) {
		if ($(this).is("input[type='file']")) {
			$(this).attr('id', 'rye-c2d614d2-7c79-4107-b0e2-62c855e25369_upload'+i).wrap("<span class='htmlSnippetForm'></span>").after("<label for='rye-c2d614d2-7c79-4107-b0e2-62c855e25369_upload"+i+"' class='customInput'>Choose file</label>");
		} else {
			$(this).wrap("<span class='htmlSnippetForm'></span>").after("<span class='customInput'></span>");
		}
	}
});
</script>

	<style>
		#rye-c2d614d2-7c79-4107-b0e2-62c855e25369 iframe{ height:;}
		@media screen and (min-width: 768px) { #rye-c2d614d2-7c79-4107-b0e2-62c855e25369 iframe{ height:;} }
		@media screen and (min-width: 992px) { #rye-c2d614d2-7c79-4107-b0e2-62c855e25369 iframe{ height:;} }
