	if (!$(this).hasClass('form-control')) {
		if ($(this).is("input[type='file']")) {
			$(this).attr('id', 'rye-cd901b10-5baf-41e9-ad3e-be8e44c709b1_upload'+i).wrap("<span class='htmlSnippetForm'></span>").after("<label for='rye-cd901b10-5baf-41e9-ad3e-be8e44c709b1_upload"+i+"' class='customInput'>Choose file</label>");
		} else {
