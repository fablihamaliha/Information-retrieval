        <div class="sr-only">Map of 80 Gould St, Toronto, ON M5B 2M7</div>
        <script>
        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#3286a39d-fc2f-4fe8-a969-5b06cd85b351 button").click(function() {
                	jQuery("#3286a39d-fc2f-4fe8-a969-5b06cd85b351.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6586299, "lng": -79.3770727}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('3286a39d-fc2f-4fe8-a969-5b06cd85b351',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('3286a39d-fc2f-4fe8-a969-5b06cd85b351','80 Gould St, Toronto, ON M5B 2M7','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('3286a39d-fc2f-4fe8-a969-5b06cd85b351',latLng,'80 Gould St, Toronto, ON M5B 2M7','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('3286a39d-fc2f-4fe8-a969-5b06cd85b351','80 Gould St, Toronto, ON M5B 2M7','1140px','600px',18,true);
                        }
                    }
                });
            });
        </script>
    
    
    
