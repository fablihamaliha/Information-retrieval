    $(document).ready(function(){
    	
        $("#5e68e32c-3490-4cff-8861-a0aeb7631d9e-play, #5e68e32c-3490-4cff-8861-a0aeb7631d9e-pause").click(function() {
            if ($(this).attr("id") == "5e68e32c-3490-4cff-8861-a0aeb7631d9e-play") {
                $('#5e68e32c-3490-4cff-8861-a0aeb7631d9e').carousel('cycle');
                $(this).attr("id", "5e68e32c-3490-4cff-8861-a0aeb7631d9e-pause");
                $("#5e68e32c-3490-4cff-8861-a0aeb7631d9e-pause i").attr("class", "fa fa-pause");
            } else {
                $('#5e68e32c-3490-4cff-8861-a0aeb7631d9e').carousel('pause');
                $(this).attr("id", "5e68e32c-3490-4cff-8861-a0aeb7631d9e-play");
                $("#5e68e32c-3490-4cff-8861-a0aeb7631d9e-play i").attr("class", "fa fa-play");
