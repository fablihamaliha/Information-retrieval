<script>
    $(document).ready(function(){
    	
        $("#8caac2ab-9d43-4b1b-b35c-092f2b1b5aab-play, #8caac2ab-9d43-4b1b-b35c-092f2b1b5aab-pause").click(function() {
            if ($(this).attr("id") == "8caac2ab-9d43-4b1b-b35c-092f2b1b5aab-play") {
                $('#8caac2ab-9d43-4b1b-b35c-092f2b1b5aab').carousel('cycle');
                $(this).attr("id", "8caac2ab-9d43-4b1b-b35c-092f2b1b5aab-pause");
                $("#8caac2ab-9d43-4b1b-b35c-092f2b1b5aab-pause i").attr("class", "fa fa-pause");
            } else {
                $('#8caac2ab-9d43-4b1b-b35c-092f2b1b5aab').carousel('pause');
                $(this).attr("id", "8caac2ab-9d43-4b1b-b35c-092f2b1b5aab-play");
                $("#8caac2ab-9d43-4b1b-b35c-092f2b1b5aab-play i").attr("class", "fa fa-play");
            }
        });
        
