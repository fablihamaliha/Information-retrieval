			$('#bf33bd04-2c65-499d-86a5-bdb7f96dc0fc table').removeAttr("cellspacing");
			
			$('#bf33bd04-2c65-499d-86a5-bdb7f96dc0fc table').DataTable({
				
				"sDom":"lrtp",
				"sort":false,
				"pagingType": "simple_numbers","paging":false,
				"autoWidth":false
			});
		
