                        if(latLng != null){
                            drawDynamicMapByLatLng('7bc5c431-d0f5-4f88-b56b-aa154c923a34',latLng,'300px',18,true);
                        } else{
                            drawDynamicMapByAddress('7bc5c431-d0f5-4f88-b56b-aa154c923a34','50 Gould St, Toronto, ON M5B 2K3','300px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('7bc5c431-d0f5-4f88-b56b-aa154c923a34',latLng,'50 Gould St, Toronto, ON M5B 2K3','1140px','300px',18,true);
