	$(document).ready(function() {
		$('#34a77fdb-2451-42ce-97bf-3175592864eb table').addClass("table");
	    
			$('#34a77fdb-2451-42ce-97bf-3175592864eb table').addClass("table-striped");
		
		$('#34a77fdb-2451-42ce-97bf-3175592864eb table').addClass("table-borderless");
		
			var removeHeading = false;
			
			$('#34a77fdb-2451-42ce-97bf-3175592864eb table tbody tr').eq(0).find("td").replaceWith(function(){
				return $("<th>", {attr: {'width': $(this).attr('width'), 'rowspan': $(this).attr('rowspan'), 'colspan': $(this).attr('colspan'), 'valign': $(this).attr('valign'), 'headers': $(this).attr('headers'), 'style': $(this).attr('style')}, html: $(this).html()}).css("vertical-align", $(this).attr('valign'));;
			});
			$('#34a77fdb-2451-42ce-97bf-3175592864eb table:not(.dataTable) tbody tr').eq(0).wrap( "<thead></thead>" );
		
			$('#34a77fdb-2451-42ce-97bf-3175592864eb table thead').prependTo('#34a77fdb-2451-42ce-97bf-3175592864eb table');
			
			
			$('#34a77fdb-2451-42ce-97bf-3175592864eb table').removeAttr("border");
			$('#34a77fdb-2451-42ce-97bf-3175592864eb table').removeAttr("cellpadding");
			$('#34a77fdb-2451-42ce-97bf-3175592864eb table').removeAttr("cellspacing");
			
			$('#34a77fdb-2451-42ce-97bf-3175592864eb table').DataTable({
				
				"sDom":"lrtp",
				"sort":false,
				"pagingType": "simple_numbers","paging":false,
				"autoWidth":true
			});
		
			//#NOTE-REMOVE
			if(removeHeading){
				$('#34a77fdb-2451-42ce-97bf-3175592864eb table thead').remove();
			}
			
			
		    
	    
	    
	} );
	</script>
