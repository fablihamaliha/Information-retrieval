<script>
    $(document).ready(function(){
    	
        $("#bb3ba0b5-8727-49d8-bca9-aff5196ed957-play, #bb3ba0b5-8727-49d8-bca9-aff5196ed957-pause").click(function() {
            if ($(this).attr("id") == "bb3ba0b5-8727-49d8-bca9-aff5196ed957-play") {
                $('#bb3ba0b5-8727-49d8-bca9-aff5196ed957').carousel('cycle');
                $(this).attr("id", "bb3ba0b5-8727-49d8-bca9-aff5196ed957-pause");
                $("#bb3ba0b5-8727-49d8-bca9-aff5196ed957-pause i").attr("class", "fa fa-pause");
            } else {
                $('#bb3ba0b5-8727-49d8-bca9-aff5196ed957').carousel('pause');
                $(this).attr("id", "bb3ba0b5-8727-49d8-bca9-aff5196ed957-play");
                $("#bb3ba0b5-8727-49d8-bca9-aff5196ed957-play i").attr("class", "fa fa-play");
            }
        });
        
        	$('#bb3ba0b5-8727-49d8-bca9-aff5196ed957').carousel('pause');
        	
    });
</script>
