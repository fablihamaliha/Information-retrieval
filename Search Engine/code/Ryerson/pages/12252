        $("#367244c3-71c0-4e98-8897-909d1c5798ee-play, #367244c3-71c0-4e98-8897-909d1c5798ee-pause").click(function() {
            if ($(this).attr("id") == "367244c3-71c0-4e98-8897-909d1c5798ee-play") {
                $('#367244c3-71c0-4e98-8897-909d1c5798ee').carousel('cycle');
                $(this).attr("id", "367244c3-71c0-4e98-8897-909d1c5798ee-pause");
                $("#367244c3-71c0-4e98-8897-909d1c5798ee-pause i").attr("class", "fa fa-pause");
            } else {
                $('#367244c3-71c0-4e98-8897-909d1c5798ee').carousel('pause');
                $(this).attr("id", "367244c3-71c0-4e98-8897-909d1c5798ee-play");
                $("#367244c3-71c0-4e98-8897-909d1c5798ee-play i").attr("class", "fa fa-play");
