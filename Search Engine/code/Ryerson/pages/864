    $(document).ready(function(){
    	
        $("#05999cc5-bef6-4826-99a2-99880b12277b-play, #05999cc5-bef6-4826-99a2-99880b12277b-pause").click(function() {
            if ($(this).attr("id") == "05999cc5-bef6-4826-99a2-99880b12277b-play") {
                $('#05999cc5-bef6-4826-99a2-99880b12277b').carousel('cycle');
                $(this).attr("id", "05999cc5-bef6-4826-99a2-99880b12277b-pause");
                $("#05999cc5-bef6-4826-99a2-99880b12277b-pause i").attr("class", "fa fa-pause");
            } else {
                $('#05999cc5-bef6-4826-99a2-99880b12277b').carousel('pause');
                $(this).attr("id", "05999cc5-bef6-4826-99a2-99880b12277b-play");
                $("#05999cc5-bef6-4826-99a2-99880b12277b-play i").attr("class", "fa fa-play");
            }
        });
        
