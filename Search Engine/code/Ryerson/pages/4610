        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#c01ef315-ecd2-48ef-b4ab-0bc513d20145 button").click(function() {
                	jQuery("#c01ef315-ecd2-48ef-b4ab-0bc513d20145.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6577631, "lng": -79.38116409999999}');
