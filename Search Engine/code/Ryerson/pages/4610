                        if(latLng != null){
                            drawDynamicMapByLatLng('c01ef315-ecd2-48ef-b4ab-0bc513d20145',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('c01ef315-ecd2-48ef-b4ab-0bc513d20145','345 Yonge Street, Toronto, ON, M5B 2K3','600px',18,true);
