	$(document).ready(function() {
		$('#b795384d-aac0-40ae-9b60-59327e477085 table').addClass("table");
	    
			$('#b795384d-aac0-40ae-9b60-59327e477085 table').addClass("table-striped");
		
		$('#b795384d-aac0-40ae-9b60-59327e477085 table').addClass("table-borderless");
		
			var removeHeading = false;
			
			$('#b795384d-aac0-40ae-9b60-59327e477085 table tbody tr').eq(0).find("td").replaceWith(function(){
				return $("<th>", {attr: {'width': $(this).attr('width'), 'rowspan': $(this).attr('rowspan'), 'colspan': $(this).attr('colspan'), 'valign': $(this).attr('valign'), 'headers': $(this).attr('headers'), 'style': $(this).attr('style')}, html: $(this).html()}).css("vertical-align", $(this).attr('valign'));;
			});
			$('#b795384d-aac0-40ae-9b60-59327e477085 table:not(.dataTable) tbody tr').eq(0).wrap( "<thead></thead>" );
		
			$('#b795384d-aac0-40ae-9b60-59327e477085 table thead').prependTo('#b795384d-aac0-40ae-9b60-59327e477085 table');
			
			
			$('#b795384d-aac0-40ae-9b60-59327e477085 table').removeAttr("border");
			$('#b795384d-aac0-40ae-9b60-59327e477085 table').removeAttr("cellpadding");
			$('#b795384d-aac0-40ae-9b60-59327e477085 table').removeAttr("cellspacing");
			
			$('#b795384d-aac0-40ae-9b60-59327e477085 table').DataTable({
				
				"columnDefs": [ { "type": "natural-nohtml", "targets": "_all" } ],
				
				"sDom":"lrtp",
				"sort":true,
				"pagingType": "simple_numbers","paging":false,
				"autoWidth":true
			});
		
			//#NOTE-REMOVE
			if(removeHeading){
				$('#b795384d-aac0-40ae-9b60-59327e477085 table thead').remove();
			}
			
			
		    
	    
	    
	} );
	</script>
