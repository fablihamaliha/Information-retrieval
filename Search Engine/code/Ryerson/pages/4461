			$('#2cdc7f47-0da4-4792-abfa-1a98ab0c6e7b table').addClass("table-striped");
		
		$('#2cdc7f47-0da4-4792-abfa-1a98ab0c6e7b table').addClass("table-borderless");
		
			var removeHeading = false;
			
			$('#2cdc7f47-0da4-4792-abfa-1a98ab0c6e7b table tbody tr').eq(0).find("td").replaceWith(function(){
				return $("<th>", {attr: {'width': $(this).attr('width'), 'rowspan': $(this).attr('rowspan'), 'colspan': $(this).attr('colspan'), 'valign': $(this).attr('valign'), 'headers': $(this).attr('headers'), 'style': $(this).attr('style')}, html: $(this).html()}).css("vertical-align", $(this).attr('valign'));;
			});
			$('#2cdc7f47-0da4-4792-abfa-1a98ab0c6e7b table:not(.dataTable) tbody tr').eq(0).wrap( "<thead></thead>" );
		
			$('#2cdc7f47-0da4-4792-abfa-1a98ab0c6e7b table thead').prependTo('#2cdc7f47-0da4-4792-abfa-1a98ab0c6e7b table');
			
			
			$('#2cdc7f47-0da4-4792-abfa-1a98ab0c6e7b table').removeAttr("border");
			$('#2cdc7f47-0da4-4792-abfa-1a98ab0c6e7b table').removeAttr("cellpadding");
			$('#2cdc7f47-0da4-4792-abfa-1a98ab0c6e7b table').removeAttr("cellspacing");
			
			$('#2cdc7f47-0da4-4792-abfa-1a98ab0c6e7b table').DataTable({
				
				"sDom":"lrtp",
				"sort":false,
				"pagingType": "simple_numbers","paging":false,
				"autoWidth":true
			});
		
			//#NOTE-REMOVE
			if(removeHeading){
				$('#2cdc7f47-0da4-4792-abfa-1a98ab0c6e7b table thead').remove();
			}
			
			
		    
	    
	    
	} );
	</script>
