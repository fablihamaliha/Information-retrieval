		$('#e2a307ab-740c-44a9-99aa-2ba875a691a5 table').addClass("table");
	    
		$('#e2a307ab-740c-44a9-99aa-2ba875a691a5 table').addClass("table-bordered");
		
			var removeHeading = false;
			
			$('#e2a307ab-740c-44a9-99aa-2ba875a691a5 table tbody tr').eq(0).find("td").replaceWith(function(){
				return $("<th>", {attr: {'width': $(this).attr('width'), 'rowspan': $(this).attr('rowspan'), 'colspan': $(this).attr('colspan'), 'valign': $(this).attr('valign'), 'headers': $(this).attr('headers'), 'style': $(this).attr('style')}, html: $(this).html()}).css("vertical-align", $(this).attr('valign'));;
			});
			$('#e2a307ab-740c-44a9-99aa-2ba875a691a5 table:not(.dataTable) tbody tr').eq(0).wrap( "<thead></thead>" );
		
			$('#e2a307ab-740c-44a9-99aa-2ba875a691a5 table thead').prependTo('#e2a307ab-740c-44a9-99aa-2ba875a691a5 table');
			
			
