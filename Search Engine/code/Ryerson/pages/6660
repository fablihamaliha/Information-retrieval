			$('#e2a307ab-740c-44a9-99aa-2ba875a691a5 table').removeAttr("cellspacing");
			
			$('#e2a307ab-740c-44a9-99aa-2ba875a691a5 table').DataTable({
				
				"sDom":"lrtp",
				"sort":false,
				"pagingType": "simple_numbers","paging":false,
				"autoWidth":true
			});
		
			//#NOTE-REMOVE
			if(removeHeading){
				$('#e2a307ab-740c-44a9-99aa-2ba875a691a5 table thead').remove();
			}
			
			
		    
	    
	    
	} );
