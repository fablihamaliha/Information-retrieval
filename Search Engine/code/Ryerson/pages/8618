<script>
    $(document).ready(function(){
    	
        $("#2086f209-d1e5-49f1-9c7d-478d5e1545fe-play, #2086f209-d1e5-49f1-9c7d-478d5e1545fe-pause").click(function() {
            if ($(this).attr("id") == "2086f209-d1e5-49f1-9c7d-478d5e1545fe-play") {
                $('#2086f209-d1e5-49f1-9c7d-478d5e1545fe').carousel('cycle');
                $(this).attr("id", "2086f209-d1e5-49f1-9c7d-478d5e1545fe-pause");
                $("#2086f209-d1e5-49f1-9c7d-478d5e1545fe-pause i").attr("class", "fa fa-pause");
            } else {
                $('#2086f209-d1e5-49f1-9c7d-478d5e1545fe').carousel('pause');
                $(this).attr("id", "2086f209-d1e5-49f1-9c7d-478d5e1545fe-play");
