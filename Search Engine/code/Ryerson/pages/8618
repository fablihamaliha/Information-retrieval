        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#edb86f60-1117-43df-bd8d-e455cf428a9c button").click(function() {
                	jQuery("#edb86f60-1117-43df-bd8d-e455cf428a9c.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6577631, "lng": -79.38116409999999}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('edb86f60-1117-43df-bd8d-e455cf428a9c',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('edb86f60-1117-43df-bd8d-e455cf428a9c','345 Yonge Street, Toronto, ON, M5B 2K3','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('edb86f60-1117-43df-bd8d-e455cf428a9c',latLng,'345 Yonge Street, Toronto, ON, M5B 2K3','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('edb86f60-1117-43df-bd8d-e455cf428a9c','345 Yonge Street, Toronto, ON, M5B 2K3','1140px','600px',18,true);
                        }
                    }
                });
            });
