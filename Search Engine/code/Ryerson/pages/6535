        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#f55b3fb4-e9f0-46c5-8937-5be6147c3877 button").click(function() {
                	jQuery("#f55b3fb4-e9f0-46c5-8937-5be6147c3877.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6558668, "lng": -79.3819145}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('f55b3fb4-e9f0-46c5-8937-5be6147c3877',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('f55b3fb4-e9f0-46c5-8937-5be6147c3877','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('f55b3fb4-e9f0-46c5-8937-5be6147c3877',latLng,'1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('f55b3fb4-e9f0-46c5-8937-5be6147c3877','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','1140px','600px',18,true);
                        }
                    }
                });
            });
