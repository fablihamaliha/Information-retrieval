        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component1586703024 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component1586703024 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component1586703024 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component1586703024 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
