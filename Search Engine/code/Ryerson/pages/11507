<script>
$("#rye-c5cdd580-d3ed-4fff-a300-b73d879cc105 input, #rye-c5cdd580-d3ed-4fff-a300-b73d879cc105 select").each(function(i, v) {
	if (!$(this).hasClass('form-control')) {
		if ($(this).is("input[type='file']")) {
