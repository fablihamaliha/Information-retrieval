                jQuery("#87e57cfb-a23a-484b-a0fb-49bd06cbee10 button").click(function() {
                	jQuery("#87e57cfb-a23a-484b-a0fb-49bd06cbee10.res-map-placeholder").removeClass('res-map-placeholder');
