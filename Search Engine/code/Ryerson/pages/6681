                        if(latLng != null){
                            drawDynamicMapByLatLng('87e57cfb-a23a-484b-a0fb-49bd06cbee10',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('87e57cfb-a23a-484b-a0fb-49bd06cbee10','87 Gerrard Street East, Toronto, ON, M5B 1G6, Canada','600px',18,true);
                        }
