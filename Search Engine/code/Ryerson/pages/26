    $(document).ready(function(){
    	
        $("#1eb85b5d-e31d-4772-a865-40db8ba31f3e-play, #1eb85b5d-e31d-4772-a865-40db8ba31f3e-pause").click(function() {
            if ($(this).attr("id") == "1eb85b5d-e31d-4772-a865-40db8ba31f3e-play") {
                $('#1eb85b5d-e31d-4772-a865-40db8ba31f3e').carousel('cycle');
                $(this).attr("id", "1eb85b5d-e31d-4772-a865-40db8ba31f3e-pause");
                $("#1eb85b5d-e31d-4772-a865-40db8ba31f3e-pause i").attr("class", "fa fa-pause");
            } else {
                $('#1eb85b5d-e31d-4772-a865-40db8ba31f3e').carousel('pause');
                $(this).attr("id", "1eb85b5d-e31d-4772-a865-40db8ba31f3e-play");
                $("#1eb85b5d-e31d-4772-a865-40db8ba31f3e-play i").attr("class", "fa fa-play");
