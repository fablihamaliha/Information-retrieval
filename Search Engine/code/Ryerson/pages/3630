        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#e234408a-2bae-4152-baec-777b4f6b07d7 button").click(function() {
                	jQuery("#e234408a-2bae-4152-baec-777b4f6b07d7.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6558668, "lng": -79.3819145}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('e234408a-2bae-4152-baec-777b4f6b07d7',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('e234408a-2bae-4152-baec-777b4f6b07d7','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('e234408a-2bae-4152-baec-777b4f6b07d7',latLng,'1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('e234408a-2bae-4152-baec-777b4f6b07d7','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','1140px','600px',18,true);
                        }
                    }
                });
            });
        </script>
    
    
    
