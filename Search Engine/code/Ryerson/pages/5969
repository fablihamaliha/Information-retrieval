                            drawDynamicMapByLatLng('2cd3662d-a533-4dd9-855c-cde401d4285a',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('2cd3662d-a533-4dd9-855c-cde401d4285a','99 Gerrard Street East, Toronto, ON, M5B 1G7, Canada','600px',18,true);
                        }
