	if (!$(this).hasClass('form-control')) {
		if ($(this).is("input[type='file']")) {
			$(this).attr('id', 'rye-5d65dc9a-34f8-4a94-96ec-fc180c4df061_upload'+i).wrap("<span class='htmlSnippetForm'></span>").after("<label for='rye-5d65dc9a-34f8-4a94-96ec-fc180c4df061_upload"+i+"' class='customInput'>Choose file</label>");
		} else {
			$(this).wrap("<span class='htmlSnippetForm'></span>").after("<span class='customInput'></span>");
		}
	}
});
</script>

