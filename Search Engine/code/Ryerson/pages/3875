    $(document).ready(function(){
    	
        $("#c13bc650-2925-428b-a2eb-992b3ae2aa67-play, #c13bc650-2925-428b-a2eb-992b3ae2aa67-pause").click(function() {
            if ($(this).attr("id") == "c13bc650-2925-428b-a2eb-992b3ae2aa67-play") {
                $('#c13bc650-2925-428b-a2eb-992b3ae2aa67').carousel('cycle');
                $(this).attr("id", "c13bc650-2925-428b-a2eb-992b3ae2aa67-pause");
                $("#c13bc650-2925-428b-a2eb-992b3ae2aa67-pause i").attr("class", "fa fa-pause");
            } else {
                $('#c13bc650-2925-428b-a2eb-992b3ae2aa67').carousel('pause');
                $(this).attr("id", "c13bc650-2925-428b-a2eb-992b3ae2aa67-play");
                $("#c13bc650-2925-428b-a2eb-992b3ae2aa67-play i").attr("class", "fa fa-play");
