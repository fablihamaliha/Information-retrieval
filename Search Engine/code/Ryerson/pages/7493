    $(document).ready(function(){
    	
        $("#dd1dc7a8-090d-4d8e-b069-781f0a0e322e-play, #dd1dc7a8-090d-4d8e-b069-781f0a0e322e-pause").click(function() {
            if ($(this).attr("id") == "dd1dc7a8-090d-4d8e-b069-781f0a0e322e-play") {
                $('#dd1dc7a8-090d-4d8e-b069-781f0a0e322e').carousel('cycle');
                $(this).attr("id", "dd1dc7a8-090d-4d8e-b069-781f0a0e322e-pause");
                $("#dd1dc7a8-090d-4d8e-b069-781f0a0e322e-pause i").attr("class", "fa fa-pause");
            } else {
                $('#dd1dc7a8-090d-4d8e-b069-781f0a0e322e').carousel('pause');
                $(this).attr("id", "dd1dc7a8-090d-4d8e-b069-781f0a0e322e-play");
                $("#dd1dc7a8-090d-4d8e-b069-781f0a0e322e-play i").attr("class", "fa fa-play");
            }
        });
        
