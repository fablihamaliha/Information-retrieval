<script type="text/javascript">
jQuery(document).ready(function() {
    jQuery("#component637129497 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component637129497 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component637129497 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component637129497 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component637129497 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
 </script>
 
