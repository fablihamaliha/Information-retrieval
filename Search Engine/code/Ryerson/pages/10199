        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#3d1b0ac1-01d9-441e-b4f8-9248b662a6da button").click(function() {
                	jQuery("#3d1b0ac1-01d9-441e-b4f8-9248b662a6da.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6558668, "lng": -79.3819145}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('3d1b0ac1-01d9-441e-b4f8-9248b662a6da',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('3d1b0ac1-01d9-441e-b4f8-9248b662a6da','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('3d1b0ac1-01d9-441e-b4f8-9248b662a6da',latLng,'1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('3d1b0ac1-01d9-441e-b4f8-9248b662a6da','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','1140px','600px',18,true);
                        }
                    }
                });
            });
