            if ($(this).attr("id") == "dba9e2bd-1d1c-4f6b-9900-6a98df4e0977-play") {
                $('#dba9e2bd-1d1c-4f6b-9900-6a98df4e0977').carousel('cycle');
                $(this).attr("id", "dba9e2bd-1d1c-4f6b-9900-6a98df4e0977-pause");
