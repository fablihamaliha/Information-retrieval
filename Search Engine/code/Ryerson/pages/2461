jQuery(document).ready(function() {
    jQuery("#component198868600 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component198868600 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component198868600 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component198868600 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component198868600 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
 </script>
 
