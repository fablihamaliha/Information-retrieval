                    $parent.find('#s-lg-0-more-info-database-less-10894424').toggle();
                    $parent.find('#s-lg-0-more-info-database-more-10894424').toggle();
                ">more...</button>
                        <button
                            class="s-lg-label-less btn btn-sm btn-link"
                            aria-label="Show more information about this database"
                            aria-expanded="true"
                            id="s-lg-0-more-info-database-less-10894424"
                            style="display:none;"
                            onclick="
                    var $parent = jQuery(this).closest('#s-lg-content-10894424');
                    $parent.find('#s-lg-0-more-info-database-moreinfo-10894424').toggle();
                    $parent.find('#s-lg-0-more-info-database-less-10894424').toggle();
                    $parent.find('#s-lg-0-more-info-database-more-10894424').toggle();
                ">less...</button>
                    </div>
                    <div id="s-lg-0-more-info-database-moreinfo-10894424" class="s-lg-content-more-info " style="display:none;">You have access to:<br />
Baltimore Sun<br />
Chicago Tribune<br />
Greenwich Time<br />
Hartford Courant<br />
Los Angeles Times<br />
Morning Call<br />
