<script>
    $(document).ready(function(){
    	
        $("#fb5b4ff8-90d2-4b09-9305-7b9541b6f338-play, #fb5b4ff8-90d2-4b09-9305-7b9541b6f338-pause").click(function() {
            if ($(this).attr("id") == "fb5b4ff8-90d2-4b09-9305-7b9541b6f338-play") {
                $('#fb5b4ff8-90d2-4b09-9305-7b9541b6f338').carousel('cycle');
                $(this).attr("id", "fb5b4ff8-90d2-4b09-9305-7b9541b6f338-pause");
                $("#fb5b4ff8-90d2-4b09-9305-7b9541b6f338-pause i").attr("class", "fa fa-pause");
            } else {
                $('#fb5b4ff8-90d2-4b09-9305-7b9541b6f338').carousel('pause');
                $(this).attr("id", "fb5b4ff8-90d2-4b09-9305-7b9541b6f338-play");
                $("#fb5b4ff8-90d2-4b09-9305-7b9541b6f338-play i").attr("class", "fa fa-play");
            }
        });
        
        		$('#fb5b4ff8-90d2-4b09-9305-7b9541b6f338').carousel('cycle');
        	
    });
</script>
