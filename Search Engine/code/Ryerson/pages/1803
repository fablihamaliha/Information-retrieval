jQuery(document).ready(function() {
    jQuery("#component39522052 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component39522052 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component39522052 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component39522052 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component39522052 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
 </script>
 
