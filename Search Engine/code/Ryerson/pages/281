		if ($(this).is("input[type='file']")) {
			$(this).attr('id', 'rye-305e20c7-05cb-4a45-887d-35b6a6cfac1e_upload'+i).wrap("<span class='htmlSnippetForm'></span>").after("<label for='rye-305e20c7-05cb-4a45-887d-35b6a6cfac1e_upload"+i+"' class='customInput'>Choose file</label>");
		} else {
			$(this).wrap("<span class='htmlSnippetForm'></span>").after("<span class='customInput'></span>");
		}
	}
});
</script>

