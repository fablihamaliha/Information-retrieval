        <script>
        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#7ceb9894-8ce3-4ed2-b2c4-9632b985106a button").click(function() {
                	jQuery("#7ceb9894-8ce3-4ed2-b2c4-9632b985106a.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6586299, "lng": -79.3770727}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('7ceb9894-8ce3-4ed2-b2c4-9632b985106a',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('7ceb9894-8ce3-4ed2-b2c4-9632b985106a','80 Gould St, Toronto, ON M5B 2M7','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('7ceb9894-8ce3-4ed2-b2c4-9632b985106a',latLng,'80 Gould St, Toronto, ON M5B 2M7','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('7ceb9894-8ce3-4ed2-b2c4-9632b985106a','80 Gould St, Toronto, ON M5B 2M7','1140px','600px',18,true);
                        }
                    }
                });
            });
        </script>
    
    
    
