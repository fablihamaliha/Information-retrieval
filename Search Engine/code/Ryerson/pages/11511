    $(document).ready(function(){
    	
        $("#de475dca-907b-4e39-a187-a52720a210ec-play, #de475dca-907b-4e39-a187-a52720a210ec-pause").click(function() {
            if ($(this).attr("id") == "de475dca-907b-4e39-a187-a52720a210ec-play") {
                $('#de475dca-907b-4e39-a187-a52720a210ec').carousel('cycle');
                $(this).attr("id", "de475dca-907b-4e39-a187-a52720a210ec-pause");
                $("#de475dca-907b-4e39-a187-a52720a210ec-pause i").attr("class", "fa fa-pause");
            } else {
                $('#de475dca-907b-4e39-a187-a52720a210ec').carousel('pause');
                $(this).attr("id", "de475dca-907b-4e39-a187-a52720a210ec-play");
                $("#de475dca-907b-4e39-a187-a52720a210ec-play i").attr("class", "fa fa-play");
