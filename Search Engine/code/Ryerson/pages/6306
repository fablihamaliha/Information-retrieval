                jQuery("#68ecb4c3-a9ef-4d82-b552-b2999bdd7b76 button").click(function() {
                	jQuery("#68ecb4c3-a9ef-4d82-b552-b2999bdd7b76.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6600414, "lng": -79.3771506}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('68ecb4c3-a9ef-4d82-b552-b2999bdd7b76',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('68ecb4c3-a9ef-4d82-b552-b2999bdd7b76','99 Gerrard Street East, Toronto, ON, M5B 1G7, Canada','600px',18,true);
                        }
