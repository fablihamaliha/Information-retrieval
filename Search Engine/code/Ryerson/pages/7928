			var removeHeading = false;
			
			$('#877d53e0-9c7b-4ed9-b2c6-61d318e1339d table tbody tr').eq(0).find("td").replaceWith(function(){
				return $("<th>", {attr: {'width': $(this).attr('width'), 'rowspan': $(this).attr('rowspan'), 'colspan': $(this).attr('colspan'), 'valign': $(this).attr('valign'), 'headers': $(this).attr('headers'), 'style': $(this).attr('style')}, html: $(this).html()}).css("vertical-align", $(this).attr('valign'));;
			});
			$('#877d53e0-9c7b-4ed9-b2c6-61d318e1339d table:not(.dataTable) tbody tr').eq(0).wrap( "<thead></thead>" );
		
			$('#877d53e0-9c7b-4ed9-b2c6-61d318e1339d table thead').prependTo('#877d53e0-9c7b-4ed9-b2c6-61d318e1339d table');
			
			
			$('#877d53e0-9c7b-4ed9-b2c6-61d318e1339d table').removeAttr("border");
			$('#877d53e0-9c7b-4ed9-b2c6-61d318e1339d table').removeAttr("cellpadding");
			$('#877d53e0-9c7b-4ed9-b2c6-61d318e1339d table').removeAttr("cellspacing");
			
			$('#877d53e0-9c7b-4ed9-b2c6-61d318e1339d table').DataTable({
				
				"columnDefs": [ { "type": "natural-nohtml", "targets": "_all" } ],
				
