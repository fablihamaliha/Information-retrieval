jQuery(document).ready(function() {
    jQuery("#component534820629 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component534820629 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component534820629 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component534820629 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component534820629 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
 </script>
 
