<style>

@font-face {
  font-family: "Replica-Heavyitalic";
  src: url("/content/dam/brand/global/fonts/lineto-replica-heavyitalic.eot");
  src: url("/content/dam/brand/global/fonts/lineto-replica-heavyitalic.eot?#iefix") format("embedded-opentype"), url("/content/dam/brand/global/fonts/lineto-replica-heavyitalic.woff") format("woff"), url("/content/dam/brand/global/fonts/lineto-replica-heavyitalic.ttf") format("truetype"), url("/content/dam/brand/global/fonts/lineto-replica-heavyitalic.svg#lineto-replica-heavyitalic") format("svg");
  font-weight: normal;
  font-style: normal; }
@font-face {
