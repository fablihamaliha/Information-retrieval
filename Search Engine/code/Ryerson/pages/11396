        <script>
        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#3cc2ea1f-ad31-410a-a236-88b3dbcd3143 button").click(function() {
                	jQuery("#3cc2ea1f-ad31-410a-a236-88b3dbcd3143.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = false;
                    var latLng = JSON.parse('{"lat": 43.6586299, "lng": -79.3770727}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('3cc2ea1f-ad31-410a-a236-88b3dbcd3143',latLng,'300px',18,true);
                        } else{
                            drawDynamicMapByAddress('3cc2ea1f-ad31-410a-a236-88b3dbcd3143','80 Gould Street, Toronto, ON, M5B 2K3, Canada','300px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('3cc2ea1f-ad31-410a-a236-88b3dbcd3143',latLng,'80 Gould Street, Toronto, ON, M5B 2K3, Canada','1140px','300px',18,true);
                        } else{
                            drawStaticMapByAddress('3cc2ea1f-ad31-410a-a236-88b3dbcd3143','80 Gould Street, Toronto, ON, M5B 2K3, Canada','1140px','300px',18,true);
                        }
                    }
                });
            });
