branda_footer_node.innerHTML = "<p class=\"adminfooter\" style=\"text-align: center;\">This site is hosted by <a href=\"http:\/\/blog.ryerson.ca\">blog.ryerson.ca<\/a>. Opinions expressed in this site do not necessarily represent those of Ryerson University.<\/p>";
branda_footer = document.getElementsByTagName( 'footer' );
if ( branda_footer.length ) {
	branda_footer = branda_footer[ branda_footer.length - 1 ];
	branda_footer.appendChild( branda_footer_node, branda_footer.firstChild );
}
</script>
