            if ($(this).attr("id") == "7124110f-8f59-4986-8289-76a49a0f4dbd-play") {
                $('#7124110f-8f59-4986-8289-76a49a0f4dbd').carousel('cycle');
                $(this).attr("id", "7124110f-8f59-4986-8289-76a49a0f4dbd-pause");
                $("#7124110f-8f59-4986-8289-76a49a0f4dbd-pause i").attr("class", "fa fa-pause");
            } else {
                $('#7124110f-8f59-4986-8289-76a49a0f4dbd').carousel('pause');
                $(this).attr("id", "7124110f-8f59-4986-8289-76a49a0f4dbd-play");
                $("#7124110f-8f59-4986-8289-76a49a0f4dbd-play i").attr("class", "fa fa-play");
