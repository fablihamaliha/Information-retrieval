            <div class="panel panel-default">
                <!-- Accordion Heading -->
                <div class="panel-heading container-clipboard">
                    <h2 class="panel-title">
                        <a id="accordion-1519927582002-usage" role="tab" data-toggle="collapse" data-parent="#" href="#accordion-content-1519927582002-usage" aria-controls="accordion-content-1519927582002-usage" aria-expanded="false">
