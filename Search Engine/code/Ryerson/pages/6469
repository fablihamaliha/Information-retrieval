        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#e9f850ae-1a67-401d-8d8f-355f05484114 button").click(function() {
                	jQuery("#e9f850ae-1a67-401d-8d8f-355f05484114.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.657564, "lng": -79.3772027}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('e9f850ae-1a67-401d-8d8f-355f05484114',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('e9f850ae-1a67-401d-8d8f-355f05484114','245 Church Street, Toronto, ON, M5B 1Z2, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('e9f850ae-1a67-401d-8d8f-355f05484114',latLng,'245 Church Street, Toronto, ON, M5B 1Z2, Canada','1140px','600px',18,true);
