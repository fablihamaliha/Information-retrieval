        <script>
        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#54297637-06db-4cfb-b218-e0bb0b56134b button").click(function() {
                	jQuery("#54297637-06db-4cfb-b218-e0bb0b56134b.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6558668, "lng": -79.3819145}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('54297637-06db-4cfb-b218-e0bb0b56134b',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('54297637-06db-4cfb-b218-e0bb0b56134b','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('54297637-06db-4cfb-b218-e0bb0b56134b',latLng,'1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('54297637-06db-4cfb-b218-e0bb0b56134b','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','1140px','600px',18,true);
                        }
                    }
                });
            });
        </script>
    
    
    
</div>
