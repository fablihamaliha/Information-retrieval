<script>
function getPage(){
    var tempPage =  "NEVER_SELECT_PLACEHOLDER";
  if(location.pathname.indexOf("equity/") >= 0){
    tempPage = location.pathname.substring(location.pathname.indexOf("equity/") + 7);

    if(tempPage.indexOf("/") >= 0){
    tempPage = tempPage.substring(0,tempPage.indexOf("/"));
    }

    if(tempPage.indexOf(".") >= 0){
    tempPage = tempPage.substring(0,tempPage.indexOf("."));
    }

  }
  return tempPage;
}
jQuery(document).ready(function() {
jQuery(".leftContainer a[href*='"+getPage()+"']").addClass("selected");
if (location.pathname.indexOf("/hr/equity/index.html") >= 0) {
    jQuery(".leftContainer a").addClass('deselected');
  }
});
</script></div>
