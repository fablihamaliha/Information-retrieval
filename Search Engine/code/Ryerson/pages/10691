	$(document).ready(function() {
		$('#19511f64-b1fb-42f3-8099-155bbf2a2a4b table').addClass("table");
	    
		$('#19511f64-b1fb-42f3-8099-155bbf2a2a4b table').addClass("table-bordered");
		
			var removeHeading = false;
			
				//mandatory to have heading on initialization but remove it after. See #NOTE-REMOVE
				if($('#19511f64-b1fb-42f3-8099-155bbf2a2a4b table thead').eq(0).length == 0){
						//insert fake row and delete
						removeHeading = true;
						var numOfColsTD = $('#19511f64-b1fb-42f3-8099-155bbf2a2a4b table tbody tr').eq(0).find('td').length;
						var numOfColsTH = $('#19511f64-b1fb-42f3-8099-155bbf2a2a4b table tbody tr').eq(0).find('th').length;
						
						var appendTH = "<thead>";
						for(var i = 0; i < numOfColsTD+numOfColsTH;i++){
							appendTH += "<th>Column</th>";
						}
						appendTH += "</thead>";
						$('#19511f64-b1fb-42f3-8099-155bbf2a2a4b table').prepend(appendTH);
				}
			
			
			$('#19511f64-b1fb-42f3-8099-155bbf2a2a4b table').removeAttr("border");
			$('#19511f64-b1fb-42f3-8099-155bbf2a2a4b table').removeAttr("cellpadding");
			$('#19511f64-b1fb-42f3-8099-155bbf2a2a4b table').removeAttr("cellspacing");
			
			$('#19511f64-b1fb-42f3-8099-155bbf2a2a4b table').DataTable({
				
				"sDom":"lrtp",
				"sort":false,
				"pagingType": "simple_numbers","paging":false,
				"autoWidth":true
			});
		
			//#NOTE-REMOVE
			if(removeHeading){
				$('#19511f64-b1fb-42f3-8099-155bbf2a2a4b table thead').remove();
			}
			
			
		    
	    
	    
	} );
