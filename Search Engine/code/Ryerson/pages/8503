<script type="text/javascript">
jQuery(document).ready(function() {
    var calcWidth = 0;
    
    
                calcWidth = 0;
                jQuery('.menuItem').each(function(index) {
                    calcWidth += parseInt(jQuery(this).width(), 10) + 6;
                });
                var speed = 350;//speed of animation
                var expandedSize = '130px';
                var minSize = '50px';

                jQuery('.menuItem').hoverIntent( 

                     function () { 
                   var currentNum = jQuery(this).attr('id').substr(jQuery(this).attr('id').length - 1);//get ordering position in menu 
                            var itemWidth = jQuery(this).width();//get width of each menu item 
                            jQuery(".currentMenuItem").removeClass("currentMenuItem");
                            jQuery(this).addClass("currentMenuItem");
                            jQuery('.links').stop(true,true).animate({opacity:"0"},{duration:350});//animate it fading out 
                            jQuery('.links').css("display","none");//hide all links 
                            jQuery('#links' + currentNum).css("display","block");//make one visible 
                            jQuery('#links' + currentNum).css("height","auto"); 
                            jQuery('#links' + currentNum).stop(true,true).animate({opacity:"1.0"},{duration:350});//animate it fading in 
                     },   
                     function () { 
                       var currentNum = jQuery(this).attr('id').substr(jQuery(this).attr('id').length - 1);//get ordering position in menu 
                     } 
                   ); 
                jQuery('#megaMenuContainer').hover(function(){
                }, function(){
                    jQuery(".currentMenuItem").removeClass("currentMenuItem");
                   jQuery('.links').animate({height:"0px"}, {queue:false, duration:speed});//close all incase they are visible
                });
                jQuery('ul.sf-menu').supersubs({  
                    minWidth:    5,   // minimum width of sub-menus in em units 
                    maxWidth:    15,   // maximum width of sub-menus in em units 
                    extraWidth:  1     // extra width can ensure lines don't sometimes turn over 
                });
                jQuery('.sf-menu').css("width",calcWidth);
            
}); 
</script> 

