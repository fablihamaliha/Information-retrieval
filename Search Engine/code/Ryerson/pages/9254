        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#338b925d-9ac2-458b-b63c-a040141132ad button").click(function() {
                	jQuery("#338b925d-9ac2-458b-b63c-a040141132ad.res-map-placeholder").removeClass('res-map-placeholder');
