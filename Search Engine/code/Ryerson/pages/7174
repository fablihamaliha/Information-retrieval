<script>
    $(document).ready(function(){
    	
        $("#7368b4d4-6b31-4cb2-975e-69bcd41289a0-play, #7368b4d4-6b31-4cb2-975e-69bcd41289a0-pause").click(function() {
            if ($(this).attr("id") == "7368b4d4-6b31-4cb2-975e-69bcd41289a0-play") {
                $('#7368b4d4-6b31-4cb2-975e-69bcd41289a0').carousel('cycle');
                $(this).attr("id", "7368b4d4-6b31-4cb2-975e-69bcd41289a0-pause");
                $("#7368b4d4-6b31-4cb2-975e-69bcd41289a0-pause i").attr("class", "fa fa-pause");
            } else {
                $('#7368b4d4-6b31-4cb2-975e-69bcd41289a0').carousel('pause');
                $(this).attr("id", "7368b4d4-6b31-4cb2-975e-69bcd41289a0-play");
                $("#7368b4d4-6b31-4cb2-975e-69bcd41289a0-play i").attr("class", "fa fa-play");
            }
        });
        
