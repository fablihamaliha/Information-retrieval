<script>
    $(document).ready(function(){
    	
        $("#72e2c5e6-decf-4c5c-bf0b-c2d9c0268841-play, #72e2c5e6-decf-4c5c-bf0b-c2d9c0268841-pause").click(function() {
            if ($(this).attr("id") == "72e2c5e6-decf-4c5c-bf0b-c2d9c0268841-play") {
                $('#72e2c5e6-decf-4c5c-bf0b-c2d9c0268841').carousel('cycle');
                $(this).attr("id", "72e2c5e6-decf-4c5c-bf0b-c2d9c0268841-pause");
                $("#72e2c5e6-decf-4c5c-bf0b-c2d9c0268841-pause i").attr("class", "fa fa-pause");
            } else {
                $('#72e2c5e6-decf-4c5c-bf0b-c2d9c0268841').carousel('pause');
                $(this).attr("id", "72e2c5e6-decf-4c5c-bf0b-c2d9c0268841-play");
                $("#72e2c5e6-decf-4c5c-bf0b-c2d9c0268841-play i").attr("class", "fa fa-play");
            }
        });
        
        	$('#72e2c5e6-decf-4c5c-bf0b-c2d9c0268841').carousel('pause');
        	
    });
</script>
