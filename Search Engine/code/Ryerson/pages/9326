jQuery(document).ready(function() {
    jQuery("#component541551484 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component541551484 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component541551484 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component541551484 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component541551484 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
 </script>
 
