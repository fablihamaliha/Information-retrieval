            <div class="panel panel-default">
                <!-- Accordion Heading -->
                <div class="panel-heading container-clipboard">
                    <h2 class="panel-title">
                        <a id="accordion-1505924205742-athletics" role="tab" data-toggle="collapse" data-parent="#" href="#accordion-content-1505924205742-athletics" aria-controls="accordion-content-1505924205742-athletics" aria-expanded="false">
