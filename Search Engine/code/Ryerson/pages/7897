                $('#1d728304-d51a-4d07-84e6-74e7a0bc8522').carousel('cycle');
                $(this).attr("id", "1d728304-d51a-4d07-84e6-74e7a0bc8522-pause");
                $("#1d728304-d51a-4d07-84e6-74e7a0bc8522-pause i").attr("class", "fa fa-pause");
