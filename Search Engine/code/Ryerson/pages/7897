                $('#1d728304-d51a-4d07-84e6-74e7a0bc8522').carousel('pause');
                $(this).attr("id", "1d728304-d51a-4d07-84e6-74e7a0bc8522-play");
                $("#1d728304-d51a-4d07-84e6-74e7a0bc8522-play i").attr("class", "fa fa-play");
