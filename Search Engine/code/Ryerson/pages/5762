        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#0f0c96a5-0424-491e-b0a0-c84f73c10259 button").click(function() {
                	jQuery("#0f0c96a5-0424-491e-b0a0-c84f73c10259.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6558668, "lng": -79.3819145}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('0f0c96a5-0424-491e-b0a0-c84f73c10259',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('0f0c96a5-0424-491e-b0a0-c84f73c10259','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('0f0c96a5-0424-491e-b0a0-c84f73c10259',latLng,'1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('0f0c96a5-0424-491e-b0a0-c84f73c10259','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','1140px','600px',18,true);
                        }
                    }
                });
            });
