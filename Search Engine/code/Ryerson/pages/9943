	$(document).ready(function() {
		$('#1a9e01fa-4f00-4d19-84c9-d436dd44ee21 table').addClass("table");
	    
			$('#1a9e01fa-4f00-4d19-84c9-d436dd44ee21 table').addClass("table-striped");
		
		$('#1a9e01fa-4f00-4d19-84c9-d436dd44ee21 table').addClass("table-borderless");
		
			var removeHeading = false;
			
			$('#1a9e01fa-4f00-4d19-84c9-d436dd44ee21 table tbody tr').eq(0).find("td").replaceWith(function(){
				return $("<th>", {attr: {'width': $(this).attr('width'), 'rowspan': $(this).attr('rowspan'), 'colspan': $(this).attr('colspan'), 'valign': $(this).attr('valign'), 'headers': $(this).attr('headers'), 'style': $(this).attr('style')}, html: $(this).html()}).css("vertical-align", $(this).attr('valign'));;
			});
			$('#1a9e01fa-4f00-4d19-84c9-d436dd44ee21 table:not(.dataTable) tbody tr').eq(0).wrap( "<thead></thead>" );
		
			$('#1a9e01fa-4f00-4d19-84c9-d436dd44ee21 table thead').prependTo('#1a9e01fa-4f00-4d19-84c9-d436dd44ee21 table');
			
			
			$('#1a9e01fa-4f00-4d19-84c9-d436dd44ee21 table').removeAttr("border");
			$('#1a9e01fa-4f00-4d19-84c9-d436dd44ee21 table').removeAttr("cellpadding");
			$('#1a9e01fa-4f00-4d19-84c9-d436dd44ee21 table').removeAttr("cellspacing");
			
			$('#1a9e01fa-4f00-4d19-84c9-d436dd44ee21 table').DataTable({
				
				"sDom":"lrtp",
				"sort":false,
				"pagingType": "simple_numbers","paging":false,
				"autoWidth":true
			});
		
			//#NOTE-REMOVE
			if(removeHeading){
				$('#1a9e01fa-4f00-4d19-84c9-d436dd44ee21 table thead').remove();
			}
			
			
		    
	    
	    
	} );
	</script>
