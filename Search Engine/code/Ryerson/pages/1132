<script>
    $(document).ready(function(){
    	
        $("#a0652fac-46d9-4e11-b429-0e66fbe6c085-play, #a0652fac-46d9-4e11-b429-0e66fbe6c085-pause").click(function() {
            if ($(this).attr("id") == "a0652fac-46d9-4e11-b429-0e66fbe6c085-play") {
                $('#a0652fac-46d9-4e11-b429-0e66fbe6c085').carousel('cycle');
                $(this).attr("id", "a0652fac-46d9-4e11-b429-0e66fbe6c085-pause");
                $("#a0652fac-46d9-4e11-b429-0e66fbe6c085-pause i").attr("class", "fa fa-pause");
            } else {
                $('#a0652fac-46d9-4e11-b429-0e66fbe6c085').carousel('pause');
                $(this).attr("id", "a0652fac-46d9-4e11-b429-0e66fbe6c085-play");
                $("#a0652fac-46d9-4e11-b429-0e66fbe6c085-play i").attr("class", "fa fa-play");
            }
        });
        
        		$('#a0652fac-46d9-4e11-b429-0e66fbe6c085').carousel('cycle');
        	
    });
</script>
