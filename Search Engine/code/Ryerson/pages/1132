    $(document).ready(function(){
    	
        $("#ace8fdd9-8865-4ee8-b73a-b4b4ef3ca0ab-play, #ace8fdd9-8865-4ee8-b73a-b4b4ef3ca0ab-pause").click(function() {
            if ($(this).attr("id") == "ace8fdd9-8865-4ee8-b73a-b4b4ef3ca0ab-play") {
                $('#ace8fdd9-8865-4ee8-b73a-b4b4ef3ca0ab').carousel('cycle');
                $(this).attr("id", "ace8fdd9-8865-4ee8-b73a-b4b4ef3ca0ab-pause");
                $("#ace8fdd9-8865-4ee8-b73a-b4b4ef3ca0ab-pause i").attr("class", "fa fa-pause");
            } else {
                $('#ace8fdd9-8865-4ee8-b73a-b4b4ef3ca0ab').carousel('pause');
                $(this).attr("id", "ace8fdd9-8865-4ee8-b73a-b4b4ef3ca0ab-play");
                $("#ace8fdd9-8865-4ee8-b73a-b4b4ef3ca0ab-play i").attr("class", "fa fa-play");
            }
        });
        
        	$('#ace8fdd9-8865-4ee8-b73a-b4b4ef3ca0ab').carousel('pause');
        	
    });
