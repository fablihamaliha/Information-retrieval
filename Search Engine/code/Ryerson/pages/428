<script>
    $(document).ready(function(){
    	
        $("#1ec7d760-0b9f-4051-bdd1-0da1efc5084f-play, #1ec7d760-0b9f-4051-bdd1-0da1efc5084f-pause").click(function() {
            if ($(this).attr("id") == "1ec7d760-0b9f-4051-bdd1-0da1efc5084f-play") {
                $('#1ec7d760-0b9f-4051-bdd1-0da1efc5084f').carousel('cycle');
                $(this).attr("id", "1ec7d760-0b9f-4051-bdd1-0da1efc5084f-pause");
                $("#1ec7d760-0b9f-4051-bdd1-0da1efc5084f-pause i").attr("class", "fa fa-pause");
            } else {
                $('#1ec7d760-0b9f-4051-bdd1-0da1efc5084f').carousel('pause');
                $(this).attr("id", "1ec7d760-0b9f-4051-bdd1-0da1efc5084f-play");
                $("#1ec7d760-0b9f-4051-bdd1-0da1efc5084f-play i").attr("class", "fa fa-play");
            }
        });
        
        		$('#1ec7d760-0b9f-4051-bdd1-0da1efc5084f').carousel('cycle');
        	
    });
</script>
