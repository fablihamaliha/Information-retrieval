<script>
$("#rye-eec74580-44b9-413d-8360-aed6dc476084 input, #rye-eec74580-44b9-413d-8360-aed6dc476084 select").each(function(i, v) {
	if (!$(this).hasClass('form-control')) {
		if ($(this).is("input[type='file']")) {
			$(this).attr('id', 'rye-eec74580-44b9-413d-8360-aed6dc476084_upload'+i).wrap("<span class='htmlSnippetForm'></span>").after("<label for='rye-eec74580-44b9-413d-8360-aed6dc476084_upload"+i+"' class='customInput'>Choose file</label>");
		} else {
			$(this).wrap("<span class='htmlSnippetForm'></span>").after("<span class='customInput'></span>");
		}
	}
});
</script>

