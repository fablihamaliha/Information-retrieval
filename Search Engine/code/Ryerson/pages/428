            if ($(this).attr("id") == "1b6d7637-b4be-434a-835f-9253407d44b8-play") {
                $('#1b6d7637-b4be-434a-835f-9253407d44b8').carousel('cycle');
                $(this).attr("id", "1b6d7637-b4be-434a-835f-9253407d44b8-pause");
                $("#1b6d7637-b4be-434a-835f-9253407d44b8-pause i").attr("class", "fa fa-pause");
            } else {
                $('#1b6d7637-b4be-434a-835f-9253407d44b8').carousel('pause');
                $(this).attr("id", "1b6d7637-b4be-434a-835f-9253407d44b8-play");
                $("#1b6d7637-b4be-434a-835f-9253407d44b8-play i").attr("class", "fa fa-play");
