        <script>
        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#f4540d2b-ab5e-4644-8612-011fb7fc61d9 button").click(function() {
                	jQuery("#f4540d2b-ab5e-4644-8612-011fb7fc61d9.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6600414, "lng": -79.3771506}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('f4540d2b-ab5e-4644-8612-011fb7fc61d9',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('f4540d2b-ab5e-4644-8612-011fb7fc61d9','99 Gerrard Street East, Toronto, On M5B 1G7','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('f4540d2b-ab5e-4644-8612-011fb7fc61d9',latLng,'99 Gerrard Street East, Toronto, On M5B 1G7','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('f4540d2b-ab5e-4644-8612-011fb7fc61d9','99 Gerrard Street East, Toronto, On M5B 1G7','1140px','600px',18,true);
                        }
                    }
