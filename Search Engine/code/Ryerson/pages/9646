jQuery(document).ready(function() {
    jQuery("#component984887661 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component984887661 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component984887661 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component984887661 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component984887661 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
 </script>
 
