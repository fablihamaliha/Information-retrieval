jQuery(document).ready(function() {
    jQuery("#component324835392 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component324835392 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component324835392 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component324835392 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component324835392 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
