<script type="text/javascript">
jQuery(document).ready(function() {
    jQuery("#component750714515 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component750714515 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component750714515 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component750714515 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component750714515 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
