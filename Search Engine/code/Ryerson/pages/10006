 <script>



jqcal(document).ready(function() {
    jQuery('<img/>')[0].src = "/etc.clientlibs/ryerson/static/uiResources/resources/uiwCalendarList/loadingGif.gif";
        var currentDate = new Date();
    var datepicker = {calendarPath: "/content/ryerson/criminology/students/left/jcr:content/center/uiweventscalendar", cache: {events:new Object()},calendar:null,cacheStatus: new Object(),selectorComponent:"#component315783841",selectorCalendar:"#component315783841 .smallCalendar",selectorID:"315783841",visibleDate:new Object(),state:"loading"};
    datepicker.visibleDate = {"year":currentDate.getFullYear(),"month":currentDate.getMonth()+1};
    datepicker.calendar = jQuery(datepicker.selectorCalendar).datepicker({
        onSelect: function(){
            selectCalendarListDate(datepicker);
        },
        onChangeMonthYear:function(year,month,inst){
            datepicker.visibleDate = {"year":year,"month":month};
            if(!(year + " " + (month) in datepicker.cacheStatus) || datepicker.cacheStatus[year + " " + (month)].events == false || datepicker.cacheStatus[year + " " + (month)].sigDates == false){
                cacheCalendarEvents(year,(month),datepicker);
            }
        },
        beforeShowDay: function(date) {
			var eventClass = "";
                var dateDay = date.getDay();
                var dateKey = datepicker.visibleDate.year + " " + datepicker.visibleDate.month;
			if(datepicker.cacheStatus[dateKey] !== undefined && datepicker.cacheStatus[dateKey].events == false){
            	jQuery(datepicker.selectorComponent + " table.ui-datepicker-calendar tbody" ).css("background-color","red");
            	return [false, ' customTintCalendar'];
            }
            var currentViewPanelOption = jQuery(datepicker.selectorComponent + " .calendarEventOptionsPanel .checkListOn").html();
            if(datepicker.cacheStatus[dateKey] !== undefined && datepicker.cacheStatus[dateKey].events !== undefined && datepicker.cacheStatus[dateKey].events == true){
                var numberOfEvents = 0;
                for(var i = 0;i < datepicker.cache.events[dateKey].items.length;i++){
                    var startDate = new Date(datepicker.cache.events[dateKey].items[i].start);
                    var endDate = new Date(datepicker.cache.events[dateKey].items[i].end);
                    startDate.setHours(0,0,0,0);
                    endDate.setHours(23,59,59,999);
                    if(startDate.getTime() <= date.getTime() && date.getTime() <= endDate.getTime() ){
                        numberOfEvents++;
                    }
                }

                if(numberOfEvents > 1){
                    eventClass = "smallCalendarMultiEvent";
                } else if(numberOfEvents == 1){
                    eventClass = "smallCalendarSingleEvent";
                }

                if(eventClass.length > 0){
                    return [true, eventClass];
                }
            }
            return [true, ' customTintCalendar'];
        },
        beforeShow: function(input, inst) {      
            //remove default hovers for custom colours to work
            window.setTimeout(function(){
                jqcal("body").find('.ui-state-highlight .ui-state-hover').removeClass('ui-state-highlight ui-state-hover');
            },0)
        }
    });

    cacheCalendarEvents(currentDate.getFullYear(),currentDate.getMonth()+1,datepicker);

    //remove default hovers for custom colours to work
    jqcal("a, .ui-state-hover").removeClass('ui-state-hover');



});

</script>
