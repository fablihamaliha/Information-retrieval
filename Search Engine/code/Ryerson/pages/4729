jQuery(document).ready(function() {
    jQuery("#component684906932 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component684906932 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component684906932 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component684906932 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component684906932 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
 </script>
 
