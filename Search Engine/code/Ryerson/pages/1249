        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#ed0b424d-62b0-4ea5-b0a8-4e9d5747fd1a button").click(function() {
                	jQuery("#ed0b424d-62b0-4ea5-b0a8-4e9d5747fd1a.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6558668, "lng": -79.3819145}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('ed0b424d-62b0-4ea5-b0a8-4e9d5747fd1a',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('ed0b424d-62b0-4ea5-b0a8-4e9d5747fd1a','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('ed0b424d-62b0-4ea5-b0a8-4e9d5747fd1a',latLng,'1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','1140px','600px',18,true);
