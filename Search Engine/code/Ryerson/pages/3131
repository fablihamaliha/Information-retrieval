    $(document).ready(function(){
    	
        $("#95ec0e0f-fb96-482f-9ca4-c4ce879b9fbf-play, #95ec0e0f-fb96-482f-9ca4-c4ce879b9fbf-pause").click(function() {
            if ($(this).attr("id") == "95ec0e0f-fb96-482f-9ca4-c4ce879b9fbf-play") {
                $('#95ec0e0f-fb96-482f-9ca4-c4ce879b9fbf').carousel('cycle');
                $(this).attr("id", "95ec0e0f-fb96-482f-9ca4-c4ce879b9fbf-pause");
                $("#95ec0e0f-fb96-482f-9ca4-c4ce879b9fbf-pause i").attr("class", "fa fa-pause");
            } else {
                $('#95ec0e0f-fb96-482f-9ca4-c4ce879b9fbf').carousel('pause');
                $(this).attr("id", "95ec0e0f-fb96-482f-9ca4-c4ce879b9fbf-play");
                $("#95ec0e0f-fb96-482f-9ca4-c4ce879b9fbf-play i").attr("class", "fa fa-play");
            }
        });
        
