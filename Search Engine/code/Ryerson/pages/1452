	$(document).ready(function() {
		$('#ea07b6bf-fbdc-4c69-b4d1-eddf976d0a9c table').addClass("table");
	    
			$('#ea07b6bf-fbdc-4c69-b4d1-eddf976d0a9c table').addClass("table-striped");
		
		$('#ea07b6bf-fbdc-4c69-b4d1-eddf976d0a9c table').addClass("table-borderless");
		
			var removeHeading = false;
			
			$('#ea07b6bf-fbdc-4c69-b4d1-eddf976d0a9c table tbody tr').eq(0).find("td").replaceWith(function(){
				return $("<th>", {attr: {'width': $(this).attr('width'), 'rowspan': $(this).attr('rowspan'), 'colspan': $(this).attr('colspan'), 'valign': $(this).attr('valign'), 'headers': $(this).attr('headers'), 'style': $(this).attr('style')}, html: $(this).html()}).css("vertical-align", $(this).attr('valign'));;
			});
			$('#ea07b6bf-fbdc-4c69-b4d1-eddf976d0a9c table:not(.dataTable) tbody tr').eq(0).wrap( "<thead></thead>" );
		
			$('#ea07b6bf-fbdc-4c69-b4d1-eddf976d0a9c table thead').prependTo('#ea07b6bf-fbdc-4c69-b4d1-eddf976d0a9c table');
			
			
			$('#ea07b6bf-fbdc-4c69-b4d1-eddf976d0a9c table').removeAttr("border");
			$('#ea07b6bf-fbdc-4c69-b4d1-eddf976d0a9c table').removeAttr("cellpadding");
			$('#ea07b6bf-fbdc-4c69-b4d1-eddf976d0a9c table').removeAttr("cellspacing");
			
			$('#ea07b6bf-fbdc-4c69-b4d1-eddf976d0a9c table').DataTable({
				
				"columnDefs": [ { "type": "natural-nohtml", "targets": "_all" } ],
				
				"sDom":"lrtp",
				"sort":true,
				"pagingType": "simple_numbers","paging":false,
				"autoWidth":true
			});
		
			//#NOTE-REMOVE
			if(removeHeading){
				$('#ea07b6bf-fbdc-4c69-b4d1-eddf976d0a9c table thead').remove();
			}
			
			
		    
	    
	    
	} );
	</script>
