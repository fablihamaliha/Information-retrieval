			$('#a6b2a813-ac55-48a8-915c-0dce7be54b6e table').addClass("table-striped");
		
		$('#a6b2a813-ac55-48a8-915c-0dce7be54b6e table').addClass("table-borderless");
		
			var removeHeading = false;
			
			$('#a6b2a813-ac55-48a8-915c-0dce7be54b6e table tbody tr').eq(0).find("td").replaceWith(function(){
				return $("<th>", {attr: {'width': $(this).attr('width'), 'rowspan': $(this).attr('rowspan'), 'colspan': $(this).attr('colspan'), 'valign': $(this).attr('valign'), 'headers': $(this).attr('headers'), 'style': $(this).attr('style')}, html: $(this).html()}).css("vertical-align", $(this).attr('valign'));;
			});
			$('#a6b2a813-ac55-48a8-915c-0dce7be54b6e table:not(.dataTable) tbody tr').eq(0).wrap( "<thead></thead>" );
		
			$('#a6b2a813-ac55-48a8-915c-0dce7be54b6e table thead').prependTo('#a6b2a813-ac55-48a8-915c-0dce7be54b6e table');
			
			
