			$('#a6b2a813-ac55-48a8-915c-0dce7be54b6e table').removeAttr("cellspacing");
			
			$('#a6b2a813-ac55-48a8-915c-0dce7be54b6e table').DataTable({
				
				"columnDefs": [ { "type": "natural-nohtml", "targets": "_all" } ],
				
				"sDom":"lrtp",
				"sort":true,
				"pagingType": "simple_numbers","paging":false,
				"autoWidth":true
			});
		
			//#NOTE-REMOVE
			if(removeHeading){
				$('#a6b2a813-ac55-48a8-915c-0dce7be54b6e table thead').remove();
			}
			
			
		    
	    
	    
	} );
