			var removeHeading = false;
			
			$('#0a19075a-1c41-48f3-858c-daa54c98a00c table tbody tr').eq(0).find("td").replaceWith(function(){
				return $("<th>", {attr: {'width': $(this).attr('width'), 'rowspan': $(this).attr('rowspan'), 'colspan': $(this).attr('colspan'), 'valign': $(this).attr('valign'), 'headers': $(this).attr('headers'), 'style': $(this).attr('style')}, html: $(this).html()}).css("vertical-align", $(this).attr('valign'));;
			});
			$('#0a19075a-1c41-48f3-858c-daa54c98a00c table:not(.dataTable) tbody tr').eq(0).wrap( "<thead></thead>" );
		
			$('#0a19075a-1c41-48f3-858c-daa54c98a00c table thead').prependTo('#0a19075a-1c41-48f3-858c-daa54c98a00c table');
			
			
			$('#0a19075a-1c41-48f3-858c-daa54c98a00c table').removeAttr("border");
			$('#0a19075a-1c41-48f3-858c-daa54c98a00c table').removeAttr("cellpadding");
			$('#0a19075a-1c41-48f3-858c-daa54c98a00c table').removeAttr("cellspacing");
			
			$('#0a19075a-1c41-48f3-858c-daa54c98a00c table').DataTable({
				
				"sDom":"lrtp",
				"sort":false,
				"pagingType": "simple_numbers","paging":false,
				"autoWidth":true
			});
		
			//#NOTE-REMOVE
			if(removeHeading){
				$('#0a19075a-1c41-48f3-858c-daa54c98a00c table thead').remove();
			}
			
			
		    
	    
	    
	} );
	</script>
