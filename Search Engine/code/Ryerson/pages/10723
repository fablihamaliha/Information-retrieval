        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#9d860db1-ad21-4861-86b6-fc7bb8ff4be7 button").click(function() {
                	jQuery("#9d860db1-ad21-4861-86b6-fc7bb8ff4be7.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6597321, "lng": -79.37971449999999}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('9d860db1-ad21-4861-86b6-fc7bb8ff4be7',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('9d860db1-ad21-4861-86b6-fc7bb8ff4be7','44 Gerrard Street East','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('9d860db1-ad21-4861-86b6-fc7bb8ff4be7',latLng,'44 Gerrard Street East','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('9d860db1-ad21-4861-86b6-fc7bb8ff4be7','44 Gerrard Street East','1140px','600px',18,true);
                        }
                    }
                });
            });
        </script>
    
    
    
