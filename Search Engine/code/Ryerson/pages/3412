<div id="primary" class="">
    
         <div class="breadcrumbs"><a href="https://library.ryerson.ca">Library</a> &raquo; <a href="https://library.ryerson.ca/services/">Services</a> &raquo; <a href="https://library.ryerson.ca/services/disabilities/">Services for Persons with Disabilities</a> &raquo; Accessible Format Request Form</div>
        
		
		<h1>Accessible Format Request Form</h1>				
		
			
			<form method="POST" action="" enctype="multipart/form-data" name="access-form">
				<p><i>Note:</i> If you have other accessible format requests you
				will have to fill in this form for each one separately.</p><br />

				<p ><i>Mandatory fields marked with <em>*</em></i></p>

				<p>This service is only available to students who are registered with Academic Accommodation Support.<br />
				Are you registered with Academic Accommodation Support?</p>
				
				<input id="acm" name="acm" value="yes" type="checkbox" required="required" />
				<label for="acm"><em>*</em> Yes, I am registered with Academic Accommodation Support</label>
				<br /><br />

				<fieldset id="mainfields" style="display: none;">
					<legend>Remaining Required Information</legend>
					<label for="an"><em>*</em> <strong>Please enter the first and last name of your Facilitator:</strong></label><br />
					<input id="an" type="text" name="an"  size="50" required="required" /><br /><br />
					
					<label for="fn"><em>*</em> <strong>Your First Name:</strong></label><br />
					<input id="fn" type="text" name="fn"  size="50" required="required" /><br /><br />
					<label for="ln"><em>*</em> <strong>Your Last Name:</strong></label><br />
					<input id="ln" type="text" name="ln"  size="50" required="required" /><br /><br />
									
					<label for="em"><em>*</em> <strong>E-Mail Address:</strong></label><br />
					<input id="em" type="email" name="em" size="50"  required="required" /><br /><br />
					
					<label for="f"><em>*</em> <strong>Desired File Format:</strong></label><br />
					<select id="f" name="f" required="required">
						<option value="">-- Please select --</option>
						<option value="e-text_Word"  >e-text Word</option>
						<option value="e-text_PDF"  >e-text PDF</option>
						<option value="Braille_grade1"  >Braille grade 1</option>
						<option value="Braille_grade2"  >Braille grade 2</option>
					</select><br /><br />
					
					<label for="s"><strong>Which Software will you be using it with?:</strong></label><br />
					<input id="s" type="text" name="s" size="50"  /><br /><br />
					
					
						
						<div style="border: 1px solid lightgrey; padding: 1em;">
						
							<div style="float: left; margin-right: 2em;">
								<label for="cc1"><em>*</em> <strong>Course Code:</strong></label><br />
								<input id="cc1" type="text" name="cc1" size="20"  required="required" />
							</div>
							
							<label for="cs1"><em>*</em> <strong>Course Section:</strong></label><br />
							<input id="cs1" type="text" name="cs1" size="20"  required="required" /><br /><br />
							
							<label for="cn1"><em>*</em> <strong>Course instructor's name:</strong></label><br />
							<input id="cn1" type="text" name="cn1" size="50"  required="required" /><br /><br />
							
							<p>Do you know the specific title(s) of textbook(s) or reading(s) you need?<br /></p>
							<input id="rd1Y" name="rd1" value="yes" type="radio" />
							<label for="rd1Y">Yes, I have these details.</label>&nbsp;&nbsp;
							
							<input id="rd1N" name="rd1" value="no" type="radio" />
							<label for="rd1N">No, I do not have these details.<br /><br /></label>
							
							<div id="resourceDetails1" style="display: none;">
								
		
									<label for="t1_1"><em>*</em> <strong>Title:</strong></label><br />
									<input id="t1_1" type="text" name="t1_1" size="50"  /><br /><br />
					
									<label for="a1_1"><em>*</em> <strong>Author:</strong></label><br />
									<input id="a1_1" type="text" name="a1_1" size="50"  /><br /><br />
									
									<label for="i1_1"><strong>ISBN:</strong></label><br />
									<input id="i1_1" type="text" name="i1_1" size="49"  /><br /><br />
									
									<label for="y1_1"><em>*</em> <strong>Copyright or publication year (YYYY):</strong></label><br />
									<input id="y1_1" type="text" name="y1_1" size="50"  /><br /><br />
								
								<div id="rInsertPoint1"></div>
									
								<button type="button" name="nr1" id="nr1"><b>+</b> Add another reading for this course.</button><br /><br />
								<input type="hidden" name="rNum1" value="1" id="rNum1">
							</div>
						</div>
						<br />
					
					
					<div id="cInsertPoint"></div>
					
					<button type="button" name="newCourse" id="newCourse"><b>+</b> Add readings for a different course.</button><br /><br />
					<input type="hidden" name="courseNum" value="1" id="courseNum">
					
					<p>I acknowledge that I own a printed copy of the textbook, 
					and that I am registered in the class the textbook is to be 
					adapted	for use in.</p> 
					
					<p>I also acknowledge that I am aware that production of my 
					material may not begin until I bring my printed copy of
					the text or proof of purchase to the Library's circulation 
					desk, to the attention of Kraigen Brown.</p>
	
					<input type="checkbox" name="agree" value="yes" required="required" />
					<label for="agree"><em>*</em> <strong>You have read and agree to the above terms.</strong></label><br /> <br />
					
					Please prove you are human and answer the question below. <br />
					<label for="v"><em>*</em> <strong>If you write 3 directly next to 6 on a paper, what number did you write?</strong></label><br />
					<input id="v" type="text" name="v" size="10" required="required" /><br /><br />
					
					<input type="hidden" name="rnd" value="-70412767">
					
					
					
					<br />
					<input id="submit_form" type="submit" name="submit_form" value="Submit" />
				</fieldset>
			</form>
			
			<script type="text/javascript">
				$(document).ready(function() {
					var cCount = parseInt($('#courseNum').val());
					
					if ($("#acm").attr("checked")) {
						$("#mainfields").show(0);
					}
					$("#acm").change(function() {
						if ($("#acm").attr("checked")) {
							$("#mainfields").show(300);
						} else {
							$("#mainfields").hide(300);
						}
					});
					
					$("#newCourse").click(function() {
						cCount++;
						$("#cInsertPoint").append('<div style="border: 1px solid lightgrey; padding: 1em;">' +
							'<div style="float: left; margin-right: 2em;">' +
							'	<label for="cc'+cCount+'"><em>*</em> <strong>Course Code:</strong></label><br />' +
							'	<input id="cc'+cCount+'" type="text" name="cc'+cCount+'" size="20" required="required" />' +
							'</div>' +
							
							'<label for="cs'+cCount+'"><em>*</em> <strong>Course Section:</strong></label><br />'+
							'<input id="cs'+cCount+'" type="text" name="cs'+cCount+'" size="20" required="required" /><br /><br />'+
							
							'<label for="cn'+cCount+'"><em>*</em> <strong>Course instructor\'s name:</strong></label><br />'+
							'<input id="cn'+cCount+'" type="text" name="cn'+cCount+'" size="50" required="required" /><br /><br />'+
							
							'<p>Do you know the specific title(s) of textbook(s) or reading(s) you need?<br /></p>'+
							'<input id="rd'+cCount+'Y" name="rd'+cCount+'" value="yes" type="radio" />'+
							'<label for="rd'+cCount+'Y">Yes, I have these details.</label>&nbsp;&nbsp;'+
							
							'<input id="rd'+cCount+'N" name="rd'+cCount+'" value="no" type="radio" />'+
							'<label for="rd'+cCount+'N">No, I do not have these details.<br /><br /></label>'+
							
							'<div id="resourceDetails'+cCount+'" style="display: none;">'+
							'	<label for="t'+cCount+'_1"><em>*</em> <strong>Title:</strong></label><br />'+
							'	<input id="t'+cCount+'_1" type="text" name="t'+cCount+'_1" size="50" /><br /><br />'+
				
							'	<label for="a'+cCount+'_1"><em>*</em> <strong>Author:</strong></label><br />'+
							'	<input id="a'+cCount+'_1" type="text" name="a'+cCount+'_1" size="50" /><br /><br />'+
								
							'	<label for="i'+cCount+'_1"><strong>ISBN:</strong></label><br />'+
							'	<input id="i'+cCount+'_1" type="text" name="i'+cCount+'_1" size="49" /><br /><br />'+
								
							'	<label for="y'+cCount+'_1"><em>*</em> <strong>Copyright or publication year (YYYY):</strong></label><br />'+
							'	<input id="y'+cCount+'_1" type="text" name="y'+cCount+'_1" size="50" /><br /><br />'+
							
							'	<div id="rInsertPoint'+cCount+'"></div>'+
								
							'	<button type="button" name="nr'+cCount+'" id="nr'+cCount+'"><b>+</b> Add another reading for this course.</button><br /><br />'+
								
							'	<input type="hidden" name="rNum'+cCount+'" value="1" id="rNum'+cCount+'">'+
							'</div>'+
							'</div><br />');
						$("#courseNum").attr('value', cCount);
					});
					
					$("[type=button][name!=newCourse]").live("click", function() {
						var cNum = $(this).attr("id").substr(2);
						var rCount = parseInt($('#rNum' + cNum).val());
						
						rCount++;
						$("#rInsertPoint" + cNum).append(
							'<label for="t'+cNum+'_'+rCount+'"><em>*</em> <strong>Title:</strong></label><br />'+
							'<input id="t'+cNum+'_'+rCount+'" type="text" name="t'+cNum+'_'+rCount+'" size="50" /><br /><br />'+
				
							'<label for="a'+cNum+'_'+rCount+'"><em>*</em> <strong>Author:</strong></label><br />'+
							'<input id="a'+cNum+'_'+rCount+'" type="text" name="a'+cNum+'_'+rCount+'" size="50" /><br /><br />'+
								
							'<label for="i'+cNum+'_'+rCount+'"><strong>ISBN:</strong></label><br />'+
							'<input id="i'+cNum+'_'+rCount+'" type="text" name="i'+cNum+'_'+rCount+'" size="49" /><br /><br />'+
								
							'<label for="y'+cNum+'_'+rCount+'"><em>*</em> <strong>Copyright or publication year (YYYY):</strong></label><br />'+
							'<input id="y'+cNum+'_'+rCount+'" type="text" name="y'+cNum+'_'+rCount+'" size="50" /><br /><br />'
						);
						
						$("#rn" + cNum).attr('value', rCount);
					});
					
					$.each($('[type=radio][value="yes"]'), function(idx, itm) {
						if ($(this).attr("checked")) {
							var num = $(this).attr("name").substr(2);
							
							$("#resourceDetails" + num).show(0);
						}
					});
					
					$('[type=radio][value="yes"]').live("click", function() {
						var num = $(this).attr("name").substr(2);
						
						$("#resourceDetails" + num).show(300);
						$.each($("#resourceDetails" + num + " > input[type!=hidden][size='50']"), function(idx, itm) {
							$(itm).attr("required", "required");
						});
					});
					
					$('[type=radio][value="no"]').live("click", function() {
						var num = $(this).attr("name").substr(2);
						
						$("#resourceDetails" + num).hide(300);
						$.each($("#resourceDetails" + num + " > input[type!=hidden]"), function(idx, itm) {
							$(itm).removeAttr("required", "required");
						});
					});
				});
			</script>
			
</div>  


