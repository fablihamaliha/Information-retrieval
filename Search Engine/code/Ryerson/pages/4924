    <div id="toolbarObj397308724" class="allToolbarSelector"></div>
    
    
    
    
<script type="text/javascript"> 

function searchBio(){
    var bio_s = null;
    var bio_d = null;
    if(document.getElementById('bio_field_search') != null){
        bio_s = document.getElementById('bio_field_search').value;
    }
    
    if(document.getElementById('bio_field_department') != null){
        bio_d = document.getElementById('bio_field_department').value;
    }
    var redirectname = window.location.href;
    if(redirectname.indexOf("?") > 0){
        redirectname = redirectname.substring(0, redirectname.indexOf("?"));
    }
    if(bio_s!=null || bio_d != null){
        redirectname+="?";
    }
    if(bio_s != null){
        redirectname += "bios=" + bio_s;
    }
    if(bio_d != null && bio_s != null){
        redirectname += "&biod=" + bio_d;
    } else if (bio_d != null){
        redirectname += "biod=" + bio_d;
    }
    window.location = redirectname; 
} 
</script>

<div id="component397308724"  class="biographyListComponent"> 




    
