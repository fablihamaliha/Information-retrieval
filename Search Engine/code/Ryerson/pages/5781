	$(document).ready(function() {
		$('#cd06ef9b-f40f-4c76-b79c-8665c5c7f8ab table').addClass("table");
	    
			$('#cd06ef9b-f40f-4c76-b79c-8665c5c7f8ab table').addClass("table-striped");
		
		$('#cd06ef9b-f40f-4c76-b79c-8665c5c7f8ab table').addClass("table-borderless");
		
			var removeHeading = false;
			
			$('#cd06ef9b-f40f-4c76-b79c-8665c5c7f8ab table tbody tr').eq(0).find("td").replaceWith(function(){
				return $("<th>", {attr: {'width': $(this).attr('width'), 'rowspan': $(this).attr('rowspan'), 'colspan': $(this).attr('colspan'), 'valign': $(this).attr('valign'), 'headers': $(this).attr('headers'), 'style': $(this).attr('style')}, html: $(this).html()}).css("vertical-align", $(this).attr('valign'));;
			});
			$('#cd06ef9b-f40f-4c76-b79c-8665c5c7f8ab table:not(.dataTable) tbody tr').eq(0).wrap( "<thead></thead>" );
		
			$('#cd06ef9b-f40f-4c76-b79c-8665c5c7f8ab table thead').prependTo('#cd06ef9b-f40f-4c76-b79c-8665c5c7f8ab table');
			
			
			$('#cd06ef9b-f40f-4c76-b79c-8665c5c7f8ab table').removeAttr("border");
			$('#cd06ef9b-f40f-4c76-b79c-8665c5c7f8ab table').removeAttr("cellpadding");
			$('#cd06ef9b-f40f-4c76-b79c-8665c5c7f8ab table').removeAttr("cellspacing");
			
			$('#cd06ef9b-f40f-4c76-b79c-8665c5c7f8ab table').DataTable({
				
				"sDom":"lrtp",
				"sort":false,
				"pagingType": "simple_numbers","paging":false,
				"autoWidth":true
			});
		
			//#NOTE-REMOVE
			if(removeHeading){
				$('#cd06ef9b-f40f-4c76-b79c-8665c5c7f8ab table thead').remove();
			}
			
			
		    
	    
	    
	} );
	</script>
