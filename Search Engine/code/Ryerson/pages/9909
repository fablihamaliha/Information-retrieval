    $(document).ready(function(){
    	
        $("#651e1163-5f8d-4836-a965-d23510b749cf-play, #651e1163-5f8d-4836-a965-d23510b749cf-pause").click(function() {
            if ($(this).attr("id") == "651e1163-5f8d-4836-a965-d23510b749cf-play") {
                $('#651e1163-5f8d-4836-a965-d23510b749cf').carousel('cycle');
                $(this).attr("id", "651e1163-5f8d-4836-a965-d23510b749cf-pause");
