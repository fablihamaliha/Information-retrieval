        <script>
        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#5ebb2db2-400d-4d94-ac5e-65982404e6f8 button").click(function() {
                	jQuery("#5ebb2db2-400d-4d94-ac5e-65982404e6f8.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6600414, "lng": -79.3771506}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('5ebb2db2-400d-4d94-ac5e-65982404e6f8',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('5ebb2db2-400d-4d94-ac5e-65982404e6f8','99 Gerrard St E, Toronto, ON, M5B 2M2, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('5ebb2db2-400d-4d94-ac5e-65982404e6f8',latLng,'99 Gerrard St E, Toronto, ON, M5B 2M2, Canada','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('5ebb2db2-400d-4d94-ac5e-65982404e6f8','99 Gerrard St E, Toronto, ON, M5B 2M2, Canada','1140px','600px',18,true);
                        }
                    }
                });
            });
        </script>
    
    
    
