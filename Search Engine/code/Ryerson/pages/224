				<div class="resMap section">

























<div class="res-map">
    
        <div id="3b8c5817-9993-460b-a1bd-7d8980fbc8aa" class="res-map-content res-map-placeholder" style="height: 300px;" aria-hidden="true">
        	<div class="res-map-overlay res-map-600px">
	        	<img src="/etc.clientlibs/ryecms/static/clientlib-site/resources/images/map-marker.png" alt=""/>
	        	<button>Click to Activate Map</button>
        	</div>
        </div>
        <div class="sr-only">Map of 1 Dundas Street West, Toronto, ON, M5G 2L5, Canada</div>
        <script>
        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#3b8c5817-9993-460b-a1bd-7d8980fbc8aa button").click(function() {
                	jQuery("#3b8c5817-9993-460b-a1bd-7d8980fbc8aa.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6558668, "lng": -79.3819145}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('3b8c5817-9993-460b-a1bd-7d8980fbc8aa',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('3b8c5817-9993-460b-a1bd-7d8980fbc8aa','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('3b8c5817-9993-460b-a1bd-7d8980fbc8aa',latLng,'1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','1140px','600px',18,true);
                        } else{
