<script type="text/javascript">
jQuery(document).ready(function() {
    jQuery("#component521047877 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component521047877 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component521047877 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component521047877 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component521047877 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
 </script>
 
