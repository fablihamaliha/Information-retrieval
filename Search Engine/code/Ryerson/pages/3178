        <script>
        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#88958710-0bf4-48b5-9440-1b15664c0809 button").click(function() {
                	jQuery("#88958710-0bf4-48b5-9440-1b15664c0809.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.657564, "lng": -79.3772027}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('88958710-0bf4-48b5-9440-1b15664c0809',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('88958710-0bf4-48b5-9440-1b15664c0809','245 Church Street, Toronto, ON, M5B 1Z2, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('88958710-0bf4-48b5-9440-1b15664c0809',latLng,'245 Church Street, Toronto, ON, M5B 1Z2, Canada','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('88958710-0bf4-48b5-9440-1b15664c0809','245 Church Street, Toronto, ON, M5B 1Z2, Canada','1140px','600px',18,true);
                        }
                    }
                });
            });
        </script>
    
    
    
