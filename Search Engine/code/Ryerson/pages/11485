				<div class="resAccordion section">
























 


<div class="row">
    <div class="col-xs-12">
        <div class="panel-group accordion background-hidden" id="585b6c01-2081-482f-88b3-01dd6583c26a" role="tablist" aria-multiselectable="true">
			
				<div class="resAccordionControl" role="group" aria-label="Accordion Actions">
					<button class="resAccordionOpenAll">Open All</button> <button class="resAccordionCloseAll">Close All</button>
				</div>
				<script type="text/javascript">$(document).ready(function(){ initResAccordionControl("585b6c01-2081-482f-88b3-01dd6583c26a", true); });</script>
			
            
            <div class="panel panel-default">
                <!-- Accordion Heading -->
                <div class="panel-heading container-clipboard">
                    <h2 class="panel-title">
                        <a id="accordion-1596040548486-creating-polls-through-the-zoom-website-" role="tab" data-toggle="collapse" data-parent="#585b6c01-2081-482f-88b3-01dd6583c26a" href="#accordion-content-1596040548486-creating-polls-through-the-zoom-website-" aria-controls="accordion-content-1596040548486-creating-polls-through-the-zoom-website-" aria-expanded="false">
                            Creating Polls through the Zoom Website 
                        </a>
                    </h2>
                    
                </div>
