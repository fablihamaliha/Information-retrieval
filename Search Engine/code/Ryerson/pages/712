<script>
    $(document).ready(function(){
    	
        $("#b8b4a620-e60c-457b-99e3-f719e6156370-play, #b8b4a620-e60c-457b-99e3-f719e6156370-pause").click(function() {
            if ($(this).attr("id") == "b8b4a620-e60c-457b-99e3-f719e6156370-play") {
                $('#b8b4a620-e60c-457b-99e3-f719e6156370').carousel('cycle');
                $(this).attr("id", "b8b4a620-e60c-457b-99e3-f719e6156370-pause");
                $("#b8b4a620-e60c-457b-99e3-f719e6156370-pause i").attr("class", "fa fa-pause");
            } else {
                $('#b8b4a620-e60c-457b-99e3-f719e6156370').carousel('pause');
                $(this).attr("id", "b8b4a620-e60c-457b-99e3-f719e6156370-play");
                $("#b8b4a620-e60c-457b-99e3-f719e6156370-play i").attr("class", "fa fa-play");
            }
        });
        
