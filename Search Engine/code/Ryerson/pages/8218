        <script>
        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#3a9ae119-d94f-4d34-bdfe-0d595faea975 button").click(function() {
                	jQuery("#3a9ae119-d94f-4d34-bdfe-0d595faea975.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6577631, "lng": -79.38116409999999}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('3a9ae119-d94f-4d34-bdfe-0d595faea975',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('3a9ae119-d94f-4d34-bdfe-0d595faea975','345 Yonge Street, Toronto, ON, M5B 2K3','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('3a9ae119-d94f-4d34-bdfe-0d595faea975',latLng,'345 Yonge Street, Toronto, ON, M5B 2K3','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('3a9ae119-d94f-4d34-bdfe-0d595faea975','345 Yonge Street, Toronto, ON, M5B 2K3','1140px','600px',18,true);
                        }
                    }
                });
            });
        </script>
    
    
    
