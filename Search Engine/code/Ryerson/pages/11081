	<script>
	
	//convert to plugin in pure js....later (soon)
	$(document).ready(function() {
		$('#e84379e1-5d9d-4905-8be9-8830d79345c3 table').addClass("table");
	    
		$('#e84379e1-5d9d-4905-8be9-8830d79345c3 table').addClass("table-bordered");
		
