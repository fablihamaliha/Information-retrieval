<div class="res-text richtextContent background-hidden" id="a0e3cc6b-bec5-42ff-9b50-e597e38175b7" >

    
    





</div>
<script>$(document).ready(function(){addCoursePopover("#a0e3cc6b-bec5-42ff-9b50-e597e38175b7");});</script></div>

				

				
				<div class="resAccordion section">
























 


<div class="row">
    <div class="col-xs-12">
        <div class="panel-group accordion background-opaque" id="e14f6952-4c33-420b-958f-70b952f91852" role="tablist" aria-multiselectable="true">
			
				<div class="resAccordionControl" role="group" aria-label="Accordion Actions">
