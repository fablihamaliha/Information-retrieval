                jQuery("#a8444ee2-f669-4b68-8693-cc23272b0cdc button").click(function() {
                	jQuery("#a8444ee2-f669-4b68-8693-cc23272b0cdc.res-map-placeholder").removeClass('res-map-placeholder');
