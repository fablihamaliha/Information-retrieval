                        if(latLng != null){
                            drawDynamicMapByLatLng('a8444ee2-f669-4b68-8693-cc23272b0cdc',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('a8444ee2-f669-4b68-8693-cc23272b0cdc','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','600px',18,true);
                        }
