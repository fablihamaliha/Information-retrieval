<script>
function submitFormAR(){
var d = document.questionForm;
var irurl = 'https://ask.ryerson.ca/index.jsp?interfaceID='+d.interfaceID.value+'&requestType=NormalRequest&question='+d.question.value; 
  var newIRWindow = window.open(irurl,'IRWindow');
  return false;
}
