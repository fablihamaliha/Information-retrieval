    $(document).ready(function(){
    	
        $("#7b69edab-3d80-4d24-bc3e-2e367fb6d7c4-play, #7b69edab-3d80-4d24-bc3e-2e367fb6d7c4-pause").click(function() {
            if ($(this).attr("id") == "7b69edab-3d80-4d24-bc3e-2e367fb6d7c4-play") {
                $('#7b69edab-3d80-4d24-bc3e-2e367fb6d7c4').carousel('cycle');
                $(this).attr("id", "7b69edab-3d80-4d24-bc3e-2e367fb6d7c4-pause");
                $("#7b69edab-3d80-4d24-bc3e-2e367fb6d7c4-pause i").attr("class", "fa fa-pause");
