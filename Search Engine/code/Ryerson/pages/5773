                $('#7b69edab-3d80-4d24-bc3e-2e367fb6d7c4').carousel('pause');
                $(this).attr("id", "7b69edab-3d80-4d24-bc3e-2e367fb6d7c4-play");
                $("#7b69edab-3d80-4d24-bc3e-2e367fb6d7c4-play i").attr("class", "fa fa-play");
            }
        });
        
