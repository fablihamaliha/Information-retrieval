<script>
    $(document).ready(function(){
    	
        $("#4d1a5169-b3c1-47bb-ab05-fe03b1f2e86f-play, #4d1a5169-b3c1-47bb-ab05-fe03b1f2e86f-pause").click(function() {
            if ($(this).attr("id") == "4d1a5169-b3c1-47bb-ab05-fe03b1f2e86f-play") {
                $('#4d1a5169-b3c1-47bb-ab05-fe03b1f2e86f').carousel('cycle');
                $(this).attr("id", "4d1a5169-b3c1-47bb-ab05-fe03b1f2e86f-pause");
                $("#4d1a5169-b3c1-47bb-ab05-fe03b1f2e86f-pause i").attr("class", "fa fa-pause");
            } else {
                $('#4d1a5169-b3c1-47bb-ab05-fe03b1f2e86f').carousel('pause');
                $(this).attr("id", "4d1a5169-b3c1-47bb-ab05-fe03b1f2e86f-play");
                $("#4d1a5169-b3c1-47bb-ab05-fe03b1f2e86f-play i").attr("class", "fa fa-play");
            }
        });
        
        		$('#4d1a5169-b3c1-47bb-ab05-fe03b1f2e86f').carousel('cycle');
        	
    });
</script>
