    $(document).ready(function(){
    	
        $("#a2c38b56-f47c-4610-8b26-38a6bbc7aa2b-play, #a2c38b56-f47c-4610-8b26-38a6bbc7aa2b-pause").click(function() {
            if ($(this).attr("id") == "a2c38b56-f47c-4610-8b26-38a6bbc7aa2b-play") {
                $('#a2c38b56-f47c-4610-8b26-38a6bbc7aa2b').carousel('cycle');
                $(this).attr("id", "a2c38b56-f47c-4610-8b26-38a6bbc7aa2b-pause");
                $("#a2c38b56-f47c-4610-8b26-38a6bbc7aa2b-pause i").attr("class", "fa fa-pause");
            } else {
                $('#a2c38b56-f47c-4610-8b26-38a6bbc7aa2b').carousel('pause');
                $(this).attr("id", "a2c38b56-f47c-4610-8b26-38a6bbc7aa2b-play");
                $("#a2c38b56-f47c-4610-8b26-38a6bbc7aa2b-play i").attr("class", "fa fa-play");
            }
        });
        
