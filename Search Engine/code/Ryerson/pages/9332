jQuery(document).ready(function() {
    jQuery("#component241498865 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component241498865 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component241498865 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component241498865 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component241498865 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
