<script>
    $(document).ready(function(){
    	
        $("#ac04afd4-c3b9-4487-b318-6ca7fcfcef61-play, #ac04afd4-c3b9-4487-b318-6ca7fcfcef61-pause").click(function() {
            if ($(this).attr("id") == "ac04afd4-c3b9-4487-b318-6ca7fcfcef61-play") {
                $('#ac04afd4-c3b9-4487-b318-6ca7fcfcef61').carousel('cycle');
                $(this).attr("id", "ac04afd4-c3b9-4487-b318-6ca7fcfcef61-pause");
                $("#ac04afd4-c3b9-4487-b318-6ca7fcfcef61-pause i").attr("class", "fa fa-pause");
            } else {
                $('#ac04afd4-c3b9-4487-b318-6ca7fcfcef61').carousel('pause');
                $(this).attr("id", "ac04afd4-c3b9-4487-b318-6ca7fcfcef61-play");
                $("#ac04afd4-c3b9-4487-b318-6ca7fcfcef61-play i").attr("class", "fa fa-play");
            }
        });
        
        		$('#ac04afd4-c3b9-4487-b318-6ca7fcfcef61').carousel('cycle');
        	
    });
</script>
