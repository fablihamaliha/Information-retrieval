        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#9df81421-0453-4396-b127-10f0a70e7155 button").click(function() {
                	jQuery("#9df81421-0453-4396-b127-10f0a70e7155.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6558668, "lng": -79.3819145}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('9df81421-0453-4396-b127-10f0a70e7155',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('9df81421-0453-4396-b127-10f0a70e7155','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('9df81421-0453-4396-b127-10f0a70e7155',latLng,'1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('9df81421-0453-4396-b127-10f0a70e7155','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','1140px','600px',18,true);
                        }
                    }
                });
            });
