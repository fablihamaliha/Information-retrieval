        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#86e9bf31-f611-46ff-9278-74ca93915c0e button").click(function() {
                	jQuery("#86e9bf31-f611-46ff-9278-74ca93915c0e.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = false;
                    var latLng = JSON.parse('{"lat": 43.6586299, "lng": -79.3770727}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('86e9bf31-f611-46ff-9278-74ca93915c0e',latLng,'300px',18,true);
                        } else{
                            drawDynamicMapByAddress('86e9bf31-f611-46ff-9278-74ca93915c0e','80 Gould Street, Toronto, ON, M5B 2K3, Canada','300px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('86e9bf31-f611-46ff-9278-74ca93915c0e',latLng,'80 Gould Street, Toronto, ON, M5B 2K3, Canada','1140px','300px',18,true);
                        } else{
                            drawStaticMapByAddress('86e9bf31-f611-46ff-9278-74ca93915c0e','80 Gould Street, Toronto, ON, M5B 2K3, Canada','1140px','300px',18,true);
                        }
                    }
                });
            });
