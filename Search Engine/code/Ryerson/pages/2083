<script type="text/javascript">
jQuery(document).ready(function() {
    jQuery("#component997315910 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component997315910 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component997315910 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component997315910 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component997315910 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
 </script>
 
