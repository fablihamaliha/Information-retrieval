    $(document).ready(function(){
    	
        $("#72b17cfa-0013-4278-b649-ee07fc755927-play, #72b17cfa-0013-4278-b649-ee07fc755927-pause").click(function() {
            if ($(this).attr("id") == "72b17cfa-0013-4278-b649-ee07fc755927-play") {
                $('#72b17cfa-0013-4278-b649-ee07fc755927').carousel('cycle');
                $(this).attr("id", "72b17cfa-0013-4278-b649-ee07fc755927-pause");
