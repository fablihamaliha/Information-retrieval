                        if(latLng != null){
                            drawDynamicMapByLatLng('cee53a36-8106-4521-8032-4bdbf69adbc9',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('cee53a36-8106-4521-8032-4bdbf69adbc9','44 Gerrard Street East','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('cee53a36-8106-4521-8032-4bdbf69adbc9',latLng,'44 Gerrard Street East','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('cee53a36-8106-4521-8032-4bdbf69adbc9','44 Gerrard Street East','1140px','600px',18,true);
                        }
                    }
                });
