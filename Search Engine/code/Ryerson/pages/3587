                $('#72b17cfa-0013-4278-b649-ee07fc755927').carousel('pause');
                $(this).attr("id", "72b17cfa-0013-4278-b649-ee07fc755927-play");
                $("#72b17cfa-0013-4278-b649-ee07fc755927-play i").attr("class", "fa fa-play");
