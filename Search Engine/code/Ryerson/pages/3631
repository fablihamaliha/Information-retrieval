        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#9467e0b8-e793-4ec6-991d-ecf166e45dd2 button").click(function() {
                	jQuery("#9467e0b8-e793-4ec6-991d-ecf166e45dd2.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6586299, "lng": -79.3770727}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('9467e0b8-e793-4ec6-991d-ecf166e45dd2',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('9467e0b8-e793-4ec6-991d-ecf166e45dd2','80 Gould St, Toronto, ON M5B 2M7','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('9467e0b8-e793-4ec6-991d-ecf166e45dd2',latLng,'80 Gould St, Toronto, ON M5B 2M7','1140px','600px',18,true);
