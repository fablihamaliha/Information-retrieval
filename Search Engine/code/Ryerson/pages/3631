                $('#9570fe47-74a5-4429-880e-567369268173').carousel('cycle');
                $(this).attr("id", "9570fe47-74a5-4429-880e-567369268173-pause");
                $("#9570fe47-74a5-4429-880e-567369268173-pause i").attr("class", "fa fa-pause");
            } else {
                $('#9570fe47-74a5-4429-880e-567369268173').carousel('pause');
                $(this).attr("id", "9570fe47-74a5-4429-880e-567369268173-play");
                $("#9570fe47-74a5-4429-880e-567369268173-play i").attr("class", "fa fa-play");
