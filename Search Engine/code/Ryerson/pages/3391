        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#faed3efb-ef75-43d4-ab10-662fc5b62884 button").click(function() {
                	jQuery("#faed3efb-ef75-43d4-ab10-662fc5b62884.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6597321, "lng": -79.37971449999999}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('faed3efb-ef75-43d4-ab10-662fc5b62884',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('faed3efb-ef75-43d4-ab10-662fc5b62884','44 Gerrard Street East','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('faed3efb-ef75-43d4-ab10-662fc5b62884',latLng,'44 Gerrard Street East','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('faed3efb-ef75-43d4-ab10-662fc5b62884','44 Gerrard Street East','1140px','600px',18,true);
                        }
                    }
                });
            });
        </script>
    
    
    
