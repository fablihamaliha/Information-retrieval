                jQuery("#05085d8a-a8cc-42a9-b1c7-c5fd9149342e button").click(function() {
                	jQuery("#05085d8a-a8cc-42a9-b1c7-c5fd9149342e.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6558668, "lng": -79.3819145}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('05085d8a-a8cc-42a9-b1c7-c5fd9149342e',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('05085d8a-a8cc-42a9-b1c7-c5fd9149342e','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('05085d8a-a8cc-42a9-b1c7-c5fd9149342e',latLng,'1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','1140px','600px',18,true);
                        } else{
