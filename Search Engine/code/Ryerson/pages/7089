  <script type="text/javascript">
  var jQuery = jQuery.noConflict(); 
  jQuery(function() {
    
             jQuery('img.image0').data('ad-title', "<a target='_blank' >SWISS flies first passenger flight with new Bombardier jetliner</a>");                              
             
             jQuery('img.image1').data('ad-title', "<a target='_blank' >OAC Industrial Research Assistance Program</a>");                              
             
             jQuery('img.image2').data('ad-title', "<a target='_blank' >Canada – Student Unmanned Aircraft Systems Competition</a>");                              
             
    
    var galleries = jQuery('.ad-gallery').adGallery({
        effect: 'fade',width:false, height: false, slideshow: {animate_first_image: false, speed: 7000}
    });
    galleries[0].settings.effect = 'fade'; 
   
    jQuery('#toggle-slideshow').click(
      function() {
        galleries[0].slideshow.toggle();
        return false;
      }
    );
    jQuery('#toggle-description').click(
      function() {
        if(!galleries[0].settings.description_wrapper) {
          galleries[0].settings.description_wrapper = jQuery('#descriptions');
        } else {
          galleries[0].settings.description_wrapper = false;
        }
        return false;
      }
    );
  });
  </script>

