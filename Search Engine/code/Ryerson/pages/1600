    $(document).ready(function(){
    	
        $("#75b1b9ff-e02f-4dda-b011-78f3228ae895-play, #75b1b9ff-e02f-4dda-b011-78f3228ae895-pause").click(function() {
            if ($(this).attr("id") == "75b1b9ff-e02f-4dda-b011-78f3228ae895-play") {
                $('#75b1b9ff-e02f-4dda-b011-78f3228ae895').carousel('cycle');
                $(this).attr("id", "75b1b9ff-e02f-4dda-b011-78f3228ae895-pause");
                $("#75b1b9ff-e02f-4dda-b011-78f3228ae895-pause i").attr("class", "fa fa-pause");
            } else {
                $('#75b1b9ff-e02f-4dda-b011-78f3228ae895').carousel('pause');
                $(this).attr("id", "75b1b9ff-e02f-4dda-b011-78f3228ae895-play");
                $("#75b1b9ff-e02f-4dda-b011-78f3228ae895-play i").attr("class", "fa fa-play");
            }
        });
        
