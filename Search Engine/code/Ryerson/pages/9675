	$(document).ready(function() {
		$('#06cc30e6-51d7-4e8c-90ba-8f2ce37c51f7 table').addClass("table");
	    
		$('#06cc30e6-51d7-4e8c-90ba-8f2ce37c51f7 table').addClass("table-bordered");
		
			var removeHeading = false;
			
			$('#06cc30e6-51d7-4e8c-90ba-8f2ce37c51f7 table tbody tr').eq(0).find("td").replaceWith(function(){
				return $("<th>", {attr: {'width': $(this).attr('width'), 'rowspan': $(this).attr('rowspan'), 'colspan': $(this).attr('colspan'), 'valign': $(this).attr('valign'), 'headers': $(this).attr('headers'), 'style': $(this).attr('style')}, html: $(this).html()}).css("vertical-align", $(this).attr('valign'));;
			});
			$('#06cc30e6-51d7-4e8c-90ba-8f2ce37c51f7 table:not(.dataTable) tbody tr').eq(0).wrap( "<thead></thead>" );
		
			$('#06cc30e6-51d7-4e8c-90ba-8f2ce37c51f7 table thead').prependTo('#06cc30e6-51d7-4e8c-90ba-8f2ce37c51f7 table');
			
			
			$('#06cc30e6-51d7-4e8c-90ba-8f2ce37c51f7 table').removeAttr("border");
			$('#06cc30e6-51d7-4e8c-90ba-8f2ce37c51f7 table').removeAttr("cellpadding");
			$('#06cc30e6-51d7-4e8c-90ba-8f2ce37c51f7 table').removeAttr("cellspacing");
			
			$('#06cc30e6-51d7-4e8c-90ba-8f2ce37c51f7 table').DataTable({
				
				"sDom":"lrtp",
				"sort":false,
				"pagingType": "simple_numbers","paging":false,
				"autoWidth":true
			});
		
			//#NOTE-REMOVE
			if(removeHeading){
				$('#06cc30e6-51d7-4e8c-90ba-8f2ce37c51f7 table thead').remove();
			}
			
			
		    
	    
	    
	} );
	</script>
