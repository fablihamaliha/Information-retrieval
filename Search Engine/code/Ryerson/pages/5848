jQuery(document).ready(function() {
    jQuery("#component961452718 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component961452718 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component961452718 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component961452718 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component961452718 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
 </script>
 
