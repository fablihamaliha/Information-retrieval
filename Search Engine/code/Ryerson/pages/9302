<script>
    $(document).ready(function(){
    	
        $("#58f568d1-17d5-459f-b9c5-ad7e57df22f9-play, #58f568d1-17d5-459f-b9c5-ad7e57df22f9-pause").click(function() {
            if ($(this).attr("id") == "58f568d1-17d5-459f-b9c5-ad7e57df22f9-play") {
                $('#58f568d1-17d5-459f-b9c5-ad7e57df22f9').carousel('cycle');
                $(this).attr("id", "58f568d1-17d5-459f-b9c5-ad7e57df22f9-pause");
                $("#58f568d1-17d5-459f-b9c5-ad7e57df22f9-pause i").attr("class", "fa fa-pause");
            } else {
                $('#58f568d1-17d5-459f-b9c5-ad7e57df22f9').carousel('pause');
                $(this).attr("id", "58f568d1-17d5-459f-b9c5-ad7e57df22f9-play");
                $("#58f568d1-17d5-459f-b9c5-ad7e57df22f9-play i").attr("class", "fa fa-play");
            }
        });
        
