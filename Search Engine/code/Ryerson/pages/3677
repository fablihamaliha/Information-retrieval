            } else {
                $('#8545a86c-da27-4975-b634-a8c13597fe84').carousel('pause');
                $(this).attr("id", "8545a86c-da27-4975-b634-a8c13597fe84-play");
                $("#8545a86c-da27-4975-b634-a8c13597fe84-play i").attr("class", "fa fa-play");
            }
        });
        
