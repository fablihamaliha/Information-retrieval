                $('#7982b70a-6626-439a-ab1f-696293ef76ab').carousel('pause');
                $(this).attr("id", "7982b70a-6626-439a-ab1f-696293ef76ab-play");
                $("#7982b70a-6626-439a-ab1f-696293ef76ab-play i").attr("class", "fa fa-play");
