  var jQuery = jQuery.noConflict(); 
  jQuery(function() {
    
             jQuery('img.image0').data('ad-title', "<a  href='/content/ryerson/fcs-news-events/news/2020/06/2020-dennis-mock-leadership-awards'>2020 Dennis Mock leadership award nominees</a>");                              
             
