        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#4e9e138f-7a2b-420b-ad76-7c7dc2d33f11 button").click(function() {
                	jQuery("#4e9e138f-7a2b-420b-ad76-7c7dc2d33f11.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6586299, "lng": -79.3770727}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('4e9e138f-7a2b-420b-ad76-7c7dc2d33f11',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('4e9e138f-7a2b-420b-ad76-7c7dc2d33f11','80 Gould St, Toronto, ON M5B 2M7','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('4e9e138f-7a2b-420b-ad76-7c7dc2d33f11',latLng,'80 Gould St, Toronto, ON M5B 2M7','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('4e9e138f-7a2b-420b-ad76-7c7dc2d33f11','80 Gould St, Toronto, ON M5B 2M7','1140px','600px',18,true);
                        }
                    }
                });
            });
        </script>
    
    
    
