        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#dc7d1587-3948-44ed-8854-9ea765e31704 button").click(function() {
                	jQuery("#dc7d1587-3948-44ed-8854-9ea765e31704.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6570679, "lng": -79.380062}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('dc7d1587-3948-44ed-8854-9ea765e31704',latLng,'600px',15,false);
                        } else{
                            drawDynamicMapByAddress('dc7d1587-3948-44ed-8854-9ea765e31704','285 Victoria Street, Toronto, ON, M5B 2K3, Canada','600px',15,false);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('dc7d1587-3948-44ed-8854-9ea765e31704',latLng,'285 Victoria Street, Toronto, ON, M5B 2K3, Canada','1140px','600px',15,false);
                        } else{
                            drawStaticMapByAddress('dc7d1587-3948-44ed-8854-9ea765e31704','285 Victoria Street, Toronto, ON, M5B 2K3, Canada','1140px','600px',15,false);
                        }
                    }
                });
            });
        </script>
    
    
    
