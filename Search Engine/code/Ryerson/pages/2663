jQuery(document).ready(function() {
    jQuery("#component359383357 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component359383357 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component359383357 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component359383357 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component359383357 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
 </script>
 
