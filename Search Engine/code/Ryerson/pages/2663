jQuery(document).ready(function() {
    jQuery("#component831142233 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component831142233 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component831142233 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component831142233 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component831142233 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
 </script>
 
