<script>
$("#rye-bce74fbb-0205-4130-b658-f471cc1604b0 input, #rye-bce74fbb-0205-4130-b658-f471cc1604b0 select").each(function(i, v) {
	if (!$(this).hasClass('form-control')) {
		if ($(this).is("input[type='file']")) {
			$(this).attr('id', 'rye-bce74fbb-0205-4130-b658-f471cc1604b0_upload'+i).wrap("<span class='htmlSnippetForm'></span>").after("<label for='rye-bce74fbb-0205-4130-b658-f471cc1604b0_upload"+i+"' class='customInput'>Choose file</label>");
		} else {
			$(this).wrap("<span class='htmlSnippetForm'></span>").after("<span class='customInput'></span>");
		}
	}
});
</script>

