        <script>
        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#cbbdaf7b-1d5f-4d23-8304-78a941a44d9d button").click(function() {
                	jQuery("#cbbdaf7b-1d5f-4d23-8304-78a941a44d9d.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6600414, "lng": -79.3771506}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('cbbdaf7b-1d5f-4d23-8304-78a941a44d9d',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('cbbdaf7b-1d5f-4d23-8304-78a941a44d9d','99 Gerrard St East, Toronto, ON, M5B 1G7, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('cbbdaf7b-1d5f-4d23-8304-78a941a44d9d',latLng,'99 Gerrard St East, Toronto, ON, M5B 1G7, Canada','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('cbbdaf7b-1d5f-4d23-8304-78a941a44d9d','99 Gerrard St East, Toronto, ON, M5B 1G7, Canada','1140px','600px',18,true);
                        }
                    }
                });
            });
        </script>
    
    
    
