<script>
    $(document).ready(function(){
    	
        $("#05df01ba-b665-4937-bc6e-6ecd1d321f7d-play, #05df01ba-b665-4937-bc6e-6ecd1d321f7d-pause").click(function() {
            if ($(this).attr("id") == "05df01ba-b665-4937-bc6e-6ecd1d321f7d-play") {
                $('#05df01ba-b665-4937-bc6e-6ecd1d321f7d').carousel('cycle');
                $(this).attr("id", "05df01ba-b665-4937-bc6e-6ecd1d321f7d-pause");
                $("#05df01ba-b665-4937-bc6e-6ecd1d321f7d-pause i").attr("class", "fa fa-pause");
            } else {
                $('#05df01ba-b665-4937-bc6e-6ecd1d321f7d').carousel('pause');
                $(this).attr("id", "05df01ba-b665-4937-bc6e-6ecd1d321f7d-play");
                $("#05df01ba-b665-4937-bc6e-6ecd1d321f7d-play i").attr("class", "fa fa-play");
            }
        });
        
