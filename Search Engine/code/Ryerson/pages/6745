<div class="row">
    <div class="col-xs-12">
        <div class="panel-group accordion background-hidden" id="98ac99f6-3b11-4422-aaa8-9e549833fc0b" role="tablist" aria-multiselectable="true">
			
            
            <div class="panel panel-default">
                <!-- Accordion Heading -->
                <div class="panel-heading container-clipboard">
                    <h3 class="panel-title">
                        <a id="accordion-1517854762323-google-chrome" role="tab" data-toggle="collapse" data-parent="#98ac99f6-3b11-4422-aaa8-9e549833fc0b" href="#accordion-content-1517854762323-google-chrome" aria-controls="accordion-content-1517854762323-google-chrome" aria-expanded="false">
                            Google Chrome
                        </a>
                    </h3>
                    
                </div>
                <!-- Accordion Body -->
                <div tabindex="0" id="accordion-content-1517854762323-google-chrome" class="panel-collapse collapse" role="tabpanel" aria-labelledby="accordion-1517854762323-google-chrome">
