                        if(latLng != null){
                            drawDynamicMapByLatLng('c9ea7e68-acd8-424f-b17f-5f12ca1486cb',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('c9ea7e68-acd8-424f-b17f-5f12ca1486cb','1 Dundas Street West, Toronto, ON m5b 2k3','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
