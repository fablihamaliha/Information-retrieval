        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#c9ea7e68-acd8-424f-b17f-5f12ca1486cb button").click(function() {
                	jQuery("#c9ea7e68-acd8-424f-b17f-5f12ca1486cb.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6559513, "lng": -79.381742}');
