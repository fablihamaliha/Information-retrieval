			$(this).attr('id', 'rye-3e0d755b-0ee6-4e06-aa0f-e1107e8a949c_upload'+i).wrap("<span class='htmlSnippetForm'></span>").after("<label for='rye-3e0d755b-0ee6-4e06-aa0f-e1107e8a949c_upload"+i+"' class='customInput'>Choose file</label>");
		} else {
			$(this).wrap("<span class='htmlSnippetForm'></span>").after("<span class='customInput'></span>");
		}
	}
});
</script>

