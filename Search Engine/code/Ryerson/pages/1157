        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#dc21d177-9f67-4021-9ee3-ba797b0357e1 button").click(function() {
                	jQuery("#dc21d177-9f67-4021-9ee3-ba797b0357e1.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6558668, "lng": -79.3819145}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('dc21d177-9f67-4021-9ee3-ba797b0357e1',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('dc21d177-9f67-4021-9ee3-ba797b0357e1','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('dc21d177-9f67-4021-9ee3-ba797b0357e1',latLng,'1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','1140px','600px',18,true);
