        <script>
        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#14b8d5ff-4aa0-4b03-8795-58c93e717f1e button").click(function() {
                	jQuery("#14b8d5ff-4aa0-4b03-8795-58c93e717f1e.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6558668, "lng": -79.3819145}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('14b8d5ff-4aa0-4b03-8795-58c93e717f1e',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('14b8d5ff-4aa0-4b03-8795-58c93e717f1e','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('14b8d5ff-4aa0-4b03-8795-58c93e717f1e',latLng,'1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','1140px','600px',18,true);
                        } else{
