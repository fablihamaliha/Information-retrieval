$("#rye-cc4eb062-ce0e-417f-a353-ad7f584eeb31 input, #rye-cc4eb062-ce0e-417f-a353-ad7f584eeb31 select").each(function(i, v) {
	if (!$(this).hasClass('form-control')) {
		if ($(this).is("input[type='file']")) {
			$(this).attr('id', 'rye-cc4eb062-ce0e-417f-a353-ad7f584eeb31_upload'+i).wrap("<span class='htmlSnippetForm'></span>").after("<label for='rye-cc4eb062-ce0e-417f-a353-ad7f584eeb31_upload"+i+"' class='customInput'>Choose file</label>");
		} else {
			$(this).wrap("<span class='htmlSnippetForm'></span>").after("<span class='customInput'></span>");
		}
	}
});
</script>

