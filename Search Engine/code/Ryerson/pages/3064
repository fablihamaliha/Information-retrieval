<script>
$("#rye-3d7cbdd7-ec8d-43f3-847e-07bb6b33ae12 input, #rye-3d7cbdd7-ec8d-43f3-847e-07bb6b33ae12 select").each(function(i, v) {
	if (!$(this).hasClass('form-control')) {
		if ($(this).is("input[type='file']")) {
			$(this).attr('id', 'rye-3d7cbdd7-ec8d-43f3-847e-07bb6b33ae12_upload'+i).wrap("<span class='htmlSnippetForm'></span>").after("<label for='rye-3d7cbdd7-ec8d-43f3-847e-07bb6b33ae12_upload"+i+"' class='customInput'>Choose file</label>");
		} else {
			$(this).wrap("<span class='htmlSnippetForm'></span>").after("<span class='customInput'></span>");
		}
	}
});
</script>

