<script>
$("#rye-76b16971-f11b-4ec5-8657-df4c21fb3a18 input, #rye-76b16971-f11b-4ec5-8657-df4c21fb3a18 select").each(function(i, v) {
	if (!$(this).hasClass('form-control')) {
		if ($(this).is("input[type='file']")) {
			$(this).attr('id', 'rye-76b16971-f11b-4ec5-8657-df4c21fb3a18_upload'+i).wrap("<span class='htmlSnippetForm'></span>").after("<label for='rye-76b16971-f11b-4ec5-8657-df4c21fb3a18_upload"+i+"' class='customInput'>Choose file</label>");
		} else {
			$(this).wrap("<span class='htmlSnippetForm'></span>").after("<span class='customInput'></span>");
		}
	}
});
</script>

