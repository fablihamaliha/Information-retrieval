    $(document).ready(function(){
    	
        $("#adf8c6da-9450-4f91-ada2-5998c8ba19d5-play, #adf8c6da-9450-4f91-ada2-5998c8ba19d5-pause").click(function() {
            if ($(this).attr("id") == "adf8c6da-9450-4f91-ada2-5998c8ba19d5-play") {
                $('#adf8c6da-9450-4f91-ada2-5998c8ba19d5').carousel('cycle');
                $(this).attr("id", "adf8c6da-9450-4f91-ada2-5998c8ba19d5-pause");
                $("#adf8c6da-9450-4f91-ada2-5998c8ba19d5-pause i").attr("class", "fa fa-pause");
            } else {
                $('#adf8c6da-9450-4f91-ada2-5998c8ba19d5').carousel('pause');
                $(this).attr("id", "adf8c6da-9450-4f91-ada2-5998c8ba19d5-play");
                $("#adf8c6da-9450-4f91-ada2-5998c8ba19d5-play i").attr("class", "fa fa-play");
