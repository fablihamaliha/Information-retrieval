        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#b2f46635-010c-464d-98c5-2188b8342123 button").click(function() {
                	jQuery("#b2f46635-010c-464d-98c5-2188b8342123.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.657564, "lng": -79.3772027}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('b2f46635-010c-464d-98c5-2188b8342123',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('b2f46635-010c-464d-98c5-2188b8342123','245 Church Street, Toronto, ON, M5B 1Z2, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('b2f46635-010c-464d-98c5-2188b8342123',latLng,'245 Church Street, Toronto, ON, M5B 1Z2, Canada','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('b2f46635-010c-464d-98c5-2188b8342123','245 Church Street, Toronto, ON, M5B 1Z2, Canada','1140px','600px',18,true);
                        }
                    }
                });
            });
