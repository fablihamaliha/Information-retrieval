<script>
    $(document).ready(function(){
    	
        $("#86e04574-72f2-46bf-a1b6-a0843c1e948d-play, #86e04574-72f2-46bf-a1b6-a0843c1e948d-pause").click(function() {
            if ($(this).attr("id") == "86e04574-72f2-46bf-a1b6-a0843c1e948d-play") {
                $('#86e04574-72f2-46bf-a1b6-a0843c1e948d').carousel('cycle');
                $(this).attr("id", "86e04574-72f2-46bf-a1b6-a0843c1e948d-pause");
                $("#86e04574-72f2-46bf-a1b6-a0843c1e948d-pause i").attr("class", "fa fa-pause");
            } else {
                $('#86e04574-72f2-46bf-a1b6-a0843c1e948d').carousel('pause');
                $(this).attr("id", "86e04574-72f2-46bf-a1b6-a0843c1e948d-play");
                $("#86e04574-72f2-46bf-a1b6-a0843c1e948d-play i").attr("class", "fa fa-play");
            }
        });
        
        		$('#86e04574-72f2-46bf-a1b6-a0843c1e948d').carousel('cycle');
        	
    });
</script>
