                <!-- Accordion Heading -->
                <div class="panel-heading container-clipboard">
                    <h3 class="panel-title">
                        <a id="accordion-1589484380136-four-year-bachelor-s-degree-from-a-recognized-institution-" role="tab" data-toggle="collapse" data-parent="#f7329b74-5460-4cc7-90c1-34649eadd25d" href="#accordion-content-1589484380136-four-year-bachelor-s-degree-from-a-recognized-institution-" aria-controls="accordion-content-1589484380136-four-year-bachelor-s-degree-from-a-recognized-institution-" aria-expanded="true">
