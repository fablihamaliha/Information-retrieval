			$('#c4ef8c04-a650-4819-9bda-cffeff0dfb8e table').removeAttr("cellpadding");
			$('#c4ef8c04-a650-4819-9bda-cffeff0dfb8e table').removeAttr("cellspacing");
			
			$('#c4ef8c04-a650-4819-9bda-cffeff0dfb8e table').DataTable({
				
				"sDom":"lrtp",
				"sort":false,
				"pagingType": "simple_numbers","paging":false,
				"autoWidth":true
			});
		
			//#NOTE-REMOVE
			if(removeHeading){
				$('#c4ef8c04-a650-4819-9bda-cffeff0dfb8e table thead').remove();
			}
			
			
		    
	    
	    
	} );
	</script>
