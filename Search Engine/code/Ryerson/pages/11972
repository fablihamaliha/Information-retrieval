		$('#c4ef8c04-a650-4819-9bda-cffeff0dfb8e table').addClass("table");
	    
			$('#c4ef8c04-a650-4819-9bda-cffeff0dfb8e table').addClass("table-striped");
		
		$('#c4ef8c04-a650-4819-9bda-cffeff0dfb8e table').addClass("table-borderless");
		
			var removeHeading = false;
			
			$('#c4ef8c04-a650-4819-9bda-cffeff0dfb8e table tbody tr').eq(0).find("td").replaceWith(function(){
				return $("<th>", {attr: {'width': $(this).attr('width'), 'rowspan': $(this).attr('rowspan'), 'colspan': $(this).attr('colspan'), 'valign': $(this).attr('valign'), 'headers': $(this).attr('headers'), 'style': $(this).attr('style')}, html: $(this).html()}).css("vertical-align", $(this).attr('valign'));;
			});
			$('#c4ef8c04-a650-4819-9bda-cffeff0dfb8e table:not(.dataTable) tbody tr').eq(0).wrap( "<thead></thead>" );
		
			$('#c4ef8c04-a650-4819-9bda-cffeff0dfb8e table thead').prependTo('#c4ef8c04-a650-4819-9bda-cffeff0dfb8e table');
			
			
