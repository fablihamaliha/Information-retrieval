<!-- GBS script -->
<script language="javascript" type="text/javascript">
	var numbers = new Array();
	if($('div.gbs').length > 0){
		$('div.gbs').each(function(divIndex) {
			var itemNumbers = new Array();
			$(this).children('div').each(function(index){
				var item_number = $(this).html().replace(/^\s+|\s+$/g,"");
				item_number = item_number.toString().split(' ', 1); //sometimes junk after number
				item_type = '';
				
				if(item_number.length > 0){
					if(index == 0){
						item_type = 'ISSN:';
					} 
					else if (index == 1) {
						item_type = 'LCCN:';
					} 
					else if (index == 2) {
						item_type = 'OCLC:';
						var temp_item_number = "";
						
						for(i=0; i<item_number.length; i++) {
							tempChar = item_number.toString().charAt(i);
							if(!isNaN(parseInt(tempChar))) {
								temp_item_number += '' + tempChar;
							}
						}
						item_number = temp_item_number;
						
					}	
					itemNumbers[index] = item_type + item_number.toString();
				}
				
			});
			numbers[divIndex] = itemNumbers;
		});
		// Construct the URL for the remote GBS script
		var api_url ="https://books.google.com/books?jscmd=viewapi&bibkeys=" + numbers.join(",") + "";
		var scriptLine = "%3Cscript src='" + api_url + "' type='text/javascript'%3E%3C/script%3E";
		document.write(unescape(scriptLine));	
	}
	else {
		if($('table.bibDetail').length >= 2){
			var isbn_list = '', lccn_list = '', numbers = new Array(), count = 0;
			var numChildren = 	$('table.bibDetail td').length;
				
			$('table.bibDetail td').each(function(index) {	
				if( $(this).html() == 'ISBN' ) {
					if(index + 1 < numChildren) {
						
						isbn_list = $(this).next().html().split(' ', 1);
						numbers[count] = 'ISBN:' + isbn_list;
						numbers[count] = numbers[count].replace(/^\s+|\s+$/g,"");
						numbers[count] = unescape(escape(numbers[count]).replace("%0A",""));
						count++;
					}
				} 
				else if ($(this).html() == 'LCCN/NLC') {
					if(index + 1 < numChildren) {
						lccn_list = $(this).next().html().split(' ', 1);
						numbers[count] = 'LCCN:' + lccn_list;
						numbers[count] = numbers[count].replace(/^\s+|\s+$/g,"");
						numbers[count] = unescape(escape(numbers[count]).replace("%0A",""));
						count++;
					}
				}
			
			});
		}
		if (numbers.length > 0) {
			// Construct the URL for the remote GBS script
			var api_url ="https://books.google.com/books?jscmd=viewapi&bibkeys=" + numbers.join(",") + "";
			var scriptLine = "%3Cscript src='" + api_url + "' type='text/javascript'%3E%3C/script%3E";
			document.write(unescape(scriptLine));	
		}
	}
</script>

<script language="javascript" type="text/javascript">
	if ($('div.gbs').length > 0) {
		//at list level
		loadGBS(numbers);
	} 
	else {
		//could be at bib level
		try {
			loadGBSBibRecord(numbers)
		}
		catch(err){};
		// createQRCode();
	}
</script>
<!-- End GBS Script -->

