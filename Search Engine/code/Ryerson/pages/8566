                jQuery("#c7fdf477-7fce-496b-924d-08129401fa97 button").click(function() {
                	jQuery("#c7fdf477-7fce-496b-924d-08129401fa97.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6558668, "lng": -79.3819145}');
