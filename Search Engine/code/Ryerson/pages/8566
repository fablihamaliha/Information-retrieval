                        if(latLng != null){
                            drawDynamicMapByLatLng('c7fdf477-7fce-496b-924d-08129401fa97',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('c7fdf477-7fce-496b-924d-08129401fa97','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','600px',18,true);
                        }
