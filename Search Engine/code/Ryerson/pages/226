        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#1e54d155-9460-4c7d-805d-7fb8789004c0 button").click(function() {
                	jQuery("#1e54d155-9460-4c7d-805d-7fb8789004c0.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.657564, "lng": -79.3772027}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('1e54d155-9460-4c7d-805d-7fb8789004c0',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('1e54d155-9460-4c7d-805d-7fb8789004c0','245 Church Street, Toronto, ON, M5B 1Z2, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('1e54d155-9460-4c7d-805d-7fb8789004c0',latLng,'245 Church Street, Toronto, ON, M5B 1Z2, Canada','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('1e54d155-9460-4c7d-805d-7fb8789004c0','245 Church Street, Toronto, ON, M5B 1Z2, Canada','1140px','600px',18,true);
                        }
                    }
                });
            });
