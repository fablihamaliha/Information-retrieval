jQuery(document).ready(function() {
    jQuery("#component214912537 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component214912537 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component214912537 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component214912537 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component214912537 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
