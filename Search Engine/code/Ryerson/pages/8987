<script>
    $(document).ready(function(){
    	
        $("#d8978a5b-c8c4-45d1-9751-323be7dc928e-play, #d8978a5b-c8c4-45d1-9751-323be7dc928e-pause").click(function() {
            if ($(this).attr("id") == "d8978a5b-c8c4-45d1-9751-323be7dc928e-play") {
                $('#d8978a5b-c8c4-45d1-9751-323be7dc928e').carousel('cycle');
                $(this).attr("id", "d8978a5b-c8c4-45d1-9751-323be7dc928e-pause");
                $("#d8978a5b-c8c4-45d1-9751-323be7dc928e-pause i").attr("class", "fa fa-pause");
            } else {
                $('#d8978a5b-c8c4-45d1-9751-323be7dc928e').carousel('pause');
                $(this).attr("id", "d8978a5b-c8c4-45d1-9751-323be7dc928e-play");
                $("#d8978a5b-c8c4-45d1-9751-323be7dc928e-play i").attr("class", "fa fa-play");
            }
        });
        
