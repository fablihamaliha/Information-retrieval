	<script>
	
	//convert to plugin in pure js....later (soon)
	$(document).ready(function() {
		$('#4b749882-58dc-4197-9434-2fe01952e252 table').addClass("table");
	    
		$('#4b749882-58dc-4197-9434-2fe01952e252 table').addClass("table-bordered");
		
			var removeHeading = false;
			
				//mandatory to have heading on initialization but remove it after. See #NOTE-REMOVE
				if($('#4b749882-58dc-4197-9434-2fe01952e252 table thead').eq(0).length == 0){
						//insert fake row and delete
						removeHeading = true;
						var numOfColsTD = $('#4b749882-58dc-4197-9434-2fe01952e252 table tbody tr').eq(0).find('td').length;
						var numOfColsTH = $('#4b749882-58dc-4197-9434-2fe01952e252 table tbody tr').eq(0).find('th').length;
						
						var appendTH = "<thead>";
						for(var i = 0; i < numOfColsTD+numOfColsTH;i++){
							appendTH += "<th>Column</th>";
						}
						appendTH += "</thead>";
						$('#4b749882-58dc-4197-9434-2fe01952e252 table').prepend(appendTH);
				}
			
			
			$('#4b749882-58dc-4197-9434-2fe01952e252 table').removeAttr("border");
			$('#4b749882-58dc-4197-9434-2fe01952e252 table').removeAttr("cellpadding");
			$('#4b749882-58dc-4197-9434-2fe01952e252 table').removeAttr("cellspacing");
			
			$('#4b749882-58dc-4197-9434-2fe01952e252 table').DataTable({
				
				"sDom":"lrtp",
				"sort":false,
				"pagingType": "simple_numbers","paging":false,
				"autoWidth":true
			});
		
			//#NOTE-REMOVE
			if(removeHeading){
				$('#4b749882-58dc-4197-9434-2fe01952e252 table thead').remove();
			}
			
			
		    
	    
	    
	} );
	</script>
