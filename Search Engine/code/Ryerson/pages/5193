                jQuery("#3fdbe36c-1b12-4a6c-99f6-66640f73ef77 button").click(function() {
                	jQuery("#3fdbe36c-1b12-4a6c-99f6-66640f73ef77.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6559513, "lng": -79.381742}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('3fdbe36c-1b12-4a6c-99f6-66640f73ef77',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('3fdbe36c-1b12-4a6c-99f6-66640f73ef77','1 Dundas Street West, Toronto, ON m5b 2k3','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('3fdbe36c-1b12-4a6c-99f6-66640f73ef77',latLng,'1 Dundas Street West, Toronto, ON m5b 2k3','1140px','600px',18,true);
