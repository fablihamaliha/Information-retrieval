        <script>
        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#e4665398-d941-4dc3-a3fa-e849af574c9c button").click(function() {
                	jQuery("#e4665398-d941-4dc3-a3fa-e849af574c9c.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = false;
                    var latLng = JSON.parse('{"lat": 43.6619296, "lng": -79.3802844}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('e4665398-d941-4dc3-a3fa-e849af574c9c',latLng,'300px',18,true);
                        } else{
                            drawDynamicMapByAddress('e4665398-d941-4dc3-a3fa-e849af574c9c','50 Carlton St, Toronto, ON M5B 1J2','300px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('e4665398-d941-4dc3-a3fa-e849af574c9c',latLng,'50 Carlton St, Toronto, ON M5B 1J2','1140px','300px',18,true);
                        } else{
                            drawStaticMapByAddress('e4665398-d941-4dc3-a3fa-e849af574c9c','50 Carlton St, Toronto, ON M5B 1J2','1140px','300px',18,true);
                        }
                    }
                });
            });
        </script>
    
    
    
