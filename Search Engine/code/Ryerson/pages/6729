jQuery(document).ready(function() {
    jQuery("#component937079241 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component937079241 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component937079241 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component937079241 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component937079241 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
