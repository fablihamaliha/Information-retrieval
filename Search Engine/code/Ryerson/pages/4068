<script>
    $(document).ready(function(){
    	
        $("#1261f549-a78a-46d9-8fe5-26ddcee5483d-play, #1261f549-a78a-46d9-8fe5-26ddcee5483d-pause").click(function() {
            if ($(this).attr("id") == "1261f549-a78a-46d9-8fe5-26ddcee5483d-play") {
                $('#1261f549-a78a-46d9-8fe5-26ddcee5483d').carousel('cycle');
                $(this).attr("id", "1261f549-a78a-46d9-8fe5-26ddcee5483d-pause");
                $("#1261f549-a78a-46d9-8fe5-26ddcee5483d-pause i").attr("class", "fa fa-pause");
            } else {
                $('#1261f549-a78a-46d9-8fe5-26ddcee5483d').carousel('pause');
                $(this).attr("id", "1261f549-a78a-46d9-8fe5-26ddcee5483d-play");
                $("#1261f549-a78a-46d9-8fe5-26ddcee5483d-play i").attr("class", "fa fa-play");
            }
        });
        
        	$('#1261f549-a78a-46d9-8fe5-26ddcee5483d').carousel('pause');
        	
    });
</script>
