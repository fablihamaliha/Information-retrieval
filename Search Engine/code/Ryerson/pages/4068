<script>
    $(document).ready(function(){
    	
        $("#e1cb5dbf-ad66-49ee-b158-a64fb4ff701d-play, #e1cb5dbf-ad66-49ee-b158-a64fb4ff701d-pause").click(function() {
            if ($(this).attr("id") == "e1cb5dbf-ad66-49ee-b158-a64fb4ff701d-play") {
                $('#e1cb5dbf-ad66-49ee-b158-a64fb4ff701d').carousel('cycle');
                $(this).attr("id", "e1cb5dbf-ad66-49ee-b158-a64fb4ff701d-pause");
                $("#e1cb5dbf-ad66-49ee-b158-a64fb4ff701d-pause i").attr("class", "fa fa-pause");
            } else {
                $('#e1cb5dbf-ad66-49ee-b158-a64fb4ff701d').carousel('pause');
                $(this).attr("id", "e1cb5dbf-ad66-49ee-b158-a64fb4ff701d-play");
                $("#e1cb5dbf-ad66-49ee-b158-a64fb4ff701d-play i").attr("class", "fa fa-play");
            }
        });
        
        	$('#e1cb5dbf-ad66-49ee-b158-a64fb4ff701d').carousel('pause');
        	
    });
</script>
