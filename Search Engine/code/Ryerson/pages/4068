<script>
    $(document).ready(function(){
    	
        $("#df46e62d-5946-48a1-853d-df58b299beb2-play, #df46e62d-5946-48a1-853d-df58b299beb2-pause").click(function() {
            if ($(this).attr("id") == "df46e62d-5946-48a1-853d-df58b299beb2-play") {
                $('#df46e62d-5946-48a1-853d-df58b299beb2').carousel('cycle');
                $(this).attr("id", "df46e62d-5946-48a1-853d-df58b299beb2-pause");
                $("#df46e62d-5946-48a1-853d-df58b299beb2-pause i").attr("class", "fa fa-pause");
            } else {
                $('#df46e62d-5946-48a1-853d-df58b299beb2').carousel('pause');
                $(this).attr("id", "df46e62d-5946-48a1-853d-df58b299beb2-play");
                $("#df46e62d-5946-48a1-853d-df58b299beb2-play i").attr("class", "fa fa-play");
            }
        });
        
        	$('#df46e62d-5946-48a1-853d-df58b299beb2').carousel('pause');
        	
    });
</script>
