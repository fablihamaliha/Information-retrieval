		if ($(this).is("input[type='file']")) {
			$(this).attr('id', 'rye-f36c279c-07af-4f43-842c-22e92b7bef2a_upload'+i).wrap("<span class='htmlSnippetForm'></span>").after("<label for='rye-f36c279c-07af-4f43-842c-22e92b7bef2a_upload"+i+"' class='customInput'>Choose file</label>");
		} else {
			$(this).wrap("<span class='htmlSnippetForm'></span>").after("<span class='customInput'></span>");
		}
	}
});
</script>

