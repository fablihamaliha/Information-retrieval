                <li role="presentation" class="active">
                    <a id="tab-1579018968819-faculty" href="#tab-content-1579018968819-faculty" role="tab" data-toggle="tab" aria-controls="tab-content-1579018968819-faculty" aria-expanded="true">
                        <span class="text">FACULTY</span>
                    </a>
                </li>
                
                <li role="presentation" class="next">
                    <a id="tab-1579018968850-associates" href="#tab-content-1579018968850-associates" role="tab" data-toggle="tab" aria-controls="tab-content-1579018968850-associates" aria-expanded="false">
                        <span class="text">ASSOCIATES</span>
                    </a>
                </li>
                
                <li role="presentation" class="">
                    <a id="tab-1581541012792-staff" href="#tab-content-1581541012792-staff" role="tab" data-toggle="tab" aria-controls="tab-content-1581541012792-staff" aria-expanded="false">
