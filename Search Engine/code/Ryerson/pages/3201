jQuery(document).ready(function() {
    jQuery("#component99331373 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component99331373 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component99331373 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component99331373 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component99331373 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
