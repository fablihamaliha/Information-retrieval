        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#116852b6-7c81-4b85-b8a8-66c3eb9b65c7 button").click(function() {
                	jQuery("#116852b6-7c81-4b85-b8a8-66c3eb9b65c7.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.65829859999999, "lng": -79.3807846}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('116852b6-7c81-4b85-b8a8-66c3eb9b65c7',latLng,'600px',20,true);
                        } else{
                            drawDynamicMapByAddress('116852b6-7c81-4b85-b8a8-66c3eb9b65c7','350 Victoria Street, Toronto, ON, M5B 2K3, Canada','600px',20,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('116852b6-7c81-4b85-b8a8-66c3eb9b65c7',latLng,'350 Victoria Street, Toronto, ON, M5B 2K3, Canada','1140px','600px',20,true);
