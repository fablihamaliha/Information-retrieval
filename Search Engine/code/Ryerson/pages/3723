                $('#9c3f8e5b-95fe-4282-b3eb-f14a2a57a1ce').carousel('cycle');
                $(this).attr("id", "9c3f8e5b-95fe-4282-b3eb-f14a2a57a1ce-pause");
                $("#9c3f8e5b-95fe-4282-b3eb-f14a2a57a1ce-pause i").attr("class", "fa fa-pause");
            } else {
                $('#9c3f8e5b-95fe-4282-b3eb-f14a2a57a1ce').carousel('pause');
                $(this).attr("id", "9c3f8e5b-95fe-4282-b3eb-f14a2a57a1ce-play");
