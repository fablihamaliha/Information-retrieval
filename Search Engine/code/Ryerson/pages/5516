        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#06dd89f0-6f88-4eb4-87b6-8a5840d6b6c3 button").click(function() {
                	jQuery("#06dd89f0-6f88-4eb4-87b6-8a5840d6b6c3.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.65815790000001, "lng": -79.37879660000002}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('06dd89f0-6f88-4eb4-87b6-8a5840d6b6c3',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('06dd89f0-6f88-4eb4-87b6-8a5840d6b6c3','50 Gould Street, Toronto, ON, M5B 1X8, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('06dd89f0-6f88-4eb4-87b6-8a5840d6b6c3',latLng,'50 Gould Street, Toronto, ON, M5B 1X8, Canada','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('06dd89f0-6f88-4eb4-87b6-8a5840d6b6c3','50 Gould Street, Toronto, ON, M5B 1X8, Canada','1140px','600px',18,true);
                        }
                    }
                });
            });
