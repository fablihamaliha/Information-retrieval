        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#6524a0a0-b360-4a99-a8a6-039ae36d2a7c button").click(function() {
                	jQuery("#6524a0a0-b360-4a99-a8a6-039ae36d2a7c.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.657564, "lng": -79.3772027}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('6524a0a0-b360-4a99-a8a6-039ae36d2a7c',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('6524a0a0-b360-4a99-a8a6-039ae36d2a7c','245 Church Street, Toronto, ON, M5B 1Z2, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('6524a0a0-b360-4a99-a8a6-039ae36d2a7c',latLng,'245 Church Street, Toronto, ON, M5B 1Z2, Canada','1140px','600px',18,true);
