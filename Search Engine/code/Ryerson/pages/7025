jQuery(document).ready(function() {
    jQuery("#component1022571287 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component1022571287 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component1022571287 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component1022571287 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component1022571287 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
 </script>
 
