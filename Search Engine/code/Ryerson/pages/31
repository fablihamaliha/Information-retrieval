    $(document).ready(function(){
    	
        $("#9333d031-cab4-4f9d-b119-01619af5ebb8-play, #9333d031-cab4-4f9d-b119-01619af5ebb8-pause").click(function() {
            if ($(this).attr("id") == "9333d031-cab4-4f9d-b119-01619af5ebb8-play") {
                $('#9333d031-cab4-4f9d-b119-01619af5ebb8').carousel('cycle');
                $(this).attr("id", "9333d031-cab4-4f9d-b119-01619af5ebb8-pause");
