                        if(latLng != null){
                            drawDynamicMapByLatLng('b30a229d-6555-400d-ac62-501a2b94c24e',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('b30a229d-6555-400d-ac62-501a2b94c24e','1 Dundas Street West, Toronto, ON m5b 2k3','600px',18,true);
