<script>
    $(document).ready(function(){
    	
        $("#d6efc9a6-542c-49eb-9a4b-045e3f08d8e8-play, #d6efc9a6-542c-49eb-9a4b-045e3f08d8e8-pause").click(function() {
            if ($(this).attr("id") == "d6efc9a6-542c-49eb-9a4b-045e3f08d8e8-play") {
                $('#d6efc9a6-542c-49eb-9a4b-045e3f08d8e8').carousel('cycle');
                $(this).attr("id", "d6efc9a6-542c-49eb-9a4b-045e3f08d8e8-pause");
                $("#d6efc9a6-542c-49eb-9a4b-045e3f08d8e8-pause i").attr("class", "fa fa-pause");
            } else {
                $('#d6efc9a6-542c-49eb-9a4b-045e3f08d8e8').carousel('pause');
                $(this).attr("id", "d6efc9a6-542c-49eb-9a4b-045e3f08d8e8-play");
                $("#d6efc9a6-542c-49eb-9a4b-045e3f08d8e8-play i").attr("class", "fa fa-play");
            }
        });
        
        		$('#d6efc9a6-542c-49eb-9a4b-045e3f08d8e8').carousel('cycle');
        	
    });
</script>
