        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#1fa5b310-dc9d-4a96-9b35-f1a77248b4cd button").click(function() {
                	jQuery("#1fa5b310-dc9d-4a96-9b35-f1a77248b4cd.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6558668, "lng": -79.3819145}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('1fa5b310-dc9d-4a96-9b35-f1a77248b4cd',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('1fa5b310-dc9d-4a96-9b35-f1a77248b4cd','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('1fa5b310-dc9d-4a96-9b35-f1a77248b4cd',latLng,'1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('1fa5b310-dc9d-4a96-9b35-f1a77248b4cd','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','1140px','600px',18,true);
                        }
                    }
                });
            });
        </script>
    
    
    
