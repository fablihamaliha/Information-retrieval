<script>
$(document).ready(function(){addCoursePopover('#e572e8d0-4125-4807-8e1b-eaf0def5a96f');});
if (flowWidthCollection == undefined) {
	var flowWidthCollection = new Object();	
}
$(document).on('_intervalshort', {
    layoutID: "e572e8d0-4125-4807-8e1b-eaf0def5a96f"
