jQuery(document).ready(function() {
    jQuery("#component16742874 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component16742874 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component16742874 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component16742874 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component16742874 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
 </script>
 
