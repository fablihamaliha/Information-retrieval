        <script>
        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#d63d24bb-bf8f-48e3-bbc2-cb396061bde4 button").click(function() {
                	jQuery("#d63d24bb-bf8f-48e3-bbc2-cb396061bde4.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = false;
                    var latLng = JSON.parse('{"lat": 43.65722239999999, "lng": -79.3777944}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('d63d24bb-bf8f-48e3-bbc2-cb396061bde4',latLng,'300px',18,true);
                        } else{
                            drawDynamicMapByAddress('d63d24bb-bf8f-48e3-bbc2-cb396061bde4','288 Church St, Toronto, ON M5B 1Z5','300px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('d63d24bb-bf8f-48e3-bbc2-cb396061bde4',latLng,'288 Church St, Toronto, ON M5B 1Z5','1140px','300px',18,true);
                        } else{
                            drawStaticMapByAddress('d63d24bb-bf8f-48e3-bbc2-cb396061bde4','288 Church St, Toronto, ON M5B 1Z5','1140px','300px',18,true);
                        }
                    }
                });
            });
        </script>
    
    
    
