jQuery(document).ready(function() {
    jQuery("#component473321899 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component473321899 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component473321899 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component473321899 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component473321899 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
 </script>
 
