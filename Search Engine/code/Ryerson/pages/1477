        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#5bd89ad2-48dc-4440-b729-64cd3069fd9d button").click(function() {
                	jQuery("#5bd89ad2-48dc-4440-b729-64cd3069fd9d.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.657564, "lng": -79.3772027}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('5bd89ad2-48dc-4440-b729-64cd3069fd9d',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('5bd89ad2-48dc-4440-b729-64cd3069fd9d','245 Church Street, Toronto, ON, M5B 1Z2, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('5bd89ad2-48dc-4440-b729-64cd3069fd9d',latLng,'245 Church Street, Toronto, ON, M5B 1Z2, Canada','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('5bd89ad2-48dc-4440-b729-64cd3069fd9d','245 Church Street, Toronto, ON, M5B 1Z2, Canada','1140px','600px',18,true);
                        }
                    }
                });
            });
        </script>
    
    
    
