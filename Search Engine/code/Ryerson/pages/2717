            if ($(this).attr("id") == "6c791ebc-de2c-4c53-b432-03ad28d6b204-play") {
                $('#6c791ebc-de2c-4c53-b432-03ad28d6b204').carousel('cycle');
                $(this).attr("id", "6c791ebc-de2c-4c53-b432-03ad28d6b204-pause");
                $("#6c791ebc-de2c-4c53-b432-03ad28d6b204-pause i").attr("class", "fa fa-pause");
            } else {
                $('#6c791ebc-de2c-4c53-b432-03ad28d6b204').carousel('pause');
                $(this).attr("id", "6c791ebc-de2c-4c53-b432-03ad28d6b204-play");
                $("#6c791ebc-de2c-4c53-b432-03ad28d6b204-play i").attr("class", "fa fa-play");
            }
        });
        
        		$('#6c791ebc-de2c-4c53-b432-03ad28d6b204').carousel('cycle');
        	
    });
</script>
