        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#b1b30918-1338-4771-97f6-8a1d9b49968a button").click(function() {
                	jQuery("#b1b30918-1338-4771-97f6-8a1d9b49968a.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6597321, "lng": -79.37971449999999}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('b1b30918-1338-4771-97f6-8a1d9b49968a',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('b1b30918-1338-4771-97f6-8a1d9b49968a','44 Gerrard Street East','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('b1b30918-1338-4771-97f6-8a1d9b49968a',latLng,'44 Gerrard Street East','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('b1b30918-1338-4771-97f6-8a1d9b49968a','44 Gerrard Street East','1140px','600px',18,true);
                        }
                    }
                });
            });
