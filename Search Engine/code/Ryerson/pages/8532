        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#81323136-5bb8-419a-93b3-ecb68cfd0d98 button").click(function() {
                	jQuery("#81323136-5bb8-419a-93b3-ecb68cfd0d98.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6558668, "lng": -79.3819145}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('81323136-5bb8-419a-93b3-ecb68cfd0d98',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('81323136-5bb8-419a-93b3-ecb68cfd0d98','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('81323136-5bb8-419a-93b3-ecb68cfd0d98',latLng,'1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','1140px','600px',18,true);
