<script>
    $(document).ready(function(){
    	
        $("#4ad2e43a-4e37-4938-816b-c243e53c9cab-play, #4ad2e43a-4e37-4938-816b-c243e53c9cab-pause").click(function() {
            if ($(this).attr("id") == "4ad2e43a-4e37-4938-816b-c243e53c9cab-play") {
                $('#4ad2e43a-4e37-4938-816b-c243e53c9cab').carousel('cycle');
                $(this).attr("id", "4ad2e43a-4e37-4938-816b-c243e53c9cab-pause");
                $("#4ad2e43a-4e37-4938-816b-c243e53c9cab-pause i").attr("class", "fa fa-pause");
            } else {
                $('#4ad2e43a-4e37-4938-816b-c243e53c9cab').carousel('pause');
                $(this).attr("id", "4ad2e43a-4e37-4938-816b-c243e53c9cab-play");
                $("#4ad2e43a-4e37-4938-816b-c243e53c9cab-play i").attr("class", "fa fa-play");
            }
        });
        
        	$('#4ad2e43a-4e37-4938-816b-c243e53c9cab').carousel('pause');
        	
    });
</script>
