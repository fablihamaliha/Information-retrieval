			$(this).attr('id', 'rye-d101dfa7-07c2-4802-8aab-09ebdd49bc13_upload'+i).wrap("<span class='htmlSnippetForm'></span>").after("<label for='rye-d101dfa7-07c2-4802-8aab-09ebdd49bc13_upload"+i+"' class='customInput'>Choose file</label>");
		} else {
			$(this).wrap("<span class='htmlSnippetForm'></span>").after("<span class='customInput'></span>");
		}
	}
});
</script>

