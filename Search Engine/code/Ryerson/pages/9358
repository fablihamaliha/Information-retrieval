		if ($(this).is("input[type='file']")) {
			$(this).attr('id', 'rye-601d7e6f-23fe-4179-afef-16a1562e0fbe_upload'+i).wrap("<span class='htmlSnippetForm'></span>").after("<label for='rye-601d7e6f-23fe-4179-afef-16a1562e0fbe_upload"+i+"' class='customInput'>Choose file</label>");
		} else {
			$(this).wrap("<span class='htmlSnippetForm'></span>").after("<span class='customInput'></span>");
		}
	}
});
</script>

