        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#4105689f-9189-415b-92bc-d1d4817fc3fc button").click(function() {
                	jQuery("#4105689f-9189-415b-92bc-d1d4817fc3fc.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6570679, "lng": -79.380062}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('4105689f-9189-415b-92bc-d1d4817fc3fc',latLng,'600px',15,false);
                        } else{
                            drawDynamicMapByAddress('4105689f-9189-415b-92bc-d1d4817fc3fc','285 Victoria Street, Toronto, ON, M5B 2K3, Canada','600px',15,false);
                        }
                    } else {
                        if(latLng != null){
