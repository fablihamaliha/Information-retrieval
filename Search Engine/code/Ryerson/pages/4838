        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#501b3efa-37d6-430b-bc1c-4425a01ed633 button").click(function() {
                	jQuery("#501b3efa-37d6-430b-bc1c-4425a01ed633.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.65867129999999, "lng": -79.3805337}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('501b3efa-37d6-430b-bc1c-4425a01ed633',latLng,'600px',18,false);
                        } else{
                            drawDynamicMapByAddress('501b3efa-37d6-430b-bc1c-4425a01ed633','379  Victoria Street, Toronto, ON, M5B 2K3, Canada','600px',18,false);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('501b3efa-37d6-430b-bc1c-4425a01ed633',latLng,'379  Victoria Street, Toronto, ON, M5B 2K3, Canada','1140px','600px',18,false);
                        } else{
                            drawStaticMapByAddress('501b3efa-37d6-430b-bc1c-4425a01ed633','379  Victoria Street, Toronto, ON, M5B 2K3, Canada','1140px','600px',18,false);
                        }
                    }
                });
            });
