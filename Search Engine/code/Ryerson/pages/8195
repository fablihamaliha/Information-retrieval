jQuery(document).ready(function() {
    jQuery("#component900537618 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component900537618 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component900537618 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component900537618 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component900537618 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
