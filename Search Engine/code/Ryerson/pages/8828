			$(this).attr('id', 'rye-ade95c3f-fe10-49aa-9113-c5c23cff8e1d_upload'+i).wrap("<span class='htmlSnippetForm'></span>").after("<label for='rye-ade95c3f-fe10-49aa-9113-c5c23cff8e1d_upload"+i+"' class='customInput'>Choose file</label>");
