        <script>
        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#28409108-d403-4f15-8510-c1960e92b3a7 button").click(function() {
                	jQuery("#28409108-d403-4f15-8510-c1960e92b3a7.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6586299, "lng": -79.3770727}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('28409108-d403-4f15-8510-c1960e92b3a7',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('28409108-d403-4f15-8510-c1960e92b3a7','80 Gould St, Toronto, ON M5B 2M7','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('28409108-d403-4f15-8510-c1960e92b3a7',latLng,'80 Gould St, Toronto, ON M5B 2M7','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('28409108-d403-4f15-8510-c1960e92b3a7','80 Gould St, Toronto, ON M5B 2M7','1140px','600px',18,true);
                        }
                    }
                });
            });
        </script>
    
    
    
