		if ($(this).is("input[type='file']")) {
			$(this).attr('id', 'rye-18b480d9-680d-4cc5-9ae1-2e6d0a0daf78_upload'+i).wrap("<span class='htmlSnippetForm'></span>").after("<label for='rye-18b480d9-680d-4cc5-9ae1-2e6d0a0daf78_upload"+i+"' class='customInput'>Choose file</label>");
