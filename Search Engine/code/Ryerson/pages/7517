<div class="heading resPageHeading">


























<h1 >Vanity URL</h1></div>

<div class="content stackparsys">










				
				<div class="resReference section">

























<div class="restext resText parbase">


























<div class="res-text richtextContent background-hidden" id="7d5f6c14-756a-4e1a-8560-c6777bb74bcf" >

    <p>Vanity URLs are customized short URLs, created to redirect users to a page deeper within your site.</p> 
<p>Vanity URLs are generally used for marketing purposes because they are shorter, easier to remember and easier to type.</p>
    





</div>
<script>$(document).ready(function(){addCoursePopover("#7d5f6c14-756a-4e1a-8560-c6777bb74bcf");});</script></div>

</div>

				

				
				<div class="resReference section">

























<div class="restable resTable parbase">























 



	<div class="scrollIndicator hidden">
		<i class="fa fa-long-arrow-left" aria-hidden="true"></i> Swipe to see more <i class="fa fa-long-arrow-right" aria-hidden="true"></i>
	</div>
	<div id="6a1d9d9e-9adb-45d8-8867-72efa053d88f">
		<table width="100%" cellspacing="0" cellpadding="1" border="1">
<tbody><tr><td>Vanity URL:</td>
<td><strong>ryerson.ca/event</strong><br>
</td>
<td>Short! &nbsp;</td>
</tr><tr><td>Page location</td>
<td><strong>ryerson.ca/sitename/foldername/subfoldername/event-name</strong><br>
</td>
<td>Long!</td>
</tr></tbody></table>

	</div>




	<script>
	
	//convert to plugin in pure js....later (soon)
	$(document).ready(function() {
		$('#6a1d9d9e-9adb-45d8-8867-72efa053d88f table').addClass("table");
	    
		$('#6a1d9d9e-9adb-45d8-8867-72efa053d88f table').addClass("table-bordered");
		
			var removeHeading = false;
			
				//mandatory to have heading on initialization but remove it after. See #NOTE-REMOVE
				if($('#6a1d9d9e-9adb-45d8-8867-72efa053d88f table thead').eq(0).length == 0){
						//insert fake row and delete
						removeHeading = true;
						var numOfColsTD = $('#6a1d9d9e-9adb-45d8-8867-72efa053d88f table tbody tr').eq(0).find('td').length;
						var numOfColsTH = $('#6a1d9d9e-9adb-45d8-8867-72efa053d88f table tbody tr').eq(0).find('th').length;
						
						var appendTH = "<thead>";
						for(var i = 0; i < numOfColsTD+numOfColsTH;i++){
							appendTH += "<th>Column</th>";
						}
						appendTH += "</thead>";
						$('#6a1d9d9e-9adb-45d8-8867-72efa053d88f table').prepend(appendTH);
				}
			
			
			$('#6a1d9d9e-9adb-45d8-8867-72efa053d88f table').removeAttr("border");
			$('#6a1d9d9e-9adb-45d8-8867-72efa053d88f table').removeAttr("cellpadding");
			$('#6a1d9d9e-9adb-45d8-8867-72efa053d88f table').removeAttr("cellspacing");
			
			$('#6a1d9d9e-9adb-45d8-8867-72efa053d88f table').DataTable({
				
				"sDom":"lrtp",
				"sort":false,
				"pagingType": "simple_numbers","paging":false,
				"autoWidth":true
			});
		
			//#NOTE-REMOVE
			if(removeHeading){
				$('#6a1d9d9e-9adb-45d8-8867-72efa053d88f table thead').remove();
			}
			
			
		    
	    
	    
				$('#6a1d9d9e-9adb-45d8-8867-72efa053d88f table').attr("summary","Short Vanity URL redirects users to longer URL");
	        
	} );
	</script>
