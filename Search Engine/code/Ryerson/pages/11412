		<div class="2b074b1f-5fa3-45ee-aaa9-9566237906d9" data-caption="true" aria-label="Image gallery. Press enter on an image to expand full screen and escape to close. Images in gallery do not link to other pages."></div>
		<script>
		jQuery(document).ready(function() {
			jQuery(".2b074b1f-5fa3-45ee-aaa9-9566237906d9").initStackComponent({
				type:"galleryGrid",
				uuid:"2b074b1f-5fa3-45ee-aaa9-9566237906d9",
		        url:"/content/ryerson/websupport/components/lists-grids/gallery-grid/jcr:content/content/resgallerystack",
		        limit:"12",
		        resultsPerPage:0,
		        offset:"0",
		        gridColumnClass:"col-lg-4 col-md-4 col-sm-12 col-xs-12",
		        displayAs:"grid",
		        paginationType:"numbered",
		        displayItems:{
		        	displayCaption:true
