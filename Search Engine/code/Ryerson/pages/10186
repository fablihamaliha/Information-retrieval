    });

    jQuery("#component1491316524 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component1491316524 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component1491316524 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component1491316524 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
