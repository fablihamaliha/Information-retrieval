                jQuery("#987d8b35-a77b-4c7f-9613-fe9be71e1e5d button").click(function() {
                	jQuery("#987d8b35-a77b-4c7f-9613-fe9be71e1e5d.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6585292, "lng": -79.3800808}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('987d8b35-a77b-4c7f-9613-fe9be71e1e5d',latLng,'600px',20,false);
                        } else{
                            drawDynamicMapByAddress('987d8b35-a77b-4c7f-9613-fe9be71e1e5d','Kerr Hall, 350 Victoria St, Toronto, ON M5B 0A1','600px',20,false);
                        }
                    } else {
