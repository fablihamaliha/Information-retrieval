<script type="text/javascript">
jQuery(document).ready(function() {
    jQuery("#component2062871252 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component2062871252 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component2062871252 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component2062871252 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component2062871252 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
 </script>
 
</div>
</div>
  












