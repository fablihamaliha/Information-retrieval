                <!-- Accordion Body -->
                <div tabindex="0" id="accordion-content-1560962511951-economics-and-management-science-major" class="panel-collapse collapse" role="tabpanel" aria-labelledby="accordion-1560962511951-economics-and-management-science-major">
                    <div class="panel-body ">
                        <div class="1560962511951 stackparsys">










				
				<div class="resReference section">

























<div class="1551368923416 stackparsys">










				
				<div class="resTwoColEven section">









  
















 
	


<div class="row">
	
		<div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
			<div class="stackparsys">










				
				<div class="resText parbase section">


























