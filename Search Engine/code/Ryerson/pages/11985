					<button class="resAccordionOpenAll">Open All</button> <button class="resAccordionCloseAll">Close All</button>
				</div>
				<script type="text/javascript">$(document).ready(function(){ initResAccordionControl("4316f522-119c-4713-b866-bc24578f6b57", true); });</script>
			
            
            <div class="panel panel-default">
                <!-- Accordion Heading -->
                <div class="panel-heading container-clipboard">
                    <div class="panel-title">
                        <a id="accordion-1560962511951-economics-and-management-science-major" role="tab" data-toggle="collapse" data-parent="#4316f522-119c-4713-b866-bc24578f6b57" href="#accordion-content-1560962511951-economics-and-management-science-major" aria-controls="accordion-content-1560962511951-economics-and-management-science-major" aria-expanded="false">
