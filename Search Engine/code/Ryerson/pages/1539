	if (!$(this).hasClass('form-control')) {
		if ($(this).is("input[type='file']")) {
			$(this).attr('id', 'rye-59ca4725-444e-41b3-bfe8-8aa50d339e0b_upload'+i).wrap("<span class='htmlSnippetForm'></span>").after("<label for='rye-59ca4725-444e-41b3-bfe8-8aa50d339e0b_upload"+i+"' class='customInput'>Choose file</label>");
		} else {
			$(this).wrap("<span class='htmlSnippetForm'></span>").after("<span class='customInput'></span>");
		}
	}
});
</script>

