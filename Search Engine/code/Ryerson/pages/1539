<script>
$("#rye-deaa289c-65d8-40cb-921b-726fac6402d6 input, #rye-deaa289c-65d8-40cb-921b-726fac6402d6 select").each(function(i, v) {
	if (!$(this).hasClass('form-control')) {
		if ($(this).is("input[type='file']")) {
			$(this).attr('id', 'rye-deaa289c-65d8-40cb-921b-726fac6402d6_upload'+i).wrap("<span class='htmlSnippetForm'></span>").after("<label for='rye-deaa289c-65d8-40cb-921b-726fac6402d6_upload"+i+"' class='customInput'>Choose file</label>");
		} else {
			$(this).wrap("<span class='htmlSnippetForm'></span>").after("<span class='customInput'></span>");
		}
	}
});
</script>

