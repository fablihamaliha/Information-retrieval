<script>
$("#rye-c1d17567-1008-4442-832e-0df805dabf1a input, #rye-c1d17567-1008-4442-832e-0df805dabf1a select").each(function(i, v) {
	if (!$(this).hasClass('form-control')) {
		if ($(this).is("input[type='file']")) {
			$(this).attr('id', 'rye-c1d17567-1008-4442-832e-0df805dabf1a_upload'+i).wrap("<span class='htmlSnippetForm'></span>").after("<label for='rye-c1d17567-1008-4442-832e-0df805dabf1a_upload"+i+"' class='customInput'>Choose file</label>");
		} else {
			$(this).wrap("<span class='htmlSnippetForm'></span>").after("<span class='customInput'></span>");
		}
	}
});
</script>

	<style>
		#rye-c1d17567-1008-4442-832e-0df805dabf1a iframe{ height:;}
