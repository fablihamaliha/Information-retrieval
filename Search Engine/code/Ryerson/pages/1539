    $(document).ready(function(){
    	
        $("#1206bbae-7f50-4dbf-aed0-e564b5834c75-play, #1206bbae-7f50-4dbf-aed0-e564b5834c75-pause").click(function() {
            if ($(this).attr("id") == "1206bbae-7f50-4dbf-aed0-e564b5834c75-play") {
                $('#1206bbae-7f50-4dbf-aed0-e564b5834c75').carousel('cycle');
                $(this).attr("id", "1206bbae-7f50-4dbf-aed0-e564b5834c75-pause");
                $("#1206bbae-7f50-4dbf-aed0-e564b5834c75-pause i").attr("class", "fa fa-pause");
            } else {
                $('#1206bbae-7f50-4dbf-aed0-e564b5834c75').carousel('pause');
                $(this).attr("id", "1206bbae-7f50-4dbf-aed0-e564b5834c75-play");
                $("#1206bbae-7f50-4dbf-aed0-e564b5834c75-play i").attr("class", "fa fa-play");
