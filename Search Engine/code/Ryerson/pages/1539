		if ($(this).is("input[type='file']")) {
			$(this).attr('id', 'rye-06ae9d90-06f0-4309-9494-e7d622444c96_upload'+i).wrap("<span class='htmlSnippetForm'></span>").after("<label for='rye-06ae9d90-06f0-4309-9494-e7d622444c96_upload"+i+"' class='customInput'>Choose file</label>");
		} else {
			$(this).wrap("<span class='htmlSnippetForm'></span>").after("<span class='customInput'></span>");
