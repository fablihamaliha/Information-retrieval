                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('0f4495f0-08f7-478e-a1e9-72b85a8cbcfe',latLng,'Kerr Hall, 350 Victoria St, Toronto, ON M5B 0A1','1140px','600px',20,false);
                        } else{
                            drawStaticMapByAddress('0f4495f0-08f7-478e-a1e9-72b85a8cbcfe','Kerr Hall, 350 Victoria St, Toronto, ON M5B 0A1','1140px','600px',20,false);
                        }
                    }
                });
            });
        </script>
    
    
    
</div>
