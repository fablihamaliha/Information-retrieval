<script>
    $(document).ready(function(){
    	
        $("#70c6c322-4e60-4bec-8587-dc718c2ea4ab-play, #70c6c322-4e60-4bec-8587-dc718c2ea4ab-pause").click(function() {
            if ($(this).attr("id") == "70c6c322-4e60-4bec-8587-dc718c2ea4ab-play") {
                $('#70c6c322-4e60-4bec-8587-dc718c2ea4ab').carousel('cycle');
                $(this).attr("id", "70c6c322-4e60-4bec-8587-dc718c2ea4ab-pause");
                $("#70c6c322-4e60-4bec-8587-dc718c2ea4ab-pause i").attr("class", "fa fa-pause");
            } else {
                $('#70c6c322-4e60-4bec-8587-dc718c2ea4ab').carousel('pause');
                $(this).attr("id", "70c6c322-4e60-4bec-8587-dc718c2ea4ab-play");
                $("#70c6c322-4e60-4bec-8587-dc718c2ea4ab-play i").attr("class", "fa fa-play");
            }
        });
        
