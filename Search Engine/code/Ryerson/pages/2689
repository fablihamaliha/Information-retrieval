$("#rye-182cad4c-77b2-40fd-8abb-e5808664621f input, #rye-182cad4c-77b2-40fd-8abb-e5808664621f select").each(function(i, v) {
	if (!$(this).hasClass('form-control')) {
		if ($(this).is("input[type='file']")) {
			$(this).attr('id', 'rye-182cad4c-77b2-40fd-8abb-e5808664621f_upload'+i).wrap("<span class='htmlSnippetForm'></span>").after("<label for='rye-182cad4c-77b2-40fd-8abb-e5808664621f_upload"+i+"' class='customInput'>Choose file</label>");
		} else {
			$(this).wrap("<span class='htmlSnippetForm'></span>").after("<span class='customInput'></span>");
		}
	}
});
</script>

