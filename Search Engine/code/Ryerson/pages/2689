    $(document).ready(function(){
    	
        $("#bf06dc6f-1cea-4660-9930-6734d29ccaaa-play, #bf06dc6f-1cea-4660-9930-6734d29ccaaa-pause").click(function() {
            if ($(this).attr("id") == "bf06dc6f-1cea-4660-9930-6734d29ccaaa-play") {
                $('#bf06dc6f-1cea-4660-9930-6734d29ccaaa').carousel('cycle');
                $(this).attr("id", "bf06dc6f-1cea-4660-9930-6734d29ccaaa-pause");
