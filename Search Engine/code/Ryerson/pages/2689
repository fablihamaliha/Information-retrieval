<script>
    $(document).ready(function(){
    	
        $("#aa89022c-e264-4c93-9dee-850644a0b41b-play, #aa89022c-e264-4c93-9dee-850644a0b41b-pause").click(function() {
            if ($(this).attr("id") == "aa89022c-e264-4c93-9dee-850644a0b41b-play") {
                $('#aa89022c-e264-4c93-9dee-850644a0b41b').carousel('cycle');
                $(this).attr("id", "aa89022c-e264-4c93-9dee-850644a0b41b-pause");
                $("#aa89022c-e264-4c93-9dee-850644a0b41b-pause i").attr("class", "fa fa-pause");
            } else {
                $('#aa89022c-e264-4c93-9dee-850644a0b41b').carousel('pause');
                $(this).attr("id", "aa89022c-e264-4c93-9dee-850644a0b41b-play");
                $("#aa89022c-e264-4c93-9dee-850644a0b41b-play i").attr("class", "fa fa-play");
            }
        });
        
