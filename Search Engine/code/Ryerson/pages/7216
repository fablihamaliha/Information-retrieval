jQuery(document).ready(function() {
    jQuery("#component754973724 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component754973724 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component754973724 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component754973724 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component754973724 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
