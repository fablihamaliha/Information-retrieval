jQuery(document).ready(function() {
    jQuery("#component493239494 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component493239494 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component493239494 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component493239494 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component493239494 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
