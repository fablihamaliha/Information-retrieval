    jQuery("#component1004937672 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component1004937672 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component1004937672 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component1004937672 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component1004937672 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
