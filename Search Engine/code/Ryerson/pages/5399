jQuery(document).ready(function() {
    jQuery("#component378715450 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component378715450 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component378715450 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component378715450 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component378715450 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
