jQuery(document).ready(function() {
    jQuery("#component853145878 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component853145878 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component853145878 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component853145878 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component853145878 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
 </script>
 
