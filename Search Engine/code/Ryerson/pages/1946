                        if(latLng != null){
                            drawDynamicMapByLatLng('1a2c6d90-786d-4fce-aa65-b46cbb096261',latLng,'600px',18,false);
                        } else{
                            drawDynamicMapByAddress('1a2c6d90-786d-4fce-aa65-b46cbb096261','379  Victoria Street, Toronto, ON, M5B 2K3, Canada','600px',18,false);
                        }
