            <div class="panel panel-default">
                <!-- Accordion Heading -->
                <div class="panel-heading container-clipboard">
                    <div class="panel-title">
                        <a id="accordion-1499190270199-what-s-new" role="tab" data-toggle="collapse" data-parent="#" href="#accordion-content-1499190270199-what-s-new" aria-controls="accordion-content-1499190270199-what-s-new" aria-expanded="false">
                            What&#039;s New
                        </a>
                    </div>
                    
                </div>
                <!-- Accordion Body -->
                <div tabindex="0" id="accordion-content-1499190270199-what-s-new" class="panel-collapse collapse" role="tabpanel" aria-labelledby="accordion-1499190270199-what-s-new">
                    <div class="panel-body ">
                        <div class="1499190270199 stackparsys">










				
