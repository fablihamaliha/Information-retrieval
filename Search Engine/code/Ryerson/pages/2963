    $(document).ready(function(){
    	
        $("#f78254c5-6fff-4bf2-a3be-ad1a0f247bee-play, #f78254c5-6fff-4bf2-a3be-ad1a0f247bee-pause").click(function() {
            if ($(this).attr("id") == "f78254c5-6fff-4bf2-a3be-ad1a0f247bee-play") {
                $('#f78254c5-6fff-4bf2-a3be-ad1a0f247bee').carousel('cycle');
                $(this).attr("id", "f78254c5-6fff-4bf2-a3be-ad1a0f247bee-pause");
                $("#f78254c5-6fff-4bf2-a3be-ad1a0f247bee-pause i").attr("class", "fa fa-pause");
            } else {
                $('#f78254c5-6fff-4bf2-a3be-ad1a0f247bee').carousel('pause');
                $(this).attr("id", "f78254c5-6fff-4bf2-a3be-ad1a0f247bee-play");
