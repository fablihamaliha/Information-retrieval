        <div class="sr-only">Map of 350 Victoria Street, Toronto, ON, M5B 2K3, Canada</div>
        <script>
        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#a994649c-7382-4cb5-a871-d291b55933be button").click(function() {
                	jQuery("#a994649c-7382-4cb5-a871-d291b55933be.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.65829859999999, "lng": -79.3807846}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('a994649c-7382-4cb5-a871-d291b55933be',latLng,'600px',20,true);
                        } else{
                            drawDynamicMapByAddress('a994649c-7382-4cb5-a871-d291b55933be','350 Victoria Street, Toronto, ON, M5B 2K3, Canada','600px',20,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('a994649c-7382-4cb5-a871-d291b55933be',latLng,'350 Victoria Street, Toronto, ON, M5B 2K3, Canada','1140px','600px',20,true);
                        } else{
                            drawStaticMapByAddress('a994649c-7382-4cb5-a871-d291b55933be','350 Victoria Street, Toronto, ON, M5B 2K3, Canada','1140px','600px',20,true);
                        }
                    }
                });
            });
        </script>
    
    
    
</div>
</div>

