	if (!$(this).hasClass('form-control')) {
		if ($(this).is("input[type='file']")) {
			$(this).attr('id', 'rye-58694d94-4931-4fba-9e57-db5bdcd45854_upload'+i).wrap("<span class='htmlSnippetForm'></span>").after("<label for='rye-58694d94-4931-4fba-9e57-db5bdcd45854_upload"+i+"' class='customInput'>Choose file</label>");
		} else {
			$(this).wrap("<span class='htmlSnippetForm'></span>").after("<span class='customInput'></span>");
		}
	}
});
</script>

