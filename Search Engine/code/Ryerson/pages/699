<script>
    $(document).ready(function(){
    	
        $("#8588ee4e-844c-400e-bae7-6da7ae41f20c-play, #8588ee4e-844c-400e-bae7-6da7ae41f20c-pause").click(function() {
            if ($(this).attr("id") == "8588ee4e-844c-400e-bae7-6da7ae41f20c-play") {
                $('#8588ee4e-844c-400e-bae7-6da7ae41f20c').carousel('cycle');
                $(this).attr("id", "8588ee4e-844c-400e-bae7-6da7ae41f20c-pause");
                $("#8588ee4e-844c-400e-bae7-6da7ae41f20c-pause i").attr("class", "fa fa-pause");
            } else {
                $('#8588ee4e-844c-400e-bae7-6da7ae41f20c').carousel('pause');
                $(this).attr("id", "8588ee4e-844c-400e-bae7-6da7ae41f20c-play");
                $("#8588ee4e-844c-400e-bae7-6da7ae41f20c-play i").attr("class", "fa fa-play");
