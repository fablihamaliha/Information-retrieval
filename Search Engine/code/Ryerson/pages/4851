<script>
    $(document).ready(function(){
    	
        $("#650e5ed1-8ce1-4c2c-8a0b-5b7436add620-play, #650e5ed1-8ce1-4c2c-8a0b-5b7436add620-pause").click(function() {
            if ($(this).attr("id") == "650e5ed1-8ce1-4c2c-8a0b-5b7436add620-play") {
                $('#650e5ed1-8ce1-4c2c-8a0b-5b7436add620').carousel('cycle');
                $(this).attr("id", "650e5ed1-8ce1-4c2c-8a0b-5b7436add620-pause");
                $("#650e5ed1-8ce1-4c2c-8a0b-5b7436add620-pause i").attr("class", "fa fa-pause");
            } else {
                $('#650e5ed1-8ce1-4c2c-8a0b-5b7436add620').carousel('pause');
                $(this).attr("id", "650e5ed1-8ce1-4c2c-8a0b-5b7436add620-play");
                $("#650e5ed1-8ce1-4c2c-8a0b-5b7436add620-play i").attr("class", "fa fa-play");
