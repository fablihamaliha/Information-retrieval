  <script>
    jQuery(document).ready(function ($) {
      jQuery(function() {
        jQuery('.sidebar-module__list li').each(function() {    
          var href = jQuery(this).find('a').attr('href');
          var pageURL = window.location.pathname;
          // this is for the current membership side list
          if (pageURL.startsWith("/pages/current-membership")){
            jQuery(".sidebar-module__list").find('li:eq(1) a').addClass('current');
          }
          ///////
          if (href === pageURL) {
            jQuery(this).find('a').addClass('current');
          }else{
            url = window.location.pathname.split("/");
            link = href.split("/");
            if (link[2] === url[2]){
              jQuery(this).find('a').addClass('current');
            }
          }
        });
      });  
      if($("#customr").length != 0) {
        $('.top-line').html('<h1>My Account</h1><h4>Manage my account and order history</h4>')
        $('.email-field span').text('Email / Username ');
        $('.cta-button').val('Save Username').attr('data-static','Save Username');
        $('#customr a').text('< Back to Order History');
        $('#customr').addClass('grid__item');
      }
      
      var pathname = window.location.pathname;
     if (pathname.match("^/apps/downloads")) {
        $('.top-line').html('<h1>My Account</h1><h4>Manage my account and order history</h4>');
      }

    });
  </script>
