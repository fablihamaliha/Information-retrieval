             jQuery('img.image2').data('ad-title', "<a target='_blank' href='https://www.ryerson.ca/fcs-news-events/news/2020/05/ryerson-funding-covid19-rapid-response-round1/'>Connecting with communities to address COVID-19</a>");                              
             
