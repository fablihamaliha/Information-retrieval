  var jQuery = jQuery.noConflict(); 
  jQuery(function() {
    
             jQuery('img.image0').data('ad-title', "<a target='_blank' href='https://www.ryerson.ca/research/news-events/2020/04/physical-distancing/'>Researchers propose way of improving communication around physical distancing</a>");                              
             
