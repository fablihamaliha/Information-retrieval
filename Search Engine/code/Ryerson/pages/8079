jQuery(document).ready(function() {
    jQuery("#component346045040 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component346045040 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component346045040 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component346045040 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component346045040 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
 </script>
 
