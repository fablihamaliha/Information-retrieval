                <div class="panel-heading container-clipboard">
                    <h2 class="panel-title">
                        <a id="accordion-1540556313564-same-day-appointments" role="tab" data-toggle="collapse" data-parent="#" href="#accordion-content-1540556313564-same-day-appointments" aria-controls="accordion-content-1540556313564-same-day-appointments" aria-expanded="false">
                            Same Day Appointments
                        </a>
                    </h2>
                    
                </div>
                <!-- Accordion Body -->
                <div tabindex="0" id="accordion-content-1540556313564-same-day-appointments" class="panel-collapse collapse" role="tabpanel" aria-labelledby="accordion-1540556313564-same-day-appointments">
