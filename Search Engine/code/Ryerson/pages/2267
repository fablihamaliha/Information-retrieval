jQuery(document).ready(function() {
    jQuery("#component654779706 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component654779706 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component654779706 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component654779706 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component654779706 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
