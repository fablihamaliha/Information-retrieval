  <script type="text/javascript">
  var jQuery = jQuery.noConflict(); 
  jQuery(function() {
    
             jQuery('img.image0').data('ad-title', "<a target='_blank' href='https://www.ryerson.ca/news-events/news/2020/03/covid-19-lessons-from-a-ryerson-epidemiologist/'>COVID-19 lessons from a Ryerson epidemiologist</a>");                              
             
             jQuery('img.image1').data('ad-title', "<a target='_blank' href='https://www.ryerson.ca/news-events/news/2020/03/ryerson-faculty-and-staff-champion-collaborative-course-innovation/'>Ryerson faculty and staff champion collaborative course innovation</a>");                              
             
             jQuery('img.image2').data('ad-title', "<a target='_blank' href='https://www.ryerson.ca/news-events/news/2020/04/ryerson-answers-the-call-for-personal-protective-equipment-donations/'>Ryerson answers the call for personal protective equipment donations</a>");                              
             
             jQuery('img.image3').data('ad-title', "<a target='_blank' href='https://www.ryerson.ca/fcs-news-events/news/2020/05/ryerson-funding-covid19-rapid-response-round1/'>Connecting with communities to address COVID-19<br></a>");                              
             
             jQuery('img.image4').data('ad-title', "<a  href='/content/ryerson/fcs-student-connect'>FCS Student Connect<br></a>");                              
             
    
    var galleries = jQuery('.ad-gallery').adGallery({
        effect: 'fade',width:false, height: false, slideshow: {animate_first_image: false, speed: 5000}
    });
    galleries[0].settings.effect = 'fade'; 
   
    jQuery('#toggle-slideshow').click(
      function() {
        galleries[0].slideshow.toggle();
        return false;
      }
    );
    jQuery('#toggle-description').click(
      function() {
        if(!galleries[0].settings.description_wrapper) {
          galleries[0].settings.description_wrapper = jQuery('#descriptions');
        } else {
          galleries[0].settings.description_wrapper = false;
        }
        return false;
      }
    );
  });
  </script>

<style type="text/css">

	#component1873446840 .ad-gallery {
