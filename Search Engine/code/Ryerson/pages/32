<script>
    $(document).ready(function(){
    	
        $("#4968bee4-54b4-4092-b282-94fcf7e7ce8c-play, #4968bee4-54b4-4092-b282-94fcf7e7ce8c-pause").click(function() {
            if ($(this).attr("id") == "4968bee4-54b4-4092-b282-94fcf7e7ce8c-play") {
                $('#4968bee4-54b4-4092-b282-94fcf7e7ce8c').carousel('cycle');
                $(this).attr("id", "4968bee4-54b4-4092-b282-94fcf7e7ce8c-pause");
                $("#4968bee4-54b4-4092-b282-94fcf7e7ce8c-pause i").attr("class", "fa fa-pause");
            } else {
                $('#4968bee4-54b4-4092-b282-94fcf7e7ce8c').carousel('pause');
                $(this).attr("id", "4968bee4-54b4-4092-b282-94fcf7e7ce8c-play");
                $("#4968bee4-54b4-4092-b282-94fcf7e7ce8c-play i").attr("class", "fa fa-play");
            }
        });
        
