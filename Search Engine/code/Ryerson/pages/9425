        <script>
        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#0ca5d5df-7d5f-4ca0-91fb-bffc0542ff66 button").click(function() {
                	jQuery("#0ca5d5df-7d5f-4ca0-91fb-bffc0542ff66.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.657564, "lng": -79.3772027}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('0ca5d5df-7d5f-4ca0-91fb-bffc0542ff66',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('0ca5d5df-7d5f-4ca0-91fb-bffc0542ff66','245 Church Street, Toronto, ON, M5B 1Z2, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('0ca5d5df-7d5f-4ca0-91fb-bffc0542ff66',latLng,'245 Church Street, Toronto, ON, M5B 1Z2, Canada','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('0ca5d5df-7d5f-4ca0-91fb-bffc0542ff66','245 Church Street, Toronto, ON, M5B 1Z2, Canada','1140px','600px',18,true);
                        }
                    }
                });
            });
