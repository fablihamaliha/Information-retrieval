<script>
    $(document).ready(function(){
    	
        $("#84d2f61c-e005-4449-8c99-5d5dda1c58c3-play, #84d2f61c-e005-4449-8c99-5d5dda1c58c3-pause").click(function() {
            if ($(this).attr("id") == "84d2f61c-e005-4449-8c99-5d5dda1c58c3-play") {
                $('#84d2f61c-e005-4449-8c99-5d5dda1c58c3').carousel('cycle');
                $(this).attr("id", "84d2f61c-e005-4449-8c99-5d5dda1c58c3-pause");
                $("#84d2f61c-e005-4449-8c99-5d5dda1c58c3-pause i").attr("class", "fa fa-pause");
            } else {
                $('#84d2f61c-e005-4449-8c99-5d5dda1c58c3').carousel('pause');
                $(this).attr("id", "84d2f61c-e005-4449-8c99-5d5dda1c58c3-play");
                $("#84d2f61c-e005-4449-8c99-5d5dda1c58c3-play i").attr("class", "fa fa-play");
            }
        });
        
