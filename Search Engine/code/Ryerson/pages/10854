    $(document).ready(function(){
    	
        $("#366a2102-3d2c-4bce-88f6-ee6849c50517-play, #366a2102-3d2c-4bce-88f6-ee6849c50517-pause").click(function() {
            if ($(this).attr("id") == "366a2102-3d2c-4bce-88f6-ee6849c50517-play") {
                $('#366a2102-3d2c-4bce-88f6-ee6849c50517').carousel('cycle');
                $(this).attr("id", "366a2102-3d2c-4bce-88f6-ee6849c50517-pause");
                $("#366a2102-3d2c-4bce-88f6-ee6849c50517-pause i").attr("class", "fa fa-pause");
            } else {
                $('#366a2102-3d2c-4bce-88f6-ee6849c50517').carousel('pause');
                $(this).attr("id", "366a2102-3d2c-4bce-88f6-ee6849c50517-play");
                $("#366a2102-3d2c-4bce-88f6-ee6849c50517-play i").attr("class", "fa fa-play");
