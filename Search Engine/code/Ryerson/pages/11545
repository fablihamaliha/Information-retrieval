        <script>
        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#8e6f8270-378b-41fb-98ad-3050d737879e button").click(function() {
                	jQuery("#8e6f8270-378b-41fb-98ad-3050d737879e.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.657564, "lng": -79.3772027}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('8e6f8270-378b-41fb-98ad-3050d737879e',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('8e6f8270-378b-41fb-98ad-3050d737879e','245 Church Street, Toronto, ON, M5B 1Z2, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('8e6f8270-378b-41fb-98ad-3050d737879e',latLng,'245 Church Street, Toronto, ON, M5B 1Z2, Canada','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('8e6f8270-378b-41fb-98ad-3050d737879e','245 Church Street, Toronto, ON, M5B 1Z2, Canada','1140px','600px',18,true);
                        }
                    }
                });
            });
        </script>
    
    
    
</div>
