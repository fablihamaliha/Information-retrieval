        <script>
        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#c5bc2808-a4e1-4859-b3cb-dd89f9f1af77 button").click(function() {
                	jQuery("#c5bc2808-a4e1-4859-b3cb-dd89f9f1af77.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.65696579999999, "lng": -79.3804789}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('c5bc2808-a4e1-4859-b3cb-dd89f9f1af77',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('c5bc2808-a4e1-4859-b3cb-dd89f9f1af77','10 Dundas Street East, Toronto, ON, M5B 2G9, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('c5bc2808-a4e1-4859-b3cb-dd89f9f1af77',latLng,'10 Dundas Street East, Toronto, ON, M5B 2G9, Canada','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('c5bc2808-a4e1-4859-b3cb-dd89f9f1af77','10 Dundas Street East, Toronto, ON, M5B 2G9, Canada','1140px','600px',18,true);
                        }
                    }
                });
            });
        </script>
    
    
    
</div>
