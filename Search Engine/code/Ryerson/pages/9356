<script type="text/javascript">
jQuery(document).ready(function() {
	jQuery("div.contentArea div.columnsComponent div.wholeGrid").each(function() {
		if (parseInt(jQuery(this).css('paddingLeft')) > 0 && parseInt(jQuery(this).css('paddingRight')) == parseInt(jQuery(this).css('paddingLeft'))) {
			jQuery("body").append('<style type="text/css">@media print { #'+jQuery(this).parent().attr('id')+' .wholeGrid:first-child { padding: 0px !important; } }</style>');
		}
	});
});
