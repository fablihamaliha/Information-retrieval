        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component357475830 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component357475830 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component357475830 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component357475830 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
 </script>
 
</div>
</div>
  












</div>

</div>
 </div>
       </div><div style="clear:both;"></div>
    <div style="clear:both;"></div>
</div>
</div>
<script> 
      
</script> 














</div>

