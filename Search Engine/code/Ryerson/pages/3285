<script>

jQuery(document).ready(function() {
	if (getpage() == 'your-client-services-team.html')
		jQuery("ul.teaser:first a:eq(0)").addClass('selected');
	else if (getpage() == 'a-z-listing.html')
		jQuery("ul.teaser:first a:eq(1)").addClass('selected');
	else if (getpage() == 'avp.html')
		jQuery("ul.teaser:first a:eq(2)").addClass('selected');
	else if (getpage() == 'consulting-services.html')
		jQuery("ul.teaser:first a:eq(3)").addClass('selected');
	else if (getpage() == 'oee.html')
		jQuery("ul.teaser:first a:eq(4)").addClass('selected');
	else if (getpage() == 'operations-technology.html')
		jQuery("ul.teaser:first a:eq(5)").addClass('selected');
	else if (getpage() == 'pension-benefits.html')
		jQuery("ul.teaser:first a:eq(6)").addClass('selected');
	else if (getpage() == 'total-compensation.html')
		jQuery("ul.teaser:first a:eq(7)").addClass('selected');
	else if (getpage() == 'workplace-wellbeing.html')
		jQuery("ul.teaser:first a:eq(8)").addClass('selected');
	else if (getpage() == 'about-hr.html')
		jQuery("ul.teaser:first a:eq(9)").addClass('selected');
});
function getpage() {
	return location.pathname.substring(location.pathname.lastIndexOf("/") + 1);
}

</script>


