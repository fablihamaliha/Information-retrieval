    $(document).ready(function(){
    	
        $("#a518000e-aac9-4d37-a2a2-09060c503768-play, #a518000e-aac9-4d37-a2a2-09060c503768-pause").click(function() {
            if ($(this).attr("id") == "a518000e-aac9-4d37-a2a2-09060c503768-play") {
                $('#a518000e-aac9-4d37-a2a2-09060c503768').carousel('cycle');
                $(this).attr("id", "a518000e-aac9-4d37-a2a2-09060c503768-pause");
                $("#a518000e-aac9-4d37-a2a2-09060c503768-pause i").attr("class", "fa fa-pause");
            } else {
                $('#a518000e-aac9-4d37-a2a2-09060c503768').carousel('pause');
                $(this).attr("id", "a518000e-aac9-4d37-a2a2-09060c503768-play");
                $("#a518000e-aac9-4d37-a2a2-09060c503768-play i").attr("class", "fa fa-play");
            }
        });
        
