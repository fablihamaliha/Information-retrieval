        <script>
        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#f850eda8-9d2b-427b-bb1d-7a9cc14f6783 button").click(function() {
                	jQuery("#f850eda8-9d2b-427b-bb1d-7a9cc14f6783.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6567541, "lng": -79.3788588}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('f850eda8-9d2b-427b-bb1d-7a9cc14f6783',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('f850eda8-9d2b-427b-bb1d-7a9cc14f6783','110 Bond Street Toronto, ON M5B 1X8','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('f850eda8-9d2b-427b-bb1d-7a9cc14f6783',latLng,'110 Bond Street Toronto, ON M5B 1X8','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('f850eda8-9d2b-427b-bb1d-7a9cc14f6783','110 Bond Street Toronto, ON M5B 1X8','1140px','600px',18,true);
                        }
                    }
                });
            });
        </script>
    
    
    
