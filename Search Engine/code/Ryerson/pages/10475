    $(document).ready(function(){
    	
        $("#64410ed2-bee9-4a42-8933-7d0235f494ea-play, #64410ed2-bee9-4a42-8933-7d0235f494ea-pause").click(function() {
            if ($(this).attr("id") == "64410ed2-bee9-4a42-8933-7d0235f494ea-play") {
                $('#64410ed2-bee9-4a42-8933-7d0235f494ea').carousel('cycle');
