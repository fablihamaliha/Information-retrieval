                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('a772c65c-6953-484d-9435-8e608d1cae70',latLng,'600px',20,false);
                        } else{
                            drawDynamicMapByAddress('a772c65c-6953-484d-9435-8e608d1cae70','Kerr Hall, 350 Victoria St, Toronto, ON M5B 0A1','600px',20,false);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('a772c65c-6953-484d-9435-8e608d1cae70',latLng,'Kerr Hall, 350 Victoria St, Toronto, ON M5B 0A1','1140px','600px',20,false);
