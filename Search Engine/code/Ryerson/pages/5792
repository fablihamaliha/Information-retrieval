        <script>
        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#a772c65c-6953-484d-9435-8e608d1cae70 button").click(function() {
                	jQuery("#a772c65c-6953-484d-9435-8e608d1cae70.res-map-placeholder").removeClass('res-map-placeholder');
