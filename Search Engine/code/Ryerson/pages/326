			$(this).attr('id', 'rye-ac4ae8be-8b4a-4c5f-9882-13497a0a51f1_upload'+i).wrap("<span class='htmlSnippetForm'></span>").after("<label for='rye-ac4ae8be-8b4a-4c5f-9882-13497a0a51f1_upload"+i+"' class='customInput'>Choose file</label>");
		} else {
			$(this).wrap("<span class='htmlSnippetForm'></span>").after("<span class='customInput'></span>");
		}
	}
});
</script>

