    $(document).ready(function(){
    	
        $("#8de64c41-e06a-459b-b20f-23de44d7e64c-play, #8de64c41-e06a-459b-b20f-23de44d7e64c-pause").click(function() {
            if ($(this).attr("id") == "8de64c41-e06a-459b-b20f-23de44d7e64c-play") {
                $('#8de64c41-e06a-459b-b20f-23de44d7e64c').carousel('cycle');
                $(this).attr("id", "8de64c41-e06a-459b-b20f-23de44d7e64c-pause");
                $("#8de64c41-e06a-459b-b20f-23de44d7e64c-pause i").attr("class", "fa fa-pause");
            } else {
                $('#8de64c41-e06a-459b-b20f-23de44d7e64c').carousel('pause');
                $(this).attr("id", "8de64c41-e06a-459b-b20f-23de44d7e64c-play");
                $("#8de64c41-e06a-459b-b20f-23de44d7e64c-play i").attr("class", "fa fa-play");
