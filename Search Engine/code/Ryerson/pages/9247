        <script>
        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#3f4a0763-b8da-42ee-82aa-146e864ed2df button").click(function() {
                	jQuery("#3f4a0763-b8da-42ee-82aa-146e864ed2df.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.659324, "lng": -79.378299}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('3f4a0763-b8da-42ee-82aa-146e864ed2df',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('3f4a0763-b8da-42ee-82aa-146e864ed2df','325 church Street, Toronto, ON, M5B 1Z2, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('3f4a0763-b8da-42ee-82aa-146e864ed2df',latLng,'325 church Street, Toronto, ON, M5B 1Z2, Canada','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('3f4a0763-b8da-42ee-82aa-146e864ed2df','325 church Street, Toronto, ON, M5B 1Z2, Canada','1140px','600px',18,true);
                        }
                    }
                });
            });
        </script>
    
    
    
