<script>
    $(document).ready(function(){
    	
        $("#38726f19-eaf8-4f7b-af01-7def34b63373-play, #38726f19-eaf8-4f7b-af01-7def34b63373-pause").click(function() {
            if ($(this).attr("id") == "38726f19-eaf8-4f7b-af01-7def34b63373-play") {
                $('#38726f19-eaf8-4f7b-af01-7def34b63373').carousel('cycle');
                $(this).attr("id", "38726f19-eaf8-4f7b-af01-7def34b63373-pause");
                $("#38726f19-eaf8-4f7b-af01-7def34b63373-pause i").attr("class", "fa fa-pause");
            } else {
                $('#38726f19-eaf8-4f7b-af01-7def34b63373').carousel('pause');
                $(this).attr("id", "38726f19-eaf8-4f7b-af01-7def34b63373-play");
                $("#38726f19-eaf8-4f7b-af01-7def34b63373-play i").attr("class", "fa fa-play");
            }
        });
        
        		$('#38726f19-eaf8-4f7b-af01-7def34b63373').carousel('cycle');
        	
    });
</script>
