	$(document).ready(function() {
		$('#2e3cb615-aad9-4647-aae7-73b3d8642f71 table').addClass("table");
	    
			$('#2e3cb615-aad9-4647-aae7-73b3d8642f71 table').addClass("table-striped");
		
		$('#2e3cb615-aad9-4647-aae7-73b3d8642f71 table').addClass("table-borderless");
		
			var removeHeading = false;
			
			$('#2e3cb615-aad9-4647-aae7-73b3d8642f71 table tbody tr').eq(0).find("td").replaceWith(function(){
				return $("<th>", {attr: {'width': $(this).attr('width'), 'rowspan': $(this).attr('rowspan'), 'colspan': $(this).attr('colspan'), 'valign': $(this).attr('valign'), 'headers': $(this).attr('headers'), 'style': $(this).attr('style')}, html: $(this).html()}).css("vertical-align", $(this).attr('valign'));;
			});
			$('#2e3cb615-aad9-4647-aae7-73b3d8642f71 table:not(.dataTable) tbody tr').eq(0).wrap( "<thead></thead>" );
		
			$('#2e3cb615-aad9-4647-aae7-73b3d8642f71 table thead').prependTo('#2e3cb615-aad9-4647-aae7-73b3d8642f71 table');
			
			
			$('#2e3cb615-aad9-4647-aae7-73b3d8642f71 table').removeAttr("border");
			$('#2e3cb615-aad9-4647-aae7-73b3d8642f71 table').removeAttr("cellpadding");
			$('#2e3cb615-aad9-4647-aae7-73b3d8642f71 table').removeAttr("cellspacing");
			
			$('#2e3cb615-aad9-4647-aae7-73b3d8642f71 table').DataTable({
				
				"sDom":"lrtp",
				"sort":false,
				"pagingType": "simple_numbers","paging":false,
				"autoWidth":true
			});
		
			//#NOTE-REMOVE
			if(removeHeading){
				$('#2e3cb615-aad9-4647-aae7-73b3d8642f71 table thead').remove();
			}
			
			
		    
	    
	    
	} );
	</script>
