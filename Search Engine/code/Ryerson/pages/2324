jQuery(document).ready(function() {
    jQuery("#component799948324 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component799948324 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component799948324 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component799948324 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component799948324 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
 </script>
 
