			var removeHeading = false;
			
			$('#7336806f-19ac-47b8-b817-62f7712c723e table tbody tr').eq(0).find("td").replaceWith(function(){
				return $("<th>", {attr: {'width': $(this).attr('width'), 'rowspan': $(this).attr('rowspan'), 'colspan': $(this).attr('colspan'), 'valign': $(this).attr('valign'), 'headers': $(this).attr('headers'), 'style': $(this).attr('style')}, html: $(this).html()}).css("vertical-align", $(this).attr('valign'));;
			});
			$('#7336806f-19ac-47b8-b817-62f7712c723e table:not(.dataTable) tbody tr').eq(0).wrap( "<thead></thead>" );
		
			$('#7336806f-19ac-47b8-b817-62f7712c723e table thead').prependTo('#7336806f-19ac-47b8-b817-62f7712c723e table');
			
			
