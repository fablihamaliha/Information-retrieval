    $(document).ready(function(){
    	
        $("#70f01aff-5fdd-4cc8-a8ed-dbdeae59cc05-play, #70f01aff-5fdd-4cc8-a8ed-dbdeae59cc05-pause").click(function() {
            if ($(this).attr("id") == "70f01aff-5fdd-4cc8-a8ed-dbdeae59cc05-play") {
                $('#70f01aff-5fdd-4cc8-a8ed-dbdeae59cc05').carousel('cycle');
                $(this).attr("id", "70f01aff-5fdd-4cc8-a8ed-dbdeae59cc05-pause");
                $("#70f01aff-5fdd-4cc8-a8ed-dbdeae59cc05-pause i").attr("class", "fa fa-pause");
            } else {
                $('#70f01aff-5fdd-4cc8-a8ed-dbdeae59cc05').carousel('pause');
                $(this).attr("id", "70f01aff-5fdd-4cc8-a8ed-dbdeae59cc05-play");
                $("#70f01aff-5fdd-4cc8-a8ed-dbdeae59cc05-play i").attr("class", "fa fa-play");
