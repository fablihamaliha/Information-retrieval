        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#059c397b-f3bf-425b-9935-bc29a63e6aea button").click(function() {
                	jQuery("#059c397b-f3bf-425b-9935-bc29a63e6aea.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.657564, "lng": -79.3772027}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('059c397b-f3bf-425b-9935-bc29a63e6aea',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('059c397b-f3bf-425b-9935-bc29a63e6aea','245 Church Street, Toronto, ON, M5B 1Z2, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('059c397b-f3bf-425b-9935-bc29a63e6aea',latLng,'245 Church Street, Toronto, ON, M5B 1Z2, Canada','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('059c397b-f3bf-425b-9935-bc29a63e6aea','245 Church Street, Toronto, ON, M5B 1Z2, Canada','1140px','600px',18,true);
                        }
                    }
                });
            });
