        <script>
        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#158819ae-5ab5-4e5c-a43f-0f214ccd1f3d button").click(function() {
                	jQuery("#158819ae-5ab5-4e5c-a43f-0f214ccd1f3d.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6597097, "lng": -79.37758}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('158819ae-5ab5-4e5c-a43f-0f214ccd1f3d',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('158819ae-5ab5-4e5c-a43f-0f214ccd1f3d','87 Gerrard Street East, Toronto, ON, M5B 1G6, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('158819ae-5ab5-4e5c-a43f-0f214ccd1f3d',latLng,'87 Gerrard Street East, Toronto, ON, M5B 1G6, Canada','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('158819ae-5ab5-4e5c-a43f-0f214ccd1f3d','87 Gerrard Street East, Toronto, ON, M5B 1G6, Canada','1140px','600px',18,true);
                        }
                    }
                });
            });
        </script>
    
    
    
