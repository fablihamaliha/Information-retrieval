        <script>
        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#34d8984d-f5cf-44cd-a387-7cdb1d8ddb58 button").click(function() {
                	jQuery("#34d8984d-f5cf-44cd-a387-7cdb1d8ddb58.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6570679, "lng": -79.380062}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('34d8984d-f5cf-44cd-a387-7cdb1d8ddb58',latLng,'600px',15,false);
                        } else{
                            drawDynamicMapByAddress('34d8984d-f5cf-44cd-a387-7cdb1d8ddb58','285 Victoria Street, Toronto, ON, M5B 2K3, Canada','600px',15,false);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('34d8984d-f5cf-44cd-a387-7cdb1d8ddb58',latLng,'285 Victoria Street, Toronto, ON, M5B 2K3, Canada','1140px','600px',15,false);
                        } else{
                            drawStaticMapByAddress('34d8984d-f5cf-44cd-a387-7cdb1d8ddb58','285 Victoria Street, Toronto, ON, M5B 2K3, Canada','1140px','600px',15,false);
                        }
                    }
                });
            });
        </script>
    
    
    
</div>
