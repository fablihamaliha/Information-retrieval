jQuery(document).ready(function() {
    jQuery("#component145307419 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component145307419 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component145307419 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component145307419 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component145307419 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
 </script>
 
