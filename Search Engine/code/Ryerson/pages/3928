        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#9ad1fbd5-027c-4c17-80f6-03434f6525f0 button").click(function() {
                	jQuery("#9ad1fbd5-027c-4c17-80f6-03434f6525f0.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.657564, "lng": -79.3772027}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('9ad1fbd5-027c-4c17-80f6-03434f6525f0',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('9ad1fbd5-027c-4c17-80f6-03434f6525f0','245 Church Street, Toronto, ON, M5B 1Z2, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('9ad1fbd5-027c-4c17-80f6-03434f6525f0',latLng,'245 Church Street, Toronto, ON, M5B 1Z2, Canada','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('9ad1fbd5-027c-4c17-80f6-03434f6525f0','245 Church Street, Toronto, ON, M5B 1Z2, Canada','1140px','600px',18,true);
                        }
                    }
                });
            });
