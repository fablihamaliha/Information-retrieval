			$('#b5f95c88-9276-4b04-a34b-6e96a7f61501 table tbody tr').eq(0).find("td").replaceWith(function(){
				return $("<th>", {attr: {'width': $(this).attr('width'), 'rowspan': $(this).attr('rowspan'), 'colspan': $(this).attr('colspan'), 'valign': $(this).attr('valign'), 'headers': $(this).attr('headers'), 'style': $(this).attr('style')}, html: $(this).html()}).css("vertical-align", $(this).attr('valign'));;
			});
			$('#b5f95c88-9276-4b04-a34b-6e96a7f61501 table:not(.dataTable) tbody tr').eq(0).wrap( "<thead></thead>" );
		
			$('#b5f95c88-9276-4b04-a34b-6e96a7f61501 table thead').prependTo('#b5f95c88-9276-4b04-a34b-6e96a7f61501 table');
			
			
