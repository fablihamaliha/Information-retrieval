        <script>
        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#60967981-5b9d-43a1-be70-8f75f969c000 button").click(function() {
                	jQuery("#60967981-5b9d-43a1-be70-8f75f969c000.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.657564, "lng": -79.3772027}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('60967981-5b9d-43a1-be70-8f75f969c000',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('60967981-5b9d-43a1-be70-8f75f969c000','245 Church Street, Toronto, ON, M5B 1Z2, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('60967981-5b9d-43a1-be70-8f75f969c000',latLng,'245 Church Street, Toronto, ON, M5B 1Z2, Canada','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('60967981-5b9d-43a1-be70-8f75f969c000','245 Church Street, Toronto, ON, M5B 1Z2, Canada','1140px','600px',18,true);
                        }
                    }
                });
            });
        </script>
    
    
    
</div>
</div>

				

				
