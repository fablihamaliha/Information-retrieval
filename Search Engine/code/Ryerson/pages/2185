        <script>
        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#06412c94-ce05-4e66-85c0-f57bbf555319 button").click(function() {
                	jQuery("#06412c94-ce05-4e66-85c0-f57bbf555319.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6597321, "lng": -79.37971449999999}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('06412c94-ce05-4e66-85c0-f57bbf555319',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('06412c94-ce05-4e66-85c0-f57bbf555319','44 Gerrard Street East','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('06412c94-ce05-4e66-85c0-f57bbf555319',latLng,'44 Gerrard Street East','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('06412c94-ce05-4e66-85c0-f57bbf555319','44 Gerrard Street East','1140px','600px',18,true);
                        }
                    }
                });
            });
        </script>
    
    
    
