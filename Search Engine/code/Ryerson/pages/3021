<script language="JavaScript" type="text/JavaScript">
		$(document).ready(function() {
			$("#searchOption").show();
			//window.onload = function(){
			//	$(".searchType").attr("checked", false);
			//}
			//focus on the chosen method's text
			var checkedButton = $("input:radio[@name=searchType]:checked").val();
			setCheckedRadio(checkedButton);
			$("#searchDiv").show();
			$("#searchByBirthDate").hide();
			$("#searchByStudentNumber").hide();
			//toggles between search methods
			$("input:radio[@name=searchType]").click(function() {
		    	var checkedvalue = $(this).val();
		    	setCheckedRadio(checkedvalue);
			});
		   });
		   //focus on the chosen method's text
		   function setCheckedRadio(checkedvalue){
		   	if (checkedvalue == "birthDate"){
		   		$("#searchByStudentNumber").fadeOut(300);
		   		$("#searchByBirthDate").delay(350).slideDown(300);
		   		$("#birthDateText").attr('class', 'selected');
		   		$("#studentNumberText").attr('class', 'regular');
		   	}
		   	else if (checkedvalue == "studentNumber"){
				$("#searchByBirthDate").fadeOut(300);
				$("#searchByStudentNumber").delay(350).slideDown(300);
				$("#studentNumberText").attr('class', 'selected');
		   		$("#birthDateText").attr('class', 'regular');
		   	}
		   }
	</script>
