	if (!$(this).hasClass('form-control')) {
		if ($(this).is("input[type='file']")) {
			$(this).attr('id', 'rye-ce107413-92c9-4d79-83b5-599f72fe264e_upload'+i).wrap("<span class='htmlSnippetForm'></span>").after("<label for='rye-ce107413-92c9-4d79-83b5-599f72fe264e_upload"+i+"' class='customInput'>Choose file</label>");
		} else {
			$(this).wrap("<span class='htmlSnippetForm'></span>").after("<span class='customInput'></span>");
		}
	}
});
</script>

