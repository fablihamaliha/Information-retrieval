    $(document).ready(function(){
    	
        $("#1a426974-26ba-411c-ab13-d7dd04dd7ecf-play, #1a426974-26ba-411c-ab13-d7dd04dd7ecf-pause").click(function() {
            if ($(this).attr("id") == "1a426974-26ba-411c-ab13-d7dd04dd7ecf-play") {
                $('#1a426974-26ba-411c-ab13-d7dd04dd7ecf').carousel('cycle');
                $(this).attr("id", "1a426974-26ba-411c-ab13-d7dd04dd7ecf-pause");
                $("#1a426974-26ba-411c-ab13-d7dd04dd7ecf-pause i").attr("class", "fa fa-pause");
            } else {
                $('#1a426974-26ba-411c-ab13-d7dd04dd7ecf').carousel('pause');
                $(this).attr("id", "1a426974-26ba-411c-ab13-d7dd04dd7ecf-play");
                $("#1a426974-26ba-411c-ab13-d7dd04dd7ecf-play i").attr("class", "fa fa-play");
