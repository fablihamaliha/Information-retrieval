                jQuery("#96f93c2a-6c32-4db6-94f3-f702dabff9c5 button").click(function() {
                	jQuery("#96f93c2a-6c32-4db6-94f3-f702dabff9c5.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6558668, "lng": -79.3819145}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('96f93c2a-6c32-4db6-94f3-f702dabff9c5',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('96f93c2a-6c32-4db6-94f3-f702dabff9c5','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','600px',18,true);
                        }
