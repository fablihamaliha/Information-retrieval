<script>
    $(document).ready(function(){
    	
        $("#4f69af75-240d-4e4a-9bfb-8fbbd3833e0d-play, #4f69af75-240d-4e4a-9bfb-8fbbd3833e0d-pause").click(function() {
            if ($(this).attr("id") == "4f69af75-240d-4e4a-9bfb-8fbbd3833e0d-play") {
                $('#4f69af75-240d-4e4a-9bfb-8fbbd3833e0d').carousel('cycle');
                $(this).attr("id", "4f69af75-240d-4e4a-9bfb-8fbbd3833e0d-pause");
                $("#4f69af75-240d-4e4a-9bfb-8fbbd3833e0d-pause i").attr("class", "fa fa-pause");
            } else {
                $('#4f69af75-240d-4e4a-9bfb-8fbbd3833e0d').carousel('pause');
                $(this).attr("id", "4f69af75-240d-4e4a-9bfb-8fbbd3833e0d-play");
                $("#4f69af75-240d-4e4a-9bfb-8fbbd3833e0d-play i").attr("class", "fa fa-play");
            }
        });
        
