                $('#6acf1268-471e-4afb-8093-4f3bb5f1fe20').carousel('cycle');
                $(this).attr("id", "6acf1268-471e-4afb-8093-4f3bb5f1fe20-pause");
                $("#6acf1268-471e-4afb-8093-4f3bb5f1fe20-pause i").attr("class", "fa fa-pause");
