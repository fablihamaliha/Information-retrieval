                    <h2 class="panel-title">
                        <a id="accordion-1584378315872-chemistry" role="tab" data-toggle="collapse" data-parent="#b0152e91-6ec5-4d0f-bf84-694c65731367" href="#accordion-content-1584378315872-chemistry" aria-controls="accordion-content-1584378315872-chemistry" aria-expanded="false">
                            Chemistry
                        </a>
                    </h2>
                    
                </div>
                <!-- Accordion Body -->
                <div tabindex="0" id="accordion-content-1584378315872-chemistry" class="panel-collapse collapse" role="tabpanel" aria-labelledby="accordion-1584378315872-chemistry">
                    <div class="panel-body ">
                        <div class="1584378315872 stackparsys">










				
