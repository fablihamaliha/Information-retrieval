        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#19a63d99-a943-4360-9ad5-c7d340100593 button").click(function() {
                	jQuery("#19a63d99-a943-4360-9ad5-c7d340100593.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6558668, "lng": -79.3819145}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('19a63d99-a943-4360-9ad5-c7d340100593',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('19a63d99-a943-4360-9ad5-c7d340100593','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('19a63d99-a943-4360-9ad5-c7d340100593',latLng,'1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('19a63d99-a943-4360-9ad5-c7d340100593','1 Dundas Street West, Toronto, ON, M5G 2L5, Canada','1140px','600px',18,true);
                        }
                    }
                });
            });
