				if ($('#'+event.data.layoutID+' > .row > div:first > div.caption').length) {
					$('#'+event.data.layoutID+' > .row > div:first').addClass('overlayTextSwap').insertAfter($('#'+event.data.layoutID+' > .row > div:eq(1)'));
