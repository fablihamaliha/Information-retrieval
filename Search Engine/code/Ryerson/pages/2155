jQuery(document).ready(function() {
    jQuery("#component1851768170 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component1851768170 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component1851768170 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component1851768170 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component1851768170 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
 </script>
 
