<script type="text/javascript">
jQuery(document).ready(function() {
    jQuery("#component1394822763 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component1394822763 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component1394822763 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component1394822763 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component1394822763 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
 </script>
 
</div>
</div>
  












</div>

</div>

</div>

