<script type="text/javascript">
jQuery(document).ready(function() {
    jQuery("#component978763872 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component978763872 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component978763872 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component978763872 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component978763872 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
 </script>
 
</div>
</div>
  












</div>

</div>

</div>

