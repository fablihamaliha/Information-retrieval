                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.65824310000001, "lng": -79.3782968}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('5cfc18be-8514-4410-84a7-b35c22a939e9',latLng,'600px',18,false);
                        } else{
                            drawDynamicMapByAddress('5cfc18be-8514-4410-84a7-b35c22a939e9','60 Gould Street Toronto, Ontario ','600px',18,false);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('5cfc18be-8514-4410-84a7-b35c22a939e9',latLng,'60 Gould Street Toronto, Ontario ','1140px','600px',18,false);
