        <script>
        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#5c2de562-88fd-4493-80c2-b83c1755dc7d button").click(function() {
                	jQuery("#5c2de562-88fd-4493-80c2-b83c1755dc7d.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.6586299, "lng": -79.3770727}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('5c2de562-88fd-4493-80c2-b83c1755dc7d',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('5c2de562-88fd-4493-80c2-b83c1755dc7d','80 Gould St, Toronto, ON M5B 2M7','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('5c2de562-88fd-4493-80c2-b83c1755dc7d',latLng,'80 Gould St, Toronto, ON M5B 2M7','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('5c2de562-88fd-4493-80c2-b83c1755dc7d','80 Gould St, Toronto, ON M5B 2M7','1140px','600px',18,true);
                        }
                    }
                });
            });
        </script>
    
    
    
