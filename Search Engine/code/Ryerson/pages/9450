        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#b44cf60f-809a-4c7c-912c-9fa3cc64f157 button").click(function() {
                	jQuery("#b44cf60f-809a-4c7c-912c-9fa3cc64f157.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.659324, "lng": -79.378299}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('b44cf60f-809a-4c7c-912c-9fa3cc64f157',latLng,'600px',18,true);
                        } else{
                            drawDynamicMapByAddress('b44cf60f-809a-4c7c-912c-9fa3cc64f157','325 church Street, Toronto, ON, M5B 1Z2, Canada','600px',18,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('b44cf60f-809a-4c7c-912c-9fa3cc64f157',latLng,'325 church Street, Toronto, ON, M5B 1Z2, Canada','1140px','600px',18,true);
                        } else{
                            drawStaticMapByAddress('b44cf60f-809a-4c7c-912c-9fa3cc64f157','325 church Street, Toronto, ON, M5B 1Z2, Canada','1140px','600px',18,true);
                        }
                    }
                });
            });
