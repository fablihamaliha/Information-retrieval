        	jQuery(document).on('_initmaps', function(event) {
                jQuery("#9690aa7c-8aa9-4221-b277-1fc17ab5f13d button").click(function() {
                	jQuery("#9690aa7c-8aa9-4221-b277-1fc17ab5f13d.res-map-placeholder").removeClass('res-map-placeholder');
                    var isInResLocalFooter = true;
                    var latLng = JSON.parse('{"lat": 43.65829859999999, "lng": -79.3807846}');
                    if(!isInResLocalFooter) {
                        if(latLng != null){
                            drawDynamicMapByLatLng('9690aa7c-8aa9-4221-b277-1fc17ab5f13d',latLng,'600px',20,true);
                        } else{
                            drawDynamicMapByAddress('9690aa7c-8aa9-4221-b277-1fc17ab5f13d','350 Victoria Street, Toronto, ON, M5B 2K3, Canada','600px',20,true);
                        }
                    } else {
                        if(latLng != null){
                            drawStaticMapByLatLng('9690aa7c-8aa9-4221-b277-1fc17ab5f13d',latLng,'350 Victoria Street, Toronto, ON, M5B 2K3, Canada','1140px','600px',20,true);
                        } else{
                            drawStaticMapByAddress('9690aa7c-8aa9-4221-b277-1fc17ab5f13d','350 Victoria Street, Toronto, ON, M5B 2K3, Canada','1140px','600px',20,true);
                        }
                    }
                });
            });
        </script>
    
    
    
