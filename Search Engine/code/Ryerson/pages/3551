jQuery(document).ready(function() {
    jQuery("#component773324619 .accordion").accordion({
        header: "div.header",
        autoHeight: false,
        collapsible:true,
        active:false
       
    });

    jQuery("#component773324619 .openAll").click(function(event){
        event.preventDefault();
    	jQuery("#component773324619 .accordion").each(function (idx, item) {
    	    if (jQuery(item).accordion("option", "active") === false) {
    	    	jQuery(item).accordion('activate', 0);
    	    }
    	});
    });
    jQuery("#component773324619 .closeAll").click(function(event){
    	event.preventDefault();
        jQuery("#component773324619 .accordion").each(function (idx, item) {
            	jQuery(item).accordion("option", "active",false);
        });
    });
    
});
 </script>
 
