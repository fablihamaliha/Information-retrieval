<script>

var slideIndex = 1;

var divName="nextSlide";

//showDivs(slideIndex);



function plusDivs(n,val) {

  if ("nextSlide"+val !=divName){

	slideIndex=1;

	divName="nextSlide"+val;

	}

  showDivs(slideIndex += n,val);

}



function showDivs(n,val) {

  var i;

  var x = document.getElementsByClassName(divName);

  if (n > x.length) {slideIndex = 1}    

  if (n < 1) {slideIndex = x.length}

  for (i = 0; i < x.length; i++) {

     x[i].style.display = "none";  

  }

  x[slideIndex-1].style.display = "block";  

}

</script><script>

    $( document ).ready(function() {

        $('.hide').hide();

    });



  </script>



