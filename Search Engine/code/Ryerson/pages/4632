function tickOut(){

        $('#ticker li:first').animate({'opacity':0}, 1000, function () {

            $(this).appendTo($('#ticker'))

            $('#ticker li:first').hide()

            $('#ticker li:first').fadeIn(1000)

              $('#ticker li:not(:first)').css('opacity', '1')

        });

    }





setInterval(function(){ tickOut () }, 5500);

